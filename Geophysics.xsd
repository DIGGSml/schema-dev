            <element ref="diggs:GP_Detector">
                <annotation>
                    <documentation/>
                </annotation>
            </element>
        </sequence>
    </complexType>
    <complexType name="GP_DetectorType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element maxOccurs="1" minOccurs="0" name="measurand"
                        type="gml:CodeWithAuthorityType">
                        <annotation>
                            <documentation>Contains the name of the property being measured and reported to the data package. Intended to come from a controlled list of termsm the authority or pointer to a properties dictionary that defines the name used is required as the value of the codeSpace attribute.

                                </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="recordingSpace"
