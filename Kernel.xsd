        <annotation>
            <documentation>A value that describes the specific approach by which the water content
                is determined, eg. whether from specimen trimmings or full specimen</documentation>
        </annotation>
        <union memberTypes="diggs:WaterContentMethodEnumType diggs:OtherNameType"/>
