    <simpleType name="ApproximatorEnumType">
        <annotation>
            <documentation>Enumeration of terms used to indicate approximate quantities or thresholds</documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="about">
                <annotation>
                    <documentation>Indicates a value that is close to but may vary slightly above or below the stated quantity. More informal than "approximately".</documentation>
                </annotation>
            </enumeration>
            <enumeration value="approximately">
                <annotation>
                    <documentation>Formal term indicating a value very close to the stated quantity, with minimal deviation. More precise than "about" or "roughly".</documentation>
                </annotation>
            </enumeration>
            <enumeration value="roughly">
                <annotation>
                    <documentation>Suggests an estimate with potentially significant variance from the stated value. Less precise than "approximately" or "about".</documentation>
                </annotation>
            </enumeration>
            <enumeration value="at least">
                <annotation>
                    <documentation>Specifies a strict minimum threshold, with values equal to or greater than the stated quantity being acceptable.</documentation>
                </annotation>
            </enumeration>
            <enumeration value="greater than">
                <annotation>
                    <documentation>Mathematical/formal version of "more than", indicating values strictly exceeding the specified quantity.</documentation>
                </annotation>
            </enumeration>
            <enumeration value="at most">
                <annotation>
                    <documentation>Specifies a maximum threshold, with values equal to or less than the stated quantity being acceptable.</documentation>
                </annotation>
            </enumeration>
            <enumeration value="less than">
                <annotation>
                    <documentation>Indicates values strictly below the specified quantity, excluding the stated value itself. More formal than general phrases like "under".</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>
    <simpleType name="ApproximatorEnumExtType">
        <union memberTypes="diggs:ApproximatorEnumType diggs:OtherNameType"/>
    </simpleType>
    <element name="intensity" type="diggs:intensityEnumExtType">
        <annotation>
            <documentation>The first value in the color description if used. For soils and rock this is typically either a value or chroma term, (eg. pale, dark, brilliant, medium, dusky). From an enumerated list.

</documentation>
        </annotation>
    </element>

