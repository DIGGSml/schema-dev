            <enumeration value="J/L"/>
            <enumeration value="kJ/L"/>
            <enumeration value="MJ/L"/>
            <enumeration value="mJ/L"/>
            <enumeration value="Wh/m3"/>
            <enumeration value="Wh/L"/>
            <pattern value=""/>
            <enumeration value="kcal/m3"/>
            <enumeration value="kcal/L"/>
            <enumeration value="BTU/gal"/>
