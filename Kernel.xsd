                    <element maxOccurs="unbounded" minOccurs="0" name="otherSamplingFeatureProperty"
                        type="diggs:ParameterPropertyType">
                        <annotation>
                            <documentation>Reference to an Parameter object containing the name,
                                value or range of values and unit of measure of a property or a
                                measurement parameter.</documentation>
                        </annotation>
                    </element>
                    <element name="locality" type="diggs:LocalityPropertyType" minOccurs="0"
                        maxOccurs="1">
                        <annotation>
                            <documentation>A reference to a Locality object, containing
                                non-coordinate location information for a feature.</documentation>
                        </annotation>
                    </element>
                    <element name="referencePoint" type="diggs:PointLocationPropertyType"
                        minOccurs="1">
                        <annotation>
                            <documentation>Reference to a PointLocation object that defines the
                                location of the samplingFeature (usually a location at the ground
                                surface) within a well-known 3D CRS or combined CRS (geographic or
                                projected, and elevation), as an extension of gml:Point object with
                                an additional uncertainty property.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="localCoordinates"
                        type="diggs:LocalCoordinatePropertyType">
                        <annotation>
                            <documentation>A reference to a LocalCoordinate object that holds the
                                values of local coordinates for the reference point of the
                                associated feature. </documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true" name="AbstractSpecimen" type="diggs:AbstractSpecimenType">
        <annotation>
            <documentation>AbstractSpecimen serves as the head of a substitution group for concrete
                specimen specializations based on specimen state or type: eg. soil, liquid, gas,
                rock. A specimen derives from one or more Samples, is created as part of a test
                procedure, and exists only in the context of the test procedure that creates
                it.</documentation>
        </annotation>
    </element>
    <complexType abstract="true" name="AbstractSpecimenType">
        <annotation>
            <documentation/>
        </annotation>
        <complexContent>
            <extension base="diggs:AbstractFeatureType">
                <sequence>
                    <element maxOccurs="unbounded" ref="diggs:sampleRef"/>
                    <element minOccurs="0" name="extractionTime" type="gml:TimePositionType">
                        <annotation>
                            <documentation>The time instant when this specimen was extracted from
                                the sample for testing. This is equivalent to
                                diggs:MaterialTest/sampleTime and diggs:Test/diggs:samplingTime
                                where only one specimen is used for the test and should generally
                                not be used in that case. Where multiple specimens are used for a
                                test and each was extracted from the host at different times, then
                                this property should be used to record the precise sampling time for
                                each specimen.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="preparationProcedure"
                        type="diggs:SpecificationPropertyType">
                        <annotation>
                            <documentation>A property that contains a Specification object or
                                reference to a Specification object that describes the procedure
                                used to prepare the specimen from the sample. This can be a simple
                                description (contained in the Specification object's gml:description
                                property), the name of a standard specification (in the gml:name) or
                                a full procedural specification using appropriate properties in the
                                Specification object. </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="otherSpecimenProperty"
                        type="diggs:ParameterPropertyType" maxOccurs="unbounded"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="AbstractTestProcedure" type="diggs:AbstractTestProcedureType"
        substitutionGroup="diggs:AbstractProcedure" abstract="true">
        <annotation>
            <documentation>Base type for all procedures that comprise a Test or MaterialTest.
            </documentation>
        </annotation>
    </element>
    <complexType name="AbstractTestProcedureType">
        <complexContent>
            <extension base="diggs:AbstractProcedureType">
                <sequence>
                    <element name="testProcedureMethod" type="diggs:SpecificationPropertyType"
                        maxOccurs="unbounded" minOccurs="0">
                        <annotation>
                            <documentation>A property element that contains a Specification object
                                (or reference to one) detailing the test procedure method
                                used.</documentation>
                        </annotation>
                    </element>
                    <element name="testProcedureEquipment" type="diggs:EquipmentPropertyType"
                        maxOccurs="unbounded" minOccurs="0">
                        <annotation>
                            <appinfo>diggs:AbstractEquipment</appinfo>
                            <documentation>Contains or references an Equipment object. Any concrete
                                object that substitutes for AbstractEquipment can be contained or
                                referenced by this property. </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="testingEnvironment"
                        type="diggs:EnvironmentPropertyType" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Environmental conditions during testing. Values derived
                                at different times during the test can be specified with multiple
                                elements and using the Environment/dateTime
                                property.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="otherTestProperty"
                        type="diggs:ParameterPropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="AbstractTimeObject" substitutionGroup="gml:AbstractTimeObject"
        type="diggs:AbstractTimeObjectType">
        <annotation>
            <documentation>Base type for DIGGS time objects</documentation>
        </annotation>
    </element>
    <complexType name="AbstractTimeObjectBaseType">
        <complexContent>
            <restriction base="gml:AbstractTimeObjectType">
                <sequence>
                    <element ref="gml:description" minOccurs="0"/>
                </sequence>
            </restriction>
        </complexContent>
    </complexType>

    <complexType abstract="true" name="AbstractTimeObjectType">
        <complexContent>
            <extension base="diggs:AbstractTimeObjectBaseType">
                <sequence>
                    <element minOccurs="0" ref="diggs:status">
                        <annotation>
                            <documentation>A text string to provide information about the status of
                                the object to be used to track QA/QC controls, review or release
                                status, etc. The values used are intended to come from a controlled
                                list of terms maintained by an organization.</documentation>
                        </annotation>
                    </element>
                    <element name="remark" type="diggs:RemarkPropertyType" minOccurs="0"
                        maxOccurs="unbounded"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="Address" type="diggs:AddressType" substitutionGroup="gml:AbstractGML"
        abstract="false">
        <annotation>
            <documentation>A collection of information used to give the location of a building,
                apartment, or other structure or a plot of land, generally using political
                boundaries.</documentation>
        </annotation>
    </element>
    <complexType name="AddressPropertyType">
        <sequence>
            <element ref="diggs:Address" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The address object contains properties of a physical address, eg.
                        the location where a person or entity can be communicated
                        with.</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>
    <complexType name="AddressType">
        <complexContent>
            <extension base="diggs:AbstractObjectBaseType">
                <sequence>
                    <element name="number" type="diggs:DiggsStringType" minOccurs="0" maxOccurs="1"/>
                    <element name="streetAddress" type="diggs:DiggsStringType" minOccurs="0"
                        maxOccurs="unbounded"/>
                    <element name="city" type="diggs:DiggsStringType" minOccurs="0" maxOccurs="1"/>
                    <element name="state" type="diggs:DiggsStringType" minOccurs="0" maxOccurs="1"/>
                    <element name="province" type="diggs:DiggsStringType" minOccurs="0"
                        maxOccurs="1"/>
                    <element name="county" type="diggs:DiggsStringType" minOccurs="0" maxOccurs="1"/>
                    <element name="country" type="diggs:DiggsStringType" minOccurs="0" maxOccurs="1"/>
                    <element name="postalCode" type="diggs:DiggsStringType" minOccurs="0"
                        maxOccurs="1"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="Advancement" type="diggs:AdvancementType"
        substitutionGroup="diggs:AbstractObject">
        <annotation>
            <documentation>Object that carries drill advancement properties.</documentation>
        </annotation>
    </element>
    <complexType mixed="false" name="AdvancementType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element name="location" type="diggs:LinearExtentPropertyType" minOccurs="1">
                        <annotation>
                            <documentation>The measured depth interval of the reported section of
                                the borehole. </documentation>
                        </annotation>
                    </element>
                    <choice>
                        <element name="time" type="diggs:TimeIntervalPropertyType" minOccurs="0">
                            <annotation>
                                <documentation>Start date/time, end date/time, and duration to
                                    advance the reported section of the borehole.</documentation>
                            </annotation>
                        </element>
                        <element name="elapsedTime" type="eml:TimeMeasure" minOccurs="0">
                            <annotation>
                                <documentation>The amount of time it took to advance over the
                                    reported interval. </documentation>
                            </annotation>
                        </element>
                    </choice>
                    <element name="advancementRate" type="diggs:LengthPerTimeMeasureType"
                        minOccurs="0" maxOccurs="1">
                        <annotation>
                            <documentation>The rate of advancement over the measured depth interval.
                                This property can be used in addition to or in lieu of time or
                                elapsedTime.</documentation>
                        </annotation>
                    </element>
                    <element name="downwardThrust" minOccurs="0" type="eml:ForceMeasure">
                        <annotation>
                            <documentation>Downward thrust on bit</documentation>
                        </annotation>
                    </element>
                    <element name="downthrustPressure" minOccurs="0"
                        type="diggs:pressureMeasureType">
                        <annotation>
                            <documentation>Pressure of downthrust system</documentation>
                        </annotation>
                    </element>
                    <element name="energyDensity" minOccurs="0" type="eml:EnergyPerVolumeMeasure">
                        <annotation>
                            <documentation>Specific energy</documentation>
                        </annotation>
                    </element>
                    <element name="fluidCirculationRate" minOccurs="0"
                        type="eml:VolumePerTimeMeasure">
                        <annotation>
                            <documentation>Fluid (mud) circulation rate</documentation>
                        </annotation>
                    </element>
                    <element name="fluidPressurePump" minOccurs="0" type="diggs:pressureMeasureType">
                        <annotation>
                            <documentation>Fluid (usually mud) pressure at output of
                                pump</documentation>
                        </annotation>
                    </element>
                    <element name="fluidRecoveryRate" minOccurs="0" type="eml:VolumePerTimeMeasure">
                        <annotation>
                            <documentation>Fluid (mud) recovery rate</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="fluidWeight"
                        type="diggs:DensityOrUnitWeightMeasureType">
                        <annotation>
                            <documentation>The mass per unit volume of a drilling fluid, synonymous
                                with fluid density or mud weight. </documentation>
                        </annotation>
                    </element>
                    <element name="hammeringUsed" minOccurs="0" type="boolean">
                        <annotation>
                            <documentation>True if drill stem was hammered while advancing (eg.
                                chiseling).</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="chiselingToolUsed" type="gml:StringOrRefType">
                        <annotation>
                            <documentation>Tool used to perform the chiseling. Intended to come from
                                a controlled list. NOTE: This is a string that could come from a
                                controlled list of terms, but could also reference an Equipment
                                metadata object.</documentation>
                        </annotation>
                    </element>
                    <element name="hammerPressure" minOccurs="0" type="diggs:pressureMeasureType">
                        <annotation>
                            <documentation>Supply pressure to downhole hammer</documentation>
                        </annotation>
                    </element>
                    <element name="mechanicalSpecificEnergy" minOccurs="0"
                        type="eml:EnergyPerVolumeMeasure">
                        <annotation>
                            <documentation>A measure of the energy required to remove a unit volume
                                of rock.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" ref="diggs:penetrationRate">
                        <annotation>
                            <documentation>Rate of advancement of the drill head. Deprecated - use
                                advamcementRate instead</documentation>
                        </annotation>
                    </element>
                    <element name="restrainingForce" minOccurs="0" type="eml:ForceMeasure">
                        <annotation>
                            <documentation>Restraining (holdback) force</documentation>
                        </annotation>
                    </element>
                    <element name="restrainingPressure" minOccurs="0"
                        type="diggs:pressureMeasureType">
                        <annotation>
                            <documentation>Pressure of restraining (holdback) system</documentation>
                        </annotation>
                    </element>
                    <element name="rotationalSpeed" minOccurs="0" type="eml:AngularVelocityMeasure">
                        <annotation>
                            <documentation>Rotational speed of the drill head</documentation>
                        </annotation>
                    </element>
                    <element name="rotationalTorque" minOccurs="0" type="eml:MomentOfForceMeasure">
                        <annotation>
                            <documentation>Rotational torque on the drill head</documentation>
                        </annotation>
                    </element>
                    <element name="torqueOnRods" minOccurs="0" type="eml:MomentOfForceMeasure">
                        <annotation>
                            <documentation>Torque applied to top of drill rods</documentation>
                        </annotation>
                    </element>
                    <element name="torquePressure" minOccurs="0" type="diggs:pressureMeasureType">
                        <annotation>
                            <documentation>Torque pressure</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <simpleType name="AllUnits">
        <annotation>
            <documentation>Enumerated list of all unit abbreviations</documentation>
        </annotation>
        <restriction>
            <simpleType>
                <union
                    memberTypes="diggs:areaPerTimeUom diggs:diggsPressureUom diggs:energyDensityUom diggs:forcePerTimeUom diggs:geotechnicalDensityUom diggs:geotechnicalLengthPerTimeUom diggs:geotechnicalPressureUom diggs:particleSizeUom diggs:percentPerTimeUom diggs:reciprocalSquareRootTimeUom diggs:reciprocalTimeSquaredUom eml:UnitOfMeasureExt"
                />
            </simpleType>
        </restriction>
    </simpleType>
    <complexType name="ReciprocalSquareRootTimeMeasureType">
        <simpleContent>
            <extension base="eml:AbstractMeasure">
                <attribute name="uom" type="diggs:reciprocalSquareRootTimeUom" use="required"/>
            </extension>
        </simpleContent>
    </complexType>
    <simpleType name="reciprocalSquareRootTimeUom">
        <annotation>
            <documentation/>
        </annotation>
        <restriction base="eml:UomEnum">
            <enumeration value="1/m^1/2"/>
        </restriction>
    </simpleType>
    <simpleType name="reciprocalTimeSquaredUom">
        <annotation>
            <documentation/>
        </annotation>
        <restriction base="eml:UomEnum">
            <enumeration value="1/s2"/>
            <enumeration value="Eo"/>
            <enumeration value="mGal/m"/>
            <enumeration value="Gal/m"/>
        </restriction>
    </simpleType>
    <complexType name="ReciprocalTimeSquaredMeasureType">
        <simpleContent>
            <extension base="eml:AbstractMeasure">
                <attribute name="uom" type="diggs:reciprocalTimeSquaredUom" use="required"/>
            </extension>
        </simpleContent>
    </complexType>
    <complexType name="AreaPerTimeMeasureType">
        <simpleContent>
            <extension base="eml:AbstractMeasure">
                <attribute name="uom" type="diggs:areaPerTimeUom" use="required"/>
            </extension>
        </simpleContent>
    </complexType>
    <simpleType name="areaPerTimeUomExt">
        <annotation>
            <documentation>This enumerated list extends eml:AreaPerTimeUom. m2/sec is deprecated.
                m2/s (eml namespace) should be used in its place. mm2/min is deprecated; use mm2/m
                instead to be consistent with eml symbology. 1) cm2/a - squqre centimeter per
                julian-year 2) cm2/d - square centimeter per day 3) cm2/h - square centimeter per
                hour 4) cm2/m - square centimeter per minute 5) m2/a - square meter per julian-year
                6) m2/d - square meter per day 7) m2/h - square meter per hour 8) m2/m - square
                meter per minute 9) m2/sec - square meter per second - deprecated 10) mm2/a - square
                millimeter per julian-year 11) mm2/d - square millimeter per day 12) mm2/h - square
                millimeter per hour 13) mm2/m - square millimeter per minute 13) mm2/min - square
                millmeter per minute - deprecated </documentation>
        </annotation>
        <restriction base="eml:UomEnum">
            <enumeration value="cm2/a">
                <annotation>
                    <documentation>square centimeter per julian-year</documentation>
                </annotation>
            </enumeration>
            <enumeration value="cm2/d">
                <annotation>
                    <documentation>square centimeter per day</documentation>
                </annotation>
            </enumeration>
            <enumeration value="cm2/h">
                <annotation>
                    <documentation>square centimeter per hour</documentation>
                </annotation>
            </enumeration>
            <enumeration value="cm2/m">
                <annotation>
                    <documentation>square centimeter per minute</documentation>
                </annotation>
            </enumeration>
            <enumeration value="m2/a">
                <annotation>
                    <documentation>square meter per julian-year</documentation>
                </annotation>
            </enumeration>
            <enumeration value="m2/d">
                <annotation>
                    <documentation>square meter per day</documentation>
                </annotation>
            </enumeration>
            <enumeration value="m2/h">
                <annotation>
                    <documentation>square meter per hour</documentation>
                </annotation>
            </enumeration>
            <enumeration value="m2/m">
                <annotation>
                    <documentation>square meter per minute</documentation>
                </annotation>
            </enumeration>
            <enumeration value="m2/sec">
                <annotation>
                    <documentation>square meter per second - deprecated</documentation>
                </annotation>
            </enumeration>
            <enumeration value="mm2/a">
                <annotation>
                    <documentation>square millimater per julian-year</documentation>
                </annotation>
            </enumeration>
            <enumeration value="mm2/d">
                <annotation>
                    <documentation>square millimater per day</documentation>
                </annotation>
            </enumeration>
            <enumeration value="mm2/h">
                <annotation>
                    <documentation>square millimater per hour</documentation>
                </annotation>
            </enumeration>
            <enumeration value="mm2/m">
                <annotation>
                    <documentation>square millimater per minute</documentation>
                </annotation>
            </enumeration>
            <enumeration value="mm2/min">
                <annotation>
                    <documentation>square millimater per minute - deprecated</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>
    <simpleType name="areaPerTimeUom">
        <union memberTypes="diggs:areaPerTimeUomExt eml:AreaPerTimeUomExt"/>
    </simpleType>
    <element name="AssociatedFile" type="diggs:AssociatedFileType"
        substitutionGroup="diggs:AbstractNamedObject" abstract="false">
        <annotation>
            <documentation>An object that provides information about external files or documents
                associated with the containing features.</documentation>
        </annotation>
    </element>
    <complexType name="AssociatedFilePropertyType">
        <sequence minOccurs="1">
            <element ref="diggs:AssociatedFile" maxOccurs="1" minOccurs="1"> </element>
        </sequence>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <complexType name="AssociatedFileType">
        <complexContent>
            <extension base="diggs:AbstractNamedObjectType">
                <sequence>
                    <sequence>
                        <element minOccurs="0" name="contact"
                            type="diggs:BusinessAssociatePropertyType" maxOccurs="unbounded">
                            <annotation>
                                <documentation>The person or business to contact about this file.
                                    This property should be populated where a fileURL is not
                                    provided.</documentation>
                            </annotation>
                        </element>
                        <element minOccurs="1" name="fileURL" type="anyURI">
                            <annotation>
                                <documentation>Path to the digital file</documentation>
                            </annotation>
                        </element>
                        <element maxOccurs="1" minOccurs="0" name="fileType" type="gml:CodeType">
                            <annotation>
                                <documentation>Digital file type, typically denoted by the file
                                    extension (eg. .jpg, .pdf)</documentation>
                            </annotation>
                        </element>
                        <element maxOccurs="1" minOccurs="0" name="creatingApplication"
                            type="diggs:SoftwareApplicationPropertyType">
                            <annotation>
                                <documentation>Name of the application that created the file, where
                                    applicable.</documentation>
                            </annotation>
                        </element>
                        <element maxOccurs="1" minOccurs="0" name="documentType" type="gml:CodeType">
                            <annotation>
                                <documentation>For documents, the type of document that the file
                                    represents, such as photo, report, log, etc.). </documentation>
                            </annotation>
                        </element>
                        <element maxOccurs="1" minOccurs="0" name="fileDate"
                            type="gml:TimePositionType">
                            <annotation>
                                <documentation>Date file created or last modified.</documentation>
                            </annotation>
                        </element>
                        <element minOccurs="0" name="fileStructure" type="gml:CodeType">
                            <annotation>
                                <documentation>A controlled term describing how the file is
                                    structured (eg. Record Interleaved)</documentation>
                            </annotation>
                        </element>
                        <element minOccurs="0" name="mimeType" type="diggs:mimeTypeEnumExtType">
                            <annotation>
                                <documentation>The mime type of the file, from an enumerated list. Non-registered mime types should be entered as Other: type/subtype where the subtype
                                    should begin with "x-", eg.
                                    application/x-segy.</documentation>
                            </annotation>
                        </element>
                        <element minOccurs="0" name="compression" type="anyURI">
                            <annotation>
                                <documentation>A value that identifies the compression algorithm
                                    used, if applicable (eg LZW).</documentation>
                            </annotation>
                        </element>
                        <element minOccurs="0" name="encryption" type="anyURI">
                            <annotation>
                                <documentation>A value that identifies the file encryption algorithm
                                    used, if applicable (eg. AES).</documentation>
                            </annotation>
                        </element>
                    </sequence>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="associatedProjectRef" type="diggs:FeatureReferenceType">
        <annotation>
            <appinfo source="urn:x-gml:targetElement">Diggs:Project</appinfo>
            <documentation>A reference to a project that uses the feature within its business
                activity. The associated project is not intended to be the one that established the
                feature in the first place. The property originalProjectRef should be used for that
                purpose. </documentation>
            <documentation>A property element that supports a value by reference only. The target
                value is indicated in the appinfo element.</documentation>
        </annotation>
    </element>
    <element name="Backfill" substitutionGroup="diggs:AbstractObject" type="diggs:BackfillType">
        <annotation>
            <documentation>An object that contains properites associated with backfilling of a
                borehole. </documentation>
        </annotation>
    </element>
    <element name="BackfillLayer" substitutionGroup="diggs:AbstractObject"
        type="diggs:BackfillLayerType">
        <annotation>
            <documentation>An object that carries properties for a layer of backfill
                material.</documentation>
        </annotation>
    </element>
    <complexType name="BackfillLayerPropertyType">
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:BackfillLayer"/>
        </sequence>
    </complexType>
    <complexType name="BackfillLayerType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element minOccurs="0" name="backfillInterval"
                        type="diggs:LinearExtentPropertyType">
                        <annotation>
                            <documentation>The depth interval in the borehole (measured in a linear
                                reference system), where the backfill material was emplaced. If
                                omitted, it is assumed that the backfill information pertains to the
                                entire length of the hole</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="whenEmplaced" type="diggs:TimeIntervalPropertyType">
                        <annotation>
                            <documentation>Indicates the date/time during which the backfill was
                                emplaced in this depth interval.</documentation>
                        </annotation>
                    </element>
                    <element name="backfillMaterial" type="gml:CodeType">
                        <annotation>
                            <documentation>The type of material used as backfill (eg. bentonite,
                                cuttings, sand, etc.). Intended to come from a controlled list
                                assigned to an authority.</documentation>
                        </annotation>
                    </element>
                    <element name="isSanitarySeal" type="boolean" minOccurs="0">
                        <annotation>
                            <documentation>If true, identifies this backfill layer as a sanitary
                                seal.</documentation>
                        </annotation>
                    </element>
                    <element name="backfillVolume" type="eml:VolumeMeasure" minOccurs="0">
                        <annotation>
                            <documentation>The total valume of backfill material emplaced in this
                                layer</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="groutingActivityRef"
                        type="diggs:FeatureReferenceType">
                        <annotation>
                            <appinfo>diggs:GroutingActivity</appinfo>
                            <documentation>If pressure and flow rate time series are recorded for
                                this backfill layer, this information is stored in a grouting
                                activity feature. This property points to the grouting activity
                                corresponding to emplacement of this layer</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="groutRef" type="diggs:FeatureReferenceType">
                        <annotation>
                            <appinfo>diggs:GroutBatch</appinfo>
                            <documentation>If information is recorded about components of a Grout
                                used for backfill or as part of a grouting activity,this property
                                points to the GroutBatch object corresponding to the grout batch
                                used in this backfill layer.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="wellOpeningRef" type="diggs:FeatureReferenceType">
                        <annotation>
                            <appinfo>diggs:GroutBatch</appinfo>
                            <documentation>If information is recorded about components of a Grout
                                used for backfill or as part of a grouting activity,this property
                                points to the GroutBatch object corresponding to the grout batch
                                used in this backfill layer.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="BackfillPropertyType">
        <sequence>
            <element minOccurs="1" ref="diggs:Backfill"/>
        </sequence>
    </complexType>
    <complexType name="BackfillType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element minOccurs="0" name="backfillDateTime"
                        type="diggs:TimeIntervalPropertyType">
                        <annotation>
                            <documentation>The date/time interval over which the entire backfilling
                                operation was conducted. Date/time intervals can also be specified
                                for the emplacement of individual backfill layers in the
                                backfillLayers property.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="backfillLayer"
                        type="diggs:BackfillLayerPropertyType" maxOccurs="unbounded">
                        <annotation>
                            <documentation>The depth intervals where backfill is emplaced. The
                                backfill intervals should not overlap, nor should there be gaps
                                between intervals.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="backfillType" type="diggs:backfillTypeEnumType">
                        <annotation>
                            <documentation>A controlled list of terms to indicate what type of
                                backfilling is described in the object. Choices are: annular space
                                within borehole - backfilling occurs within the annular space
                                between the outer casing and the natural borehole wall annular space
                                between casings - backfilling occurs within the annular space
                                between nested casings open hole - backfilling occurs within an open
                                hole within casing - backfilling occurs within the innermost
                                casing</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <simpleType name="backfillTypeEnumType">
        <restriction base="string">
            <enumeration value="annular space within borehole"/>
            <enumeration value="annular space within casings"/>
            <enumeration value="open hole"/>
            <enumeration value="within casing"/>
        </restriction>
    </simpleType>
    <complexType name="BearingMeasureType">
        <simpleContent>
            <extension base="diggs:BearingMeasureBaseType">
                <attribute name="refAzimuth" type="witsml:AziRef"/>
            </extension>
        </simpleContent>
    </complexType>
    <element abstract="false" name="Borehole"
        substitutionGroup="diggs:AbstractLinearSamplingFeature" type="diggs:BoreholeType">
        <annotation>
            <documentation>A sampling feature feature that is a deep, narrow excavation made by
                drilling and/or extraction of earth material. Boreholes are constructed typically
                for the purpose of investigating subsurface geologic or geotechnical conditions,
                exploring for water or oil, for installation of wells or other downhole monitoring
                installations.</documentation>
        </annotation>
    </element>
    <element name="BoreholeConstructionMethod" type="diggs:BoreholeConstructionMethodType">
        <annotation>
            <documentation>An object that contains properties of borehole
                construction.</documentation>
        </annotation>
    </element>
    <complexType name="BoreholeConstructionMethodPropertyType">
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:BoreholeConstructionMethod"/>
        </sequence>
    </complexType>
    <complexType name="BoreholeConstructionMethodType">
        <complexContent>
            <extension base="diggs:AbstractNamedRoleObjectType">
                <sequence>
                    <element minOccurs="0" ref="diggs:location">
                        <annotation>
                            <documentation>The location within the sampling feature that corresponds
                                to the particular construction method being referenced here. If
                                omitted, it is assumed that the procedures specified apply to the
                                entire extent of the sampling feature</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="methodTime"
                        type="diggs:TimeIntervalPropertyType">
                        <annotation>
                            <documentation>The period of time during which the construction event is
                                occurring</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="holeStability" type="diggs:DiggsStringType">
                        <annotation>
                            <documentation>Uncontrolled string describing stability conditions of
                                the hole during this interval</documentation>
                        </annotation>
                    </element>
                    <element name="constructionMethod" type="diggs:SpecificationPropertyType"
                        maxOccurs="unbounded" minOccurs="0">
                        <annotation>
                            <documentation>Properties describing the method by which the borehole is
                                constructed over this location interval</documentation>
                        </annotation>
                    </element>
                    <element name="constructionEquipment" type="diggs:EquipmentPropertyType"
                        maxOccurs="unbounded" minOccurs="0">
                        <annotation>
                            <documentation>A property element that contains an Equipment feature (or
                                reference to one) that details the construction equipment
                                used.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="cuttingToolInfo"
                        type="diggs:CuttingToolArrayPropertyType"/>
                    <element minOccurs="0" name="drillPipeInfo"
                        type="diggs:DrillPipeArrayPropertyType"/>
                    <element minOccurs="0" name="hydraulicFluidFlowRate"
                        type="eml:VolumePerTimeMeasureExt">
                        <annotation>
                            <documentation>The flow rate of the hydraulic fluid.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="BoreholeDiameter" substitutionGroup="diggs:AbstractObject"
        type="diggs:BoreholeDiameterType">
        <annotation>
            <documentation>An object that carries the diameter of a borehole and its
                location.</documentation>
        </annotation>
    </element>
    <complexType name="BoreholeDiameterPropertyType">
        <sequence>
            <element ref="diggs:BoreholeDiameter" maxOccurs="1"/>
        </sequence>
    </complexType>
    <complexType name="BoreholeDiameterType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element name="diameterLocation" type="diggs:LinearExtentPropertyType"
                        maxOccurs="1" minOccurs="0">
                        <annotation>
                            <documentation>The depth interval in the hole that pertains to the
                                diameter reported, measured in a linear referencing system. If
                                omitted, it is assumed that the diameter reported pertains to the
                                entire length of the hole</documentation>
                        </annotation>
                    </element>
                    <element name="diameter" type="eml:LengthMeasure">
                        <annotation>
                            <documentation>The diameter of the borehole over the depth interval
                                reported.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="computedBoreholeCrossSectionalArea"
                        type="eml:AreaMeasureExt">
                        <annotation>
                            <documentation>Cross-sectional area of the borehole, computed from
                                inferred borehole diameter for the interval</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="computedBoreholeVolume" type="eml:VolumeMeasureExt">
                        <annotation>
                            <documentation>Volume of the boreholein this interval, computed from the
                                inferred borehole diameter.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="BoreholeEvent" type="diggs:BoreholeEventType"
        substitutionGroup="diggs:AbstractEvent">
        <annotation>
            <documentation>Depth and/or time related information recorded during construction of the
                borehole.These events are intended to record information that is non-geological.
                Typically this includes drillers comments usually found on the boring or drilling
                log, for reporting incidents during drilling (such as "Fishing for broken U100"),
                drilling records that don't readily fit in any other groups (such as "Pushing
                boulder ahead of casing"), observations that are not strictly geological (such as
                "Strong petrol smell").</documentation>
        </annotation>
    </element>
    <complexType name="BoreholeEventPropertyType">
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:BoreholeEvent"/>
        </sequence>
    </complexType>
    <complexType name="BoreholeEventType">
        <complexContent>
            <extension base="diggs:AbstractEventType">
                <sequence>
                    <element minOccurs="0" name="casingBottom"
                        type="diggs:PointLocationPropertyType">
                        <annotation>
                            <documentation>Measured Depth to Base of Casing installed at this event
                                time, in the hole's linear referencing system. </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="holeBottom" type="diggs:PointLocationPropertyType">
                        <annotation>
                            <documentation>Measured Depth to Base of Hole at this event time,
                                reported in the hole's linear referencing system.</documentation>
                        </annotation>
                    </element>
                    <choice>
                        <element minOccurs="0" name="waterDepth"
                            type="diggs:PointLocationPropertyType">
                            <annotation>
                                <documentation>Depth to Water Surface as measured at this event
                                    time, recorded in the hole's linear referencing
                                    system.</documentation>
                            </annotation>
                        </element>
                        <element minOccurs="0" name="isDry" type="boolean" fixed="true"/>
                    </choice>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="BoreholeType">
        <complexContent>
            <extension base="diggs:AbstractLinearSamplingFeatureType">
                <sequence>
                    <element maxOccurs="1" minOccurs="0" name="whenConstructed"
                        type="diggs:TimeIntervalPropertyType">
                        <annotation>
                            <documentation>The period of time when the hole is constructed. This
                                should include the entire time the drill rig is on
                                site.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="whenDestroyed"
                        type="diggs:TimeIntervalPropertyType">
                        <annotation>
                            <documentation>The time period during which the hole was destroyed or
                                forever abandoned. This date must occur after all related
                                observations, measurements or activities had been performed in the
                                borehole, and should reflect the time after which no observations or
                                activities are possible in the hole.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="totalMeasuredDepth" type="eml:LengthMeasure">
                        <annotation>
                            <documentation>The measured depth to the bottom of the hole, measured in
                                a hole's linear referencing system.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" ref="diggs:holeBearing"/>

                    <element minOccurs="0" ref="diggs:holePlunge"/>

                    <element maxOccurs="1" minOccurs="0" name="boreholePurpose" type="gml:CodeType">
                        <annotation>
                            <documentation>A description of the purpose for drilling the hole (eg.
                                exploratory boring, monitoring well, etc.). Intended to come from a
                                controlled list.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="boreholeType"
                        type="gml:CodeType">
                        <annotation>
                            <documentation>A description of the type of boring. Can be used as a
                                generalized term for the construction method (eg. mud rotary,
                                reverse circulation, etc.) or some other classification based on a
                                unique business model. Detailed or depth dependent construction
                                information should be encoded in the constructionMethod
                                property.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="backfill" type="diggs:BackfillPropertyType"
                        maxOccurs="unbounded">
                        <annotation>
                            <documentation>Information on construction of the borehole backfill
                                after drilling. Also used to record backfill installed during well
                                construction or rock grouting activities</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="casing" type="diggs:CasingPropertyType"
                        maxOccurs="unbounded">
                        <annotation>
                            <documentation>Information on the borehole casing installed while
                                drilling and/or after completion of drilling.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="constructionMethod"
                        type="diggs:BoreholeConstructionMethodPropertyType" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Information regarding construction of the hole over
                                different depth intervals</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="constructionEvent"
                        type="diggs:BoreholeEventPropertyType" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Information about events recorded during construction of
                                the borehole.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" maxOccurs="unbounded" ref="diggs:chiseling"/>

                    <element name="drillAdvancement" maxOccurs="unbounded" minOccurs="0"
                        type="diggs:DrillAdvancementArrayPropertyType"/>
                    <element minOccurs="0" name="flush" type="diggs:FlushPropertyType"
                        maxOccurs="unbounded">
                        <annotation>
                            <documentation>Information related to flushing the borehole with fluid
                                (eg. fluid type, return, fluid color, etc.) </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="holeDiameter"
                        type="diggs:BoreholeDiameterPropertyType" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Information on the borehole diameter with depth,
                                generally representing the bit size used within a borehole interval.
                                Detailed measurements of borehole derived from a caliper log is a
                                Test property resulting from a Wireline log procedure.
                            </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="waterStrike" type="diggs:WaterStrikePropertyType"
                        maxOccurs="unbounded">
                        <annotation>
                            <documentation>Information on when ground water is struck during
                                drilling of the hole. Note, option for more than one waterstrike
                                property is deprecated in 2.6. Only one waterstrike property should
                                be used. Post-strike readings should use the postStrikeReading
                                property within the Waterstrike onject and not additional
                                waterstrike properties.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="Boundary" substitutionGroup="diggs:AbstractObject" type="diggs:BoundaryType">
        <annotation>
            <documentation>An object that contains properties describing a layer, interval, or zone
                boundary (eg. dip angle, sharpness, etc.)</documentation>
        </annotation>
    </element>
    <complexType name="BoundaryPropertyType">
        <sequence maxOccurs="1">
            <element ref="diggs:Boundary"/>
        </sequence>
    </complexType>
    <complexType name="BoundaryType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element minOccurs="0" ref="diggs:location"/>
                    <element minOccurs="0" name="dipAngle" type="eml:PlaneAngleMeasure">
                        <annotation>
                            <documentation><![CDATA[The inclinaton angle in degrees of the boundary surface, as measured downward from the horizontal.]]></documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="dipDirection" type="eml:PlaneAngleMeasure">
                        <annotation>
                            <documentation><![CDATA[The direction of the true dip of the boundary surface, as measured clockwise with respect to true north.]]></documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="distinctness" type="gml:CodeType">
                        <annotation>
                            <documentation><![CDATA[A controlled string (preferably) that defines how distinctly the boundary between zones is defined, as governed by both the contrast between adjacent zones and by the width of the boundary. (eg. sharp, gradational)]]></documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="origin" type="gml:CodeType">
                        <annotation>
                            <documentation><![CDATA[A controlled string (preferably) that defines the specific nature or origin of the boundary. (eg. unconformity, fault, etc.)]]></documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="topography" type="gml:CodeType">
                        <annotation>
                            <documentation><![CDATA[A controlled string (preferably) that defines the shape of the boundary surface between zones. (eg. wavy)]]></documentation>
                        </annotation>
                    </element>
                </sequence>
                <attribute name="howDetermined" type="diggs:DescriptorMethodEnumType">
                    <annotation>
                        <documentation>A value that describes the method by which the observation or measurement
                            was determined. </documentation>
                    </annotation>
                </attribute>
            </extension>
        </complexContent>
    </complexType>
    <element name="BusinessAssociate" type="diggs:BusinessAssociateType"
        substitutionGroup="diggs:AbstractNamedFeature" abstract="false">
        <annotation>
            <documentation>A person or entity who performs functions, activities, or
                services.</documentation>
        </annotation>
    </element>
    <complexType name="BusinessAssociatePropertyType">
        <sequence>
            <element ref="diggs:BusinessAssociate" minOccurs="0" maxOccurs="1"/>
        </sequence>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <complexType name="BusinessAssociateType">
        <complexContent>
            <extension base="diggs:AbstractNamedFeatureType">
                <sequence>
                    <element name="title" type="gml:CodeType" minOccurs="0" maxOccurs="1"/>
                    <element name="address" type="diggs:AddressPropertyType" minOccurs="0"
                        maxOccurs="unbounded"/>
                    <element name="emailAddress" type="diggs:EmailType" minOccurs="0"
                        maxOccurs="unbounded"/>
                    <element name="phoneNumber" type="diggs:PhoneType" minOccurs="0"
                        maxOccurs="unbounded"/>
                    <element name="associatedWith" type="diggs:BusinessAssociatePropertyType"
                        minOccurs="0" maxOccurs="unbounded">
                        <annotation>
                            <appinfo source="urn:x-gml:targetElement">diggs:BusinessAssociate</appinfo>
                            <documentation>A property element that supports a value by reference
                                only. The target value is indicated in the appinfo
                                element.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="Calibration" type="diggs:CalibrationType"
        substitutionGroup="diggs:AbstractFeature" abstract="false">
        <annotation>
            <documentation>An object containing properties pertaining to instrument
                calibration.</documentation>
        </annotation>
    </element>
    <complexType name="CalibrationPropertyType">
        <sequence>
            <element ref="diggs:Calibration" minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>
    <complexType name="CalibrationType">
        <complexContent>
            <extension base="diggs:AbstractFeatureType">
                <sequence>
                    <element minOccurs="0" name="calibrationData" type="gml:StringOrRefType">
                        <annotation>
                            <documentation>Procedure and recorded data obtained during calibration.
                                This can be represented as text or as a reference to another object
                                or external file via an xlink:href attribute</documentation>
                        </annotation>
                    </element>
                    <element name="cqlibrationValues" type="diggs:ParameterArrayPropertyType"
                        minOccurs="0" maxOccurs="1">
                        <annotation>
                            <documentation>Date calibration was perfomed</documentation>
                        </annotation>
                    </element>
                    <element name="date" type="gml:TimePositionType" minOccurs="0" maxOccurs="1">
                        <annotation>
                            <documentation>Date calibration was perfomed</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="useSinceLastCalibration"
                        type="diggs:DiggsStringType">
                        <annotation>
                            <documentation>Note about the amount of use of the equipment since the
                                last calibration.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="Casing" type="diggs:CasingType">
        <annotation>
            <documentation>An object containing properties pertaining to borehole
                casing.</documentation>
        </annotation>
    </element>
    <complexType name="CasingPropertyType">
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:Casing"/>
        </sequence>
    </complexType>
    <complexType name="CasingType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element minOccurs="0" name="casingLocation"
                        type="diggs:LinearExtentPropertyType">
                        <annotation>
                            <documentation>The depth interval of the casing after fully installed
                                and advanced within the borehole. If omitted, it is assumed that the
                                casing information pertains to the entire length of the borehole or
                                well.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="casingOutsideDiameter" type="eml:LengthMeasure"/>
                    <element minOccurs="0" name="casingInsideDiameter" type="eml:LengthMeasure"/>
                    <element minOccurs="0" name="casingWallThickness" type="eml:LengthMeasure"/>
                    <element minOccurs="0" name="casingMaterial" type="gml:CodeType">
                        <annotation>
                            <documentation>The material that the casing is made of, eg. PVC,
                                stainless steel, etc. Intended to come from a controlled list of
                                terms.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="numberGroutPorts"
                        type="positiveInteger">
                        <annotation>
                            <documentation>If casing is an overburden casing as sleeve pipe (OCSP)
                                or other casing through which grout is injected, this property
                                records the number of ports in the casing.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="timeCasingInstalled"
                        type="diggs:TimeIntervalType">
                        <annotation>
                            <documentation>The time interval over which the casing was installed
                            </documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="timeCasingRemoved"
                        type="diggs:TimeIntervalType">
                        <annotation>
                            <documentation>The time interval during which the casing was removed
                            </documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="casingAdvancement"
                        type="diggs:CasingAdvancementPropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="CasingAdvancement" type="diggs:CasingAdvancementType">
        <annotation>
            <documentation>If casing is advanced while drilling, or retracted during an activity,
                like grouting, this object records the time interval when casing
                advancement/retraction occurs, along with the start and end position of the casing
                bottom.</documentation>
        </annotation>
    </element>
    <complexType name="CasingAdvancementPropertyType">
        <sequence>
            <element ref="diggs:CasingAdvancement"/>
        </sequence>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <complexType name="CasingAdvancementType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element maxOccurs="1" minOccurs="0" name="advancementTime"
                        type="diggs:TimeIntervalPropertyType">
                        <annotation>
                            <documentation>Time at start and end of the casing advancement
                                interval</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="casingBaseAtStart"
                        type="diggs:PointLocationPropertyType">
                        <annotation>
                            <documentation>The location of the bottom at the casing at the beginning
                                of the casing interval.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="casingBaseAtEnd"
                        type="diggs:PointLocationPropertyType">
                        <annotation>
                            <documentation>The location of the bottom at the casing at the end of
                                the casing advancement interval.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="casingBlows" type="positiveInteger">
                        <annotation>
                            <documentation>If casing is driven, the number of blows to drive the
                                casing the reported distance. Details about the process should be
                                reported in the relevant equipment or construction method
                                object.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="ChainOfCustodyEvent" type="diggs:ChainOfCustodyEventType">
        <annotation>
            <documentation>An object containing properties pertaining to the chain of custody of a
                sample.</documentation>
        </annotation>
    </element>
    <complexType name="ChainOfCustodyEventPropertyType">
        <sequence>
            <element ref="diggs:ChainOfCustodyEvent"/>
        </sequence>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <complexType mixed="false" name="ChainOfCustodyEventType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element minOccurs="0" name="sourceCustodian"
                        type="diggs:BusinessAssociatePropertyType">
                        <annotation>
                            <documentation> A reference to the BusinessAssociate object that is
                                releasing custody of the sample.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="destinationCustodian"
                        type="diggs:BusinessAssociatePropertyType">
                        <annotation>
                            <documentation> A reference to the BusinessAssociate object that is
                                receiving custody of the sample.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="dateInitiated" type="gml:TimePositionType">
                        <annotation>
                            <documentation><![CDATA[Date that sample was shipped or date that chain of custody event was initiated]]></documentation>
                        </annotation>
                    </element>
                    <element name="dateCompleted" type="gml:TimePositionType" minOccurs="0">
                        <annotation>
                            <documentation><![CDATA[Date that sample was received or date that chain of custody event was completed]]></documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="sampleContainer" type="diggs:ContainerPropertyType">
                        <annotation>
                            <documentation><![CDATA[Information about the sample container, if repackaged from the initial container.]]></documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="shippingContainer"
                        type="diggs:ContainerPropertyType">
                        <annotation>
                            <documentation><![CDATA[Information about the container that the sample containers are transported in.]]></documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="storageLocation"
                        type="diggs:LocalityPropertyType">
                        <annotation>
                            <documentation>The location of the sample once it has reached it's
                                destination. Can be expressed as properties of a Locality object
                                (eg. address or other local term describing location, or as a
                                coordinate point (PointLocation) </documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="CodeWithAuthorityType">
        <annotation>
            <documentation>Same as gml:CodeWithAuthorityType, only without the pattern restriction
                we've placed on this term with DIGGS profile of GML 3.2</documentation>
        </annotation>
        <simpleContent>
            <extension base="string">
                <attribute name="codeSpace" type="anyURI" use="required"/>
            </extension>
        </simpleContent>
    </complexType>
    <element name="dataValues" substitutionGroup="gml:tupleList" type="gml:CoordinatesType">
        <annotation>
            <documentation>A property that holds a string of data values as part of a measurement
                result set.</documentation>
        </annotation>
    </element>
    <element name="Color" substitutionGroup="diggs:AbstractComponentObject" type="diggs:ColorType">
        <annotation>
            <documentation>An object that describes the color of earth materials.</documentation>
        </annotation>
    </element>
    <simpleType name="ColorCharacterEnumType">
        <union>
            <simpleType>
                <restriction base="string">
                    <enumeration value="banded"/>
                    <enumeration value="mottled"/>
                    <enumeration value="multicolored"/>
                    <enumeration value="uniform"/>
                    <enumeration value="variegated"/>
                </restriction>
            </simpleType>
            <simpleType>
                <restriction base="string">
                    <pattern value="Other: .{2,}"/>
                </restriction>
            </simpleType>
        </union>
    </simpleType>
    <simpleType name="ColorOriginEnumType">
        <union>
            <simpleType>
                <restriction base="string">
                    <enumeration value="matrix"/>
                    <enumeration value="mottle"/>
                    <enumeration value="clay depletion"/>
                    <enumeration value="iron depletion"/>
                    <enumeration value="iron Fe2 mass"/>
                    <enumeration value="iron Fe3 mass"/>
                    <enumeration value="iron-manganese mass"/>
                    <enumeration value="jarosite mass"/>
                    <enumeration value="manganese mass"/>
                    <enumeration value="ironstone nodule"/>
                    <enumeration value="iron-manganese nodule"/>
                    <enumeration value="jarosite nodule"/>
                    <enumeration value="plinthite"/>
                    <enumeration value="iron-manganese concretion"/>
                    <enumeration value="manganese coat or film"/>
                    <enumeration value="ferriargillans coat or film"/>
                    <enumeration value="disseminated carbonate"/>
                    <enumeration value="disseminated salts"/>
                    <enumeration value="disseminated gypsum"/>
                    <enumeration value="barite mass"/>
                    <enumeration value="gypsum mass"/>
                    <enumeration value="carbonate mass"/>
                    <enumeration value="salt mass"/>
                    <enumeration value="clay body"/>
                    <enumeration value="silica mass"/>
                    <enumeration value="gypsum crystal clusters"/>
                    <enumeration value="carbonate nodule"/>
                    <enumeration value="durinode (SiO2) nodule"/>
                    <enumeration value="gibbsite (Al2O3) nodule"/>
                    <enumeration value="opal nodule"/>
                    <enumeration value="ortstein nodule"/>
                    <enumeration value="carbonate concretion"/>
                    <enumeration value="gibbsite concretion"/>
                    <enumeration value="silica concretion"/>
                    <enumeration value="titanium oxide concretion"/>
                    <enumeration value="barite crystals"/>
                    <enumeration value="calcite crystals"/>
                    <enumeration value="gypsum crystals"/>
                    <enumeration value="salt crystals"/>
                    <enumeration value="satin spar crystals"/>
                    <enumeration value="selenite crystals"/>
                </restriction>
            </simpleType>
            <simpleType>
                <restriction base="string">
                    <pattern value="Other: .{2,}"/>
                </restriction>
            </simpleType>
        </union>
    </simpleType>

    <element name="ColorObservation" substitutionGroup="diggs:AbstractObservation"
        type="diggs:ColorObservationType">
        <annotation>
            <documentation>A color observation describes the color of materials encountered. All
                color observations within a color observation system must not overlap, although
                multiple colors can be described within a single zone.</documentation>
        </annotation>
    </element>
    <complexType name="ColorObservationPropertyType">
        <sequence maxOccurs="1">
            <element maxOccurs="1" ref="diggs:ColorObservation">
                <annotation>
                    <documentation>Color observations must not overlap</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>
    <complexType name="ColorObservationType">
        <complexContent>
            <extension base="diggs:AbstractObservationType">
                <sequence>
                    <element maxOccurs="unbounded" name="color" type="diggs:ColorPropertyType"/>
                    <element maxOccurs="unbounded" minOccurs="0" ref="diggs:placedObservation"/>
                    <element minOccurs="0" ref="diggs:baseBoundary" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Description of the boundary at the base of the observed
                                zone or interval as defined in the observation's location
                                property</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" ref="diggs:character"/>

                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <complexType name="ColorPropertyType">
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:Color"/>
        </sequence>
        <!-- These attributes now in Color object 
        <attribute name="character" type="diggs:ColorCharacterEnumType">
            <annotation>
                <documentation>Deprecated - use attribute in Color object instead</documentation>
            </annotation>
        </attribute>
        <attribute name="origin" type="diggs:ColorOriginEnumType">
            <annotation>
                <documentation>Deprecated - use attribute in Color object instead</documentation>
            </annotation>
        </attribute>
        -->
    </complexType>
    <complexType name="ColorType">
        <complexContent>
            <extension base="diggs:ComponentObjectNoDescriptionType">
                <sequence>
                    <choice>
                        <element minOccurs="1" name="colorCode" type="diggs:DescriptorCodeType">
                            <annotation>
                                <documentation>The code value that defines the color; preferably defined by an authority. Typically this would be a Munsell color. (eg. 10YR4/3)</documentation>
                            </annotation>
                        </element>
                        <sequence>
                            <element minOccurs="1" name="colorName" type="diggs:DescriptorCodeType"
                                maxOccurs="1">
                                <annotation>
                                    <documentation>The name of the color, preferably defined by an
                                        authority defined in the codespace attribute (eg. reddish
                                        brown)</documentation>
                                </annotation>
                            </element>
                            <element minOccurs="0" name="colorCode" type="diggs:DescriptorCodeType">
                                <annotation>
                                    <documentation>The code value that defines the color; preferably defined by an authority. Typically this would be a Munsell color. (eg. 10YR4/3)</documentation>
                                </annotation>
                            </element>
                        </sequence>
                    </choice>
                    <element minOccurs="0" name="colorComponents"
                        type="diggs:ColorComponentsPropertyType"/>
                    <choice>
                        <element name="abundanceCode" type="gml:CodeType" minOccurs="0">
                            <annotation>
                                <documentation>A measure of the relative amount of the color present
                                    as a string type, intended to come from a controlled list (eg.
                                    few, rare, common, etc.)</documentation>
                            </annotation>
                        </element>
                        <element name="abundancePercent" type="diggs:AbundanceMeasureType"
                            minOccurs="0">
                            <annotation>
                                <documentation>A measure of the amount of the color present,
                                    expressed as a percentage.</documentation>
                            </annotation>
                        </element>
                    </choice>
                </sequence>
                <attribute name="character" type="diggs:ColorCharacterEnumType">
                    <annotation>
                        <documentation>Describes the character of the color(s) in the associated
                            unit, from the following enumerated list: banded - multiple colors occur
                            in horizontal layers within the unit mottled - unit consists of a
                            pattern of irregular spots, blotches or patches of a different color
                            than the unit matrix color multicalored - unit consists of multiple
                            colors where there is no description of the pattern or character of the
                            various colors uniform - unit consists of a single color variegated - a
                            multicolored unit containing spots, streaks, or blotches of multiple
                            colors </documentation>
                    </annotation>
                </attribute>
                <attribute name="origin" type="diggs:ColorOriginEnumType">
                    <annotation>
                        <documentation>A term from an enumerated list that identifies the location
                            (eg matrix or mottle), or the source/cause of the color described.
                            Defined terms are: matrix - the color of the soil or rock matrix mottle
                            - the color of soil mottles clay depletion - color caused by depletion
                            of clay iron depletion - color caused by depletion of iron iron Fe2 mass
                            - color caused by masses of reduced iron iron Fe3 mass - color caused by
                            masses of oxidized iron iron-manganese mass - color caused by masses of
                            iron and manganese jarosite mass - color caused by masses of jarosite
                            manganese mass - color caused by masses of manganese ironstone module -
                            color caused by ironstone nodules iron-manganese nodule - color caused
                            by iron-manganese nodules jarosite nodule - color caused by jarosite
                            nodules plinthite - color caused by plinthite(s) (an iron-rich,
                            humus-poor mixture of clay with quartz and other minerals).
                            iron-manganese concretion - color caused by iron-manganese concretions
                            manganese coat or film - color caused by manganese coats or films
                            disseminated carbonate - color caused by disseminated calcium carbonate
                            disseminated salts - color caused by disseminated unspecified salts
                            disseminated gypsum - color caused by disseminated gypsum barite mass -
                            color caused by masses of barite gypsum mass - color caused by masses of
                            gypsum carbonate mass - color caused by masses of calcium carbonate salt
                            mass - color caused by masses of unspecified salt clay body - color
                            caused by included bodies of clay silica mass - color caused by masses
                            of silica gypsum crystal clusters - color caused by crystal clusters of
                            gypsum carbonate nodule - color caused by nodules of carbonate durinode
                            (SiO2) nodule - color caused by nodules of SiO2 gibbsite (Al1O3) nodule
                            - color caused by nodules of Al2O3 opal nodule - color caused by nodules
                            of opal ortstein nodule - color caused by hardpans cemented with iron
                            and organic matter carbonate concretion - color caused by concretions of
                            calcium carbonate gibbsite concretion - color caused by concretions of
                            gibbsite silica concretion - color caused by concretions of silica
                            titanium oxide - color caused by concretions of titanium oxide barite
                            crystals - color caused by crystals of barite calcite crystals - color
                            caused by calcite crystals gypsum crystals - color caused by gypsum
                            crystals salt crystals - color caused by salt crystals satin spar
                            crystals - color caused by crystals of satin spar gypsum selenite
                            crystals - color caused by crystals of selenite gypsum</documentation>
                    </annotation>
                </attribute>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ColorComponentsType">
        <complexContent>
            <extension base="diggs:AbstractComponentObjectBaseType">
                <sequence>
                    <element minOccurs="0" name="intensity" type="diggs:intensityEnumExtType">
                        <annotation>
                            <documentation>The first value in the color description if used. For soils and rock this is typically either a value or chroma term, (eg. pale, dark, brilliant, medium, dusky). From an enumerated list.

</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="modifier" type="diggs:modifierEnumExtType">
                        <annotation>
                            <documentation>The second term of a color description, if used (or first if intensity is not used), that modifies the base hue value, eg. yellowish, reddish, creamy, olive, etc.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="1" name="hue" type="diggs:hueEnumExtType">
                        <annotation>
                            <documentation>The base hue of the soil or rock color (eg. Black, Tan, Buff, Gray, Red, etc.). This property is mandatory.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ColorComponentsPropertyType">
        <sequence>
            <element ref="diggs:ColorComponents"/>
        </sequence>
    </complexType>
    <element name="ColorComponents" type="diggs:ColorComponentsType">
        <annotation>
            <documentation>An object that holds the components of a Munsell color description (intensity, modifier, and hue). These components can be used to programmatically construct a color description, for example:
1) intensity = dark
2) modifier = greenish
3) hue = Gray
can be used to construct a color name of "dark greenish Grey" which, for example, would correspond to one or more Munsell color codes.</documentation>
        </annotation>
    </element>
    <element name="ColorSystem" substitutionGroup="diggs:AbstractObservationSystem"
        type="diggs:ColorSystemType">
        <annotation>
            <documentation>An object that contains a system of color observations.</documentation>
        </annotation>
    </element>
    <complexType name="ColorSystemType">
        <complexContent>
            <extension base="diggs:AbstractObservationSystemType">
                <sequence>
                    <element name="colorType" type="gml:CodeType" minOccurs="0">
                        <annotation>
                            <documentation>A text string defining the kind of "Color Description"
                                system described *("eg. Munsell Color", "wet color".
                            </documentation>
                        </annotation>
                    </element>
                    <element name="colorObservation" type="diggs:ColorObservationPropertyType"
                        maxOccurs="unbounded"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="ComponentLithology" substitutionGroup="diggs:AbstractObject"
        type="diggs:ComponentLithologyType">
        <annotation>
            <documentation>A component lithology is a distict lithologic type that occurs within a lithologic zone (observation) and not descxribed as part of the primary lithology. This primarily occurs in situations where the zone is interbedded and complex enough that separating out each lithologic layer is not practical, or where one lithology occurs as inclusion within another.</documentation>
        </annotation>
    </element>
    <complexType name="ComponentLithologyPropertyType">
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:ComponentLithology"/>
        </sequence>
    </complexType>
    <complexType name="ComponentLithologyType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence maxOccurs="1" minOccurs="1">
                    <element maxOccurs="1" minOccurs="1" name="lithology"
                        type="diggs:LithologyPropertyType"/>
                    <choice>
                        <element name="abundanceCode" type="gml:CodeType" minOccurs="0">
                            <annotation>
                                <documentation>A measure of the relative amount of the subordinate
                                    lithology present as a string type, intended to come from a
                                    controlled list (eg. few, rare, common, etc.)</documentation>
                            </annotation>
                        </element>
                        <element name="abundancePercent" type="diggs:AbundanceMeasureType"
                            minOccurs="0">
                            <annotation>
                                <documentation>A measure of the amount of the subordinate lithology
                                    present, expressed as a percentage.</documentation>
                            </annotation>
                        </element>
                    </choice>
                </sequence>
                <attribute name="association" type="diggs:LithologyCharacterEnumType"/>
            </extension>
        </complexContent>
    </complexType>
    <element name="Constituent" type="diggs:ConstituentType">
        <annotation>
            <documentation>An object that describes minor constituents within a lithology
                zone.</documentation>
        </annotation>
    </element>
    <simpleType name="ConstituentCategoryEnumExtType">
        <union memberTypes="diggs:ConstituentCategoryEnumType diggs:OtherNameType"/>
    </simpleType>
    <simpleType name="ConstituentCategoryEnumType">
        <annotation>
            <documentation>Categories of constituents found in sediments and soils</documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="diagenetic features">
                <annotation>
                    <documentation>Physical, chemical, and biological changes that occur in sediments after their initial deposition and during and after their lithification. Includes features like concretions, nodules, mineral replacement, cementation, and dissolution structures.</documentation>
                </annotation>
            </enumeration>
            <enumeration value="fossils">
                <annotation>
                    <documentation>Preserved remains, impressions, or traces of ancient life forms in sedimentary rocks. Includes body fossils such as shells, bones, teeth, leaves, and other preserved organic remains of once-living organisms.</documentation>
                </annotation>
            </enumeration>
            <enumeration value="hydrocarbons">
                <annotation>
                    <documentation>Organic compounds consisting of hydrogen and carbon, including oil, natural gas, bitumen, and other petroleum products found within sedimentary rocks and soils. May appear as staining, residues, or accumulations.</documentation>
                </annotation>
            </enumeration>
            <enumeration value="lithologic">
                <annotation>
                    <documentation>Rock and mineral components of the soil or sediment, including the present of sand/silt/clay, mineral or rock fragment.</documentation>
                </annotation>
            </enumeration>
            <enumeration value="sedimentary structures">
                <annotation>
                    <documentation>Features formed during or shortly after sediment deposition, reflecting the physical processes of sedimentation. Includes bedding, lamination, cross-stratification, ripple marks, mud cracks, and graded bedding.</documentation>
                </annotation>
            </enumeration>
            <enumeration value="trace fossils">
                <annotation>
                    <documentation>Preserved evidence of biological activity rather than preserved remains of the organism itself. Includes burrows, tracks, trails, borings, and other signs of organism behavior preserved in sedimentary rocks.</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>
    <element name="ConstituentObservation" substitutionGroup="diggs:AbstractObservation"
        type="diggs:ConstituentObservationType">
        <annotation>
            <documentation>Constituent observations describe details of earth materials encountered
                within a sampling feature. Constituent observations that are restricted to a
                specific lithologic zone are encoded as constituent properties in the Lithology
                zones themselves. </documentation>
        </annotation>
    </element>
    <complexType name="ConstituentObservationPropertyType">
        <sequence maxOccurs="1">
            <element maxOccurs="1" ref="diggs:ConstituentObservation"/>
        </sequence>
    </complexType>
    <complexType name="ConstituentObservationType">
        <complexContent>
            <extension base="diggs:AbstractObservationType">
                <sequence>
                    <element name="constituent" type="diggs:ConstituentPropertyType"
                        maxOccurs="unbounded"/>
                    <element maxOccurs="unbounded" minOccurs="0" ref="diggs:placedObservation"/>
                    <element minOccurs="0" ref="diggs:baseBoundary">
                        <annotation>
                            <documentation>Description of the boundary at the base of the observed
                                zone or interval as defined in the observation's location
                                property</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ConstituentPropertyType">
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:Constituent">
                <annotation>
                    <documentation>A name (codeValue) that identifies a constituent within the extent of the described lithology (required) and optional description, along with a description of the abundance and distribution of this constituent (if applicable) within the lithologic extent described.</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>
    <element name="ConstituentSystem" substitutionGroup="diggs:AbstractObservationSystem"
        type="diggs:ConstituentSystemType">
        <annotation>
            <documentation>An object that contains a system of constituent observations.
                Observations within a constituent system need not be continuous and may overlap, but
                zones with the same named constituents cannot overlap.</documentation>
        </annotation>
    </element>
    <complexType name="ConstituentSystemType">
        <complexContent>
            <extension base="diggs:AbstractObservationSystemType">
                <sequence>
                    <element name="constituentType" type="gml:CodeType">
                        <annotation>
                            <documentation>A text string defining the kind of "Constituent
                                Description" system described (eg. :"Fossils" or "Diagenesis".
                            </documentation>
                        </annotation>
                    </element>
                    <element name="constituentObservation"
                        type="diggs:ConstituentObservationPropertyType" maxOccurs="unbounded"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ConstituentType">
        <complexContent>
            <extension base="diggs:AbstractComponentObjectBaseType">
                <sequence>
                    <element ref="gml:description" minOccurs="0"/>
                    <element minOccurs="0" ref="diggs:status">
                        <annotation>
                            <documentation>A text string to provide information about the status of
                                the object to be used to track QA/QC controls, review or release
                                status, etc. The values used are intended to come from a controlled
                                list of terms maintained by an organization.</documentation>
                        </annotation>
                    </element>
                    <element name="remark" type="diggs:RemarkPropertyType" minOccurs="0"
                        maxOccurs="unbounded">
                        <annotation>
                            <documentation>Any comment or remark associated with this specific
                                feature, the person or organization making the comment, and the time
                                comment was made.Any comment or remark associated with this specific
                                feature, the person or organization making the comment, and the time
                                comment was made.</documentation>
                        </annotation>
                    </element>
                    <element name="codeValue" type="diggs:ConstituentCategoryCodeType" minOccurs="0">
                        <annotation>
                            <documentation>The code value that defines the consituent (eg. "SM" or
                                "gastropod"; preferably defined by an authority identified in the
                                codespace attribute</documentation>
                        </annotation>
                    </element>
                    <choice>
                        <element name="abundanceCode" type="gml:CodeType" minOccurs="0">
                            <annotation>
                                <documentation>A measure of the relative amount of the constituent
                                    present as a string type, intended to come from a controlled
                                    list (eg. few, rare, common, etc.)</documentation>
                            </annotation>
                        </element>
                        <element name="abundancePercent" type="diggs:AbundanceMeasureType"
                            minOccurs="0">
                            <annotation>
                                <documentation>A measure of the amount of the constituent present,
                                    expressed as a percentage.</documentation>
                            </annotation>
                        </element>
                    </choice>
                    <element name="distribution" type="gml:CodeType" minOccurs="0">
                        <annotation>
                            <documentation>A description of how the constituent is distributed
                                within the interval (eg. scattered, sporadic, locally, etc.).
                                Intended to come from a controlled list</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="constituentProperties"
                        type="diggs:ConstituentPropertiesPropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="false" name="Container" substitutionGroup="diggs:AbstractObject"
        type="diggs:ContainerType">
        <annotation>
            <documentation>A physical object that holds a sample</documentation>
        </annotation>
    </element>
    <complexType name="ContainerPropertyType">
        <sequence>
            <element ref="diggs:Container" maxOccurs="1" minOccurs="1"/>
        </sequence>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <complexType name="ContainerType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element minOccurs="0" name="containerLabel" type="diggs:DiggsStringType">
                        <annotation>
                            <documentation>Identifier listed on the container holding the sample.
                                This might be a box number (if for core) or sample number, or the
                                same value as the sample name</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="containerType" type="gml:CodeType">
                        <annotation>
                            <documentation>Description of container for the sample; intended to be
                                derived from a controlled list.
                                Example:<![CDATA[Jar, Bucket, Vial, Bag, Box]]></documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="containerMaterial" type="gml:CodeType">
                        <annotation>
                            <documentation>Description of the material that the container is made
                                of.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="containerHeight" type="eml:LengthMeasure">
                        <annotation>
                            <documentation>Height of the container</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="containerLength" type="eml:LengthMeasure">
                        <annotation>
                            <documentation>Length of the container</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="containerVolume" type="eml:VolumeMeasure">
                        <annotation>
                            <documentation>Volume of the container</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="containerWeight" type="eml:ForceMeasure">
                        <annotation>
                            <documentation>Weight of the container</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="containerWidth" type="eml:LengthMeasure">
                        <annotation>
                            <documentation>Width of the container</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="preservativeAdded" type="gml:CodeType">
                        <annotation>
                            <documentation>Example:<![CDATA[The type of preservative added to sample, if any.]]></documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="Contract" type="diggs:ContractType" substitutionGroup="diggs:AbstractFeature"
        abstract="false">
        <annotation>
            <documentation>An object that contains properties of a business
                contract.</documentation>
        </annotation>
    </element>
    <complexType name="ContractPropertyType">
        <sequence>
            <element ref="diggs:Contract" minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>
    <complexType name="ContractType">
        <complexContent>
            <extension base="diggs:AbstractFeatureType">
                <sequence>
                    <element name="version" type="diggs:DiggsStringType" minOccurs="0" maxOccurs="1"/>
                    <element minOccurs="0" name="contractDate" type="gml:TimePositionType"/>
                    <element minOccurs="0" name="revisionDate" type="gml:TimePositionType"
                        maxOccurs="unbounded"/>
                    <element name="type" type="gml:CodeType" minOccurs="0" maxOccurs="1"/>
                    <element name="value" type="witsml:Cost" minOccurs="0" maxOccurs="1">
                        <annotation>
                            <documentation>The total value of the contract; the currency attribute
                                holds the currency type.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="CoringDevice" type="diggs:CoringDeviceType"
        substitutionGroup="diggs:AbstractEquipment" abstract="false">
        <annotation>
            <documentation>An object that contains properties of a borehole coring
                device.</documentation>
        </annotation>
    </element>
    <complexType name="CoringDeviceType">
        <complexContent>
            <extension base="diggs:AbstractEquipmentType">
                <sequence>
                    <element minOccurs="0" name="areaRatio" type="eml:AreaPerAreaMeasure">
                        <annotation>
                            <documentation>A sample disturbance parameter, defined as the difference
                                of the squares of the outside and inside diameter of the cutting
                                shoe, divided by the square of the inside diameter of the cutting
                                shoe.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="barrelExtension" type="eml:LengthMeasure">
                        <annotation>
                            <documentation>Distance that coring device extends beyond drill bit at
                                full extension. Negative values indicate barrel is recessed behind
                                bit.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="barrelInnerDiameter" type="eml:LengthMeasure">
                        <annotation>
                            <documentation>Diamter of inside of core barrel or sampling tube
                            </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="barrelOuterDiameter" type="eml:LengthMeasure">
                        <annotation>
                            <documentation>Diameter of outside of core barrel or sampling
                                tube</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="barrelLength" type="eml:LengthMeasure">
                        <annotation>
                            <documentation>Length of core barrel or sampling tube</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="insideClearanceRatio"
                        type="eml:LengthPerLengthMeasure">
                        <annotation>
                            <documentation>The inside sample diameter minus the cutting shoe inside
                                diameter divided by the cutting shoe inside diameter, expressed as a
                                percentage. A sample disturbance parameter.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="linerPresent" type="boolean">
                        <annotation>
                            <documentation>True if a liner is present in the barrel.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="linerType" type="gml:CodeType">
                        <annotation>
                            <documentation>The type of liner material or the type of coating within
                                the coring device (eg. porcelain coating on a shelby tube). Intended
                                to come from a controlled list of terms.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="outsideClearanceRatio"
                        type="eml:LengthPerLengthMeasure">
                        <annotation>
                            <documentation>The outside cutting shoe diameter minus the outside
                                sampler diameter, divided by the outside sampler diameter, expressed
                                as a percentage. A sample disturbance parameter.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="shoeInnerDiameter" type="eml:LengthMeasure">
                        <annotation>
                            <documentation>Inner diameter of the cutting shoe</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="shoeLength" type="eml:LengthMeasure">
                        <annotation>
                            <documentation>Length of the core shoe, eg. the device that fastens onto
                                the lower end of the main core barrel.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="shoeOuterDiameter" type="eml:LengthMeasure">
                        <annotation>
                            <documentation>Inner diameter of the cutting shoe</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="shoeType" type="gml:CodeType">
                        <annotation>
                            <documentation>Refers to the type of shoe used, typically manufacturer
                                specific.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
