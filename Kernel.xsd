    <complexType name="DiscontinuityObservationType">
        <annotation>
            <documentation>A discontinuity or zone of discontinuities</documentation>
        </annotation>
        <complexContent>
            <extension base="diggs:AbstractObservationType">
                <sequence>
                    <element name="fractureObservation" type="diggs:FractureObservationPropertyType"
                        minOccurs="0"/>
                    <element name="fractureSpacing" minOccurs="0"
                        type="diggs:FractureSpacingPropertyType"/>
                    <element maxOccurs="unbounded" minOccurs="0" ref="diggs:placedObservation"/>
                    <element minOccurs="0" ref="diggs:baseBoundary">
                        <annotation>
                            <documentation>Description of the boundary at the base of the observed
                                zone or interval as defined in the observation's location
                                property</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="DiscontinuityObservation" substitutionGroup="diggs:AbstractObservation"
        type="diggs:DiscontinuityObservationType">
        <annotation>
            <documentation>Descriptions of faults, fractures and joints and their spacing, with attendant mechanical and hydraulic properties. Individual
                discontinuities or zone of discontinuities within a discontinuity system may
                overlap.</documentation>
        </annotation>
    </element>
    <complexType name="DiscontinuityObservationPropertyType">
        <sequence maxOccurs="1">
            <element maxOccurs="1" ref="diggs:DiscontinuityObservation"/>
        </sequence>
    </complexType>
    <element name="DiscontinuitySystem" substitutionGroup="diggs:AbstractObservationSystem"
        type="diggs:DiscontinuitySystemType">
        <annotation>
            <documentation>An object that contains a system of discontinuity
                observations.</documentation>
        </annotation>
    </element>
    <complexType name="DiscontinuitySystemType">
        <complexContent>
            <extension base="diggs:AbstractObservationSystemType">
                <sequence>
                    <element name="discontinuityType" type="gml:CodeType">
