    <simpleType name="DescriptorMethodEnumType">
        <annotation>
            <documentation/>
        </annotation>
        <restriction base="string">
            <enumeration value="assigned">
                <annotation>
                    <documentation>Value is assigned a priori as a commonly used value or by assumption</documentation>
                </annotation>
            </enumeration>
            <enumeration value="calculated">
                <annotation>
                    <documentation>Value is computed directly from other value(s)</documentation>
                </annotation>
            </enumeration>
            <enumeration value="extrapolated">
                <annotation>
                    <documentation>Value is estimated by extrapolating trends in the known data</documentation>
                </annotation>
            </enumeration>
            <enumeration value="graphic">
                <annotation>
                    <documentation>Value is manually picked from a graph or visual display</documentation>
                </annotation>
            </enumeration>
            <enumeration value="inferred">
                <annotation>
                    <documentation>Value is estimated from some indirect measurements such as from a wireline log or transducer.</documentation>
                </annotation>
            </enumeration>
            <enumeration value="interpolated">
                <annotation>
                    <documentation>Value is estimated betweem two known values using some presumed
 function</documentation>
                </annotation>
            </enumeration>
            <enumeration value="maximum">
                <annotation>
                    <documentation>Value is the largest value inferred for the property</documentation>
                </annotation>
            </enumeration>
            <enumeration value="measured">
                <annotation>
                    <documentation>Value is determined directly from a laboratory or in situ test </documentation>
                </annotation>
            </enumeration>
            <enumeration value="minimum">
                <annotation>
                    <documentation>Value is the smallest value inferred for the property</documentation>
                </annotation>
            </enumeration>
            <enumeration value="missing">
                <annotation>
                    <documentation>Method known but not reported here</documentation>
                </annotation>
            </enumeration>
            <enumeration value="modeled">
                <annotation>
                    <documentation>Value is estimated from a representation of the measured feature</documentation>
                </annotation>
            </enumeration>
            <enumeration value="moving average">
                <annotation>
                    <documentation>Value is calculated as a moving average of reading</documentation>
                </annotation>
            </enumeration>
            <enumeration value="other">
                <annotation>
                    <documentation>Method other than the choices in this list. Further information should be provided in the remarks property</documentation>
                </annotation>
            </enumeration>
            <enumeration value="reported">
                <annotation>
                    <documentation>Value or description comes from a third party source</documentation>
                </annotation>
            </enumeration>
            <enumeration value="unknown">
                <annotation>
                    <documentation>Method of determination unknown and can likely not be determined </documentation>
                </annotation>
            </enumeration>
            <enumeration value="visual">
                <annotation>
                    <documentation>Value or description comes from visual or visual/manual inspection</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>
    <complexType name="MeasurementTechniqueEnumType">
