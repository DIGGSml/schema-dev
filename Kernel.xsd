    <element name="WaterStrike" substitutionGroup="diggs:AbstractObject"
        type="diggs:WaterStrikeType">
        <annotation>
            <documentation>An object containing properties related to encountering the water table
                or a perched water zone during borehole construction.</documentation>
        </annotation>
    </element>
    <complexType name="WaterStrikePropertyType">
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:WaterStrike"/>
        </sequence>
    </complexType>
    <element name="WaterStrikeReading" substitutionGroup="diggs:AbstractObject"
        type="diggs:WaterStrikeReadingType">
        <annotation>
            <documentation>An element containing water level reading properties following a water
                strick.</documentation>
        </annotation>
    </element>
    <complexType name="WaterStrikeReadingPropertyType">
        <sequence maxOccurs="1" minOccurs="1">
            <element ref="diggs:WaterStrikeReading"/>
        </sequence>
    </complexType>
    <complexType name="WaterStrikeReadingType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <choice minOccurs="0">
                        <element name="howMeasured" type="eml:TimeMeasure" minOccurs="0">
                            <annotation>
                                <documentation>A description of how the water level was measured,
                                    intended to come from a controlled list of terms</documentation>
                            </annotation>
                        </element>
                        <element name="elapsedTime" type="eml:TimeMeasure">
                            <annotation>
                                <documentation><![CDATA[The elapsed time following an initial water strike when water level is again measured]]></documentation>
                            </annotation>
                        </element>
                        <element name="time" type="eml:TimeMeasure">
                            <annotation>
                                <documentation><![CDATA[The elapsed time following an initial water strike when water level is again measured. Deprecated following v. 2.5. Use elapsedTime in the choice instead]]></documentation>
                            </annotation>
                        </element>
                        <element name="dateTime" type="gml:TimePositionType" minOccurs="1">
                            <annotation>
                                <documentation><![CDATA[DateTime that water strike measurement occurred.]]></documentation>
                            </annotation>
                        </element>
                    </choice>
                    <element name="waterLocation" type="diggs:PointLocationPropertyType">
                        <annotation>
                            <documentation>The measure depth to water; measured in a hole's linear
                                referencing system.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="bottomCasing"
                        type="diggs:PointLocationPropertyType">
                        <annotation>
                            <documentation>Measured depth to base of casing at time water strike
                                occurred; measured in borehole linear referencing
                                system.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="WaterStrikeType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence minOccurs="0">
                    <choice>
                        <element name="notEncountered" fixed="true">
                            <simpleType>
                                <restriction base="boolean"/>
                            </simpleType>
                        </element>
                        <sequence>
                            <element name="initialWaterStrikeReading"
                                type="diggs:WaterStrikeReadingPropertyType">
                                <annotation>
                                    <documentation>Water level information at time of initial water
                                        strike</documentation>
                                </annotation>
                            </element>
                            <element maxOccurs="unbounded" minOccurs="0" name="postStrikeReading"
                                type="diggs:WaterStrikeReadingPropertyType">
                                <annotation>
                                    <documentation>A series of water level readings that occur at
                                        times following the initial water strike</documentation>
                                </annotation>
                            </element>
                            <element minOccurs="0" name="sealLocation"
                                type="diggs:PointLocationPropertyType">
                                <annotation>
                                    <documentation>Location at which water strike is sealed by
                                        casing; measured in borehole linear referencing
                                        system.</documentation>
                                </annotation>
                            </element>
                        </sequence>
                    </choice>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
