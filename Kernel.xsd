    <simpleType name="WaterContentMethodEnumType">
        <restriction base="string">
            <enumeration value="full specimen"/>
            <enumeration value="trimmings"/>
            <enumeration value="unknown"/>
        </restriction>
    </simpleType>
    <simpleType name="WaterContentMethodType">
        <annotation>
            <documentation>A value that describes the specific approach by which the water content
