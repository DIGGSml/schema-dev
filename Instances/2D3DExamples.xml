                <PointLocation srsName="urn:diggs:def:crs:DIGGS:0.1:26911_5703" srsDimension="3"
                    gml:id="a34">
                    <gml:pos>387516.665116977 3742645.12297961 5</gml:pos>
                </PointLocation>
            </referencePoint>
            <referenceEdge>
                <LinearExtent srsName="urn:diggs:def:crs:DIGGS:0.1:26911_5703" srsDimension="3"
                    gml:id="ply1_top">
                    <gml:posList>387516.665116977 3742645.12297961 5 387546.665116977
                        3742685.12297961 5 </gml:posList>
                </LinearExtent>
            </referenceEdge>
            <!--featureExtent and/or relativeFeatureBoundary may be used to define the feature extent. 
                featureExtent must be used if no linear spatial reference system is defined for the sampling feature -->
