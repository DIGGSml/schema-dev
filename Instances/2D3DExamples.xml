    <samplingFeature>
        <VolumetricSamplingFeature gml:id="vol1">
            <gml:name>Seismic volume</gml:name>
            <investigationTarget>Natural Ground</investigationTarget>
            <projectRef xlink:href="#p1"/>
            <referencePoint>
                <PointLocation srsName="urn:diggs:def:crs:DIGGS:0.1:26911_5703" srsDimension="3"
                    gml:id="rpv">
                    <gml:pos>387516 3742645 -5</gml:pos>
                </PointLocation>
            </referencePoint>
            <referenceEdge>
                <LinearExtent srsName="urn:diggs:def:crs:DIGGS:0.1:26911_5703" srsDimension="3"
                    gml:id="rev">
                    <gml:posList>387516 3742645 -5 387546 3742645 -5</gml:posList>
                </LinearExtent>
            </referenceEdge>
            <featureExtent>
                <Solid srsName="urn:diggs:def:crs:DIGGS:0.1:26911_5703" srsDimension="3"
                    gml:id="fev">
                    <!-- A rectangular prism -->
                    <exterior>
                        <Shell aggregationType="set" gml:id="fev-shell">
                            <surfaceMembers>
                                <!-- Top surface -->
                                <PlanarSurface gml:id="fev-sm1">
                                    <gml:exterior>
                                        <gml:LinearRing>
                                            <gml:posList>387516 3742645 5 387546 3742645 5 387546
                                                3742685 5 387516 3742685 5 387516 3742645
                                                5</gml:posList>
                                        </gml:LinearRing>
                                    </gml:exterior>
                                </PlanarSurface>
                              <!-- Bottom surface -->
                                <PlanarSurface gml:id="fev-sm2">
                                    <gml:exterior>
                                        <gml:LinearRing>
                                            <gml:posList>387516 3742645 -5 387546 3742645 -5 387546
                                                3742685 -5 387516 3742685 -5 387516 3742645
                                                -5</gml:posList>
                                        </gml:LinearRing>
                                    </gml:exterior>
                                </PlanarSurface>
                               <!-- Western vertical surface -->
                                <PlanarSurface gml:id="fev-sm3">
                                    <gml:exterior>
                                        <gml:LinearRing>
                                            <gml:posList>387516 3742645 -5 387516 3742685 -5 387516
                                                3742685 5 387516 3742645 5 387516 3742645 -5
                                            </gml:posList>
                                        </gml:LinearRing>
                                    </gml:exterior>
                                </PlanarSurface>
                              <!-- Eastern vertical surface -->
                                <PlanarSurface gml:id="fev-sm4">
                                    <gml:exterior>
                                        <gml:LinearRing>
                                            <gml:posList>387546 3742645 -5 387546 3742685 -5 387546
                                                3742685 5 387546 3742645 5 387546 3742645 -5
                                            </gml:posList>
                                        </gml:LinearRing>
                                    </gml:exterior>
                                </PlanarSurface>
                               <!-- Northern vertical surface -->
                                <PlanarSurface gml:id="fev-sm5">
                                    <gml:exterior>
                                        <gml:LinearRing>
                                            <gml:posList>387516 3742685 -5 387546 3742685 -5 387546
                                                3742685 5 387516 3742685 5 387516 3742685 -5
                                            </gml:posList>
                                        </gml:LinearRing>
                                    </gml:exterior>
                                </PlanarSurface>
                               <!-- Southern vertical surface -->
                                <PlanarSurface gml:id="fev-sm6">
                                    <gml:exterior>
                                        <gml:LinearRing>
                                            <gml:posList>387516 3742645 -5 387546 3742645 -5 387546
                                                3742645 5 387516 3742645 5 387516 3742645 -5
                                            </gml:posList>
                                        </gml:LinearRing>
                                    </gml:exterior>
                                </PlanarSurface>
                            </surfaceMembers>
                        </Shell>
                    </exterior>
                </Solid>
            </featureExtent>
            <relativeFeatureExtent>
                <!-- Same solid using relative reference system coordinates -->
                <Solid srsName="#vsrs001" srsDimension="3" gml:id="rfbv">
                    <exterior>
                        <Shell aggregationType="set" gml:id="rfvb-shell">
                            <surfaceMembers>
                                <!-- Top surface -->
                                <PlanarSurface gml:id="rfbv--sm1">
                                    <gml:exterior>
                                        <gml:LinearRing>
                                            <gml:posList>0 0 5 30 0 5 30 40 5 0 40 5 0 0
                                                5</gml:posList>
                                        </gml:LinearRing>
                                    </gml:exterior>
                                </PlanarSurface>
                                <!-- Bottom surface -->
                                <PlanarSurface gml:id="rfbv--sm2">
                                    <gml:exterior>
                                        <gml:LinearRing>
                                            <gml:posList>0 0 -5 30 0 -5 30 40 -5 0 40 -5 0 0
                                                -5</gml:posList>
                                        </gml:LinearRing>
                                    </gml:exterior>
                                </PlanarSurface>
                                <!-- Western vertical surface -->
                                <PlanarSurface gml:id="rfbv--sm3">
                                    <gml:exterior>
                                        <gml:LinearRing>
                                            <gml:posList>0 0 -5 0 40 -5 0 40 5 0 0 5 0 0
                                                -5</gml:posList>
                                        </gml:LinearRing>
                                    </gml:exterior>
                                </PlanarSurface>
                                <!-- Eastern vertical surface -->
                                <PlanarSurface gml:id="rfbv--sm4">
                                    <gml:exterior>
                                        <gml:LinearRing>
                                            <gml:posList>30 0 -5 30 40 -5 30 40 5 30 0 5 30 0
                                                -5</gml:posList>
                                        </gml:LinearRing>
                                    </gml:exterior>
                                </PlanarSurface>
                                <!-- Northern vertical surface -->
                                <PlanarSurface gml:id="rfbv--sm5">
                                    <gml:exterior>
                                        <gml:LinearRing>
                                            <gml:posList>0 40 -5 30 40 -5 30 40 5 0 40 5 0 40
                                                -5</gml:posList>
                                        </gml:LinearRing>
                                    </gml:exterior>
                                </PlanarSurface>
                                <!-- Southern vertical surface -->
                                <PlanarSurface gml:id="rfbv--sm6">
                                    <gml:exterior>
                                        <gml:LinearRing>
                                            <gml:posList>0 0 -5 30 0 -5 30 0 5 0 0 5 0 0
                                                -5</gml:posList>
                                        </gml:LinearRing>
                                    </gml:exterior>
                                </PlanarSurface>
                            </surfaceMembers>
                        </Shell>
                    </exterior>
                </Solid>
            </relativeFeatureExtent>
            <volumeReferencing>
                <VectorLinearSpatialReferenceSystem gml:id="vsrs001">
                    <gml:identifier codeSpace="DIGGS">vsrs001</gml:identifier>
                    <glr:linearElement xlink:href="#rev"/>
                    <glr:lrm>
                        <glr:LinearReferencingMethod gml:id="lrm-vfe">
                            <glr:name>chainage</glr:name>
                            <glr:type>absolute</glr:type>
                            <glr:units>m</glr:units>
                        </glr:LinearReferencingMethod>
                    </glr:lrm>
                    <offsetVector>0 1 0</offsetVector>
                    <!-- Offset vector pointing north for y coordinate direction -->
                    <offsetVector>0 0 1</offsetVector>
                    <!-- Offset vector pointing up for z doordinate direction -->
                </VectorLinearSpatialReferenceSystem>
            </volumeReferencing>
        </VolumetricSamplingFeature>
    </samplingFeature>

