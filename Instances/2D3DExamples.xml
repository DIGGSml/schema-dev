                <PlanarSurface srsName="urn:diggs:def:crs:DIGGS:0.1:26911_5703" srsDimension="3"
                    gml:id="ply2">
                    <gml:exterior>
                        <gml:LinearRing>
                            <gml:posList>387516.665116977 3742645.12297961 5 387546.665116977
                                3742685.12297961 5 387546.665116977 3742685.12297961 -5
                                387516.665116977 3742645.12297961 -5 387516.665116977
                                3742645.12297961 5</gml:posList>
                        </gml:LinearRing>
                    </gml:exterior>
                </PlanarSurface>
            </featureExtent>
            <relativeFeatureBoundary>
                <PlanarSurface srsDimension="2" srsName="#LocalCartesianCRS_igf_ldl_vhc" gml:id="ply1">
