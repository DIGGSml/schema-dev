        <annotation>
            <documentation>Property type for line loss data references. Used to wrap line loss data objects or references in properties.</documentation>
        </annotation>
        <sequence>
            <element ref="diggs:LineLossData"/>
        </sequence>
    </complexType>
    <element name="LineLossTest" type="diggs:LineLossTestType"
        substitutionGroup="diggs:AbstractMaterialTestProcedure">
        <annotation>
            <documentation>A test to measure the pressure loss between the grout pump and the grout
                injection point due to friction in the grout lines.</documentation>
        </annotation>
    </element>
    <complexType name="LineLossTestType">
        <annotation>
            <documentation>Extended type based on AbstractMaterialTestlProcedureType. Adds elements for line loss test type definition.</documentation>
        </annotation>
        <complexContent>
            <extension base="diggs:AbstractMaterialTestlProcedureType">
                <sequence>
                    <element minOccurs="0" name="lineLength" type="diggs:LengthMeasureType">
                        <annotation>
                            <documentation>The length of the grout line (packer line) from the
                                pressure gauge located at the pump to the pressure gauge at the
                                grout header.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="lineLossCoeffA" type="double">
                        <annotation>
                            <documentation>Coefficient A of the regression equation generated from
                                lineLossData recorded at different flow rates.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="lineLossCoeffB" type="double">
                        <annotation>
                            <documentation>Coefficient B of the regression equation generated from
                                lineLossData recorded at different flow rates.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="lineLossCoeffC" type="double">
                        <annotation>
                            <documentation>Coefficient C of the regression equation generated from
                                lineLossData recorded at different flow rates.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="lineLossR2" type="double">
                        <annotation>
                            <documentation>The correlation coefficient (R^2) for the regression
                                equation generated from lineLossData recorded at different flow
                                rates.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="lineLossData"
                        type="diggs:LineLossDataPropertyType">
                        <annotation>
                            <documentation>The line loss data property.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="LineLossData" type="diggs:LineLossDataType">
        <annotation>
            <documentation>The line loss data property.</documentation>
        </annotation>
    </element>
    <complexType name="LineLossDataType">
        <annotation>
            <documentation>Extended type based on AbstractMetadataPropertyType. Adds elements for line loss data type definition.</documentation>
        </annotation>
        <complexContent>
            <extension base="gml:AbstractMetadataPropertyType">
                <sequence>
                    <element name="flowRate" type="diggs:VolumePerTimeMeasureType" minOccurs="0">
                        <annotation>
                            <documentation>Flow rate of the grout</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="p1" type="diggs:PressureMeasureType">
                        <annotation>
                            <documentation>Pressure measured at the pump</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="p2" type="diggs:PressureMeasureType">
                        <annotation>
                            <documentation>Pressure measured at the grout head </documentation>
                        </annotation>
                    </element>
                    <element name="lineLoss" type="diggs:PressureMeasureType" minOccurs="0">
                        <annotation>
                            <documentation>The pressure differential between the pump gauge and the
                                grout head gauge.</documentation>
                        </annotation>
                    </element>
                    <element name="lineLossNormalized" type="diggs:ForcePerVolumeMeasureType"
                        minOccurs="0">
                        <annotation>
                            <documentation>The pressure differential between the pump gauge and the
                                grout head gauge, divided by the line length between the two
                                gauges.</documentation>
                        </annotation>
                    </element>
                </sequence>
                <attribute name="index" type="positiveInteger" use="optional">
                    <annotation>
                        <documentation>An integer that indicates the order of line loss data results. Optional.</documentation>
                    </annotation>
                </attribute>
            </extension>
        </complexContent>
    </complexType>
