            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element maxOccurs="1" minOccurs="0" name="measurand"
                        type="gml:CodeWithAuthorityType">
                        <annotation>
                            <documentation>Contains the name of the property being measured and
                                reported to the data package. Intended to come from a controlled
                                list of termsm the authority or pointer to a properties dictionary
                                that defines the name used is required as the value of the codeSpace
                                attribute. </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="recordingSpace"
                        type="diggs:RecordingSpaceEnumExtType">
                        <annotation>
                            <documentation>A term that describes the recording space for the
                                detector - a controlled term. </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="measurementAxisBearing"
                        type="diggs:PlaneAngleMeasureType">
                        <annotation>
                            <documentation>Compass bearing direction (relative to true north)
                                defining the measurement axis direction (for directional
                                measurements, eg. component of an extensometer, accelerometer,
                                etc.)</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="measurementAxisInclination"
                        type="diggs:PlaneAngleMeasureType">
                        <annotation>
                            <documentation>Inclination of measurement axis (measured positively down
                                from horizontal); used for directional measurements, eg.
                                extensometer, accelerometer, etc. </documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <simpleType name="RecordingSpaceEnumType">
        <annotation>
            <documentation>1) frequency domain - the recorded values are vary with frequency 2)
                non-varying - the recorded values do not vary with frequency or time 3) time-domain
                - the recorded values vary with time</documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="frequency domain">
                <annotation>
                    <documentation>frequency domain - frequency domain value.</documentation>
                </annotation>
            </enumeration>
            <enumeration value="non-varying">
                <annotation>
                    <documentation>non-varying - non varying value.</documentation>
                </annotation>
            </enumeration>
            <enumeration value="time domain">
                <annotation>
                    <documentation>time domain - time domain value.</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>
    <simpleType name="RecordingSpaceEnumExtType">
        <annotation>
            <documentation>Union type for recording space enum ext type. Accepts values from OtherNameType or RecordingSpaceEnumType.</documentation>
        </annotation>
        <union memberTypes="diggs:OtherNameType diggs:RecordingSpaceEnumType"/>
    </simpleType>
    <element name="GP_Source" substitutionGroup="diggs:AbstractGP_Source" type="diggs:GP_SourceType">
