            </enumeration>
            <enumeration value="perpendicular">
                <annotation>
                    <documentation>perpendicular - perpendicular value.</documentation>
                </annotation>
            </enumeration>
            <enumeration value="other">
                <annotation>
                    <documentation>other - other value.</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>
