    <element abstract="false" name="SourceInfo" type="diggs:SourceInfoType">
        <annotation>
            <documentation>Contains properties about the sources and source locations associated
                with a single GP_Trackline or GP_ArealSurvey sampling feature in this configuration.
                If no specific source geometry is included, information about the spacing and number
                of sources can be provided using the sourceInterval and noSourceStations properties.
                If detailed information about specific sources and their locations are provided, The
                sourceGeometry property is used, If sourceGeometry is used, at least one GP_Source
                object must exist.</documentation>
        </annotation>
    </element>
    <complexType name="SourceInfoType">
        <annotation>
            <documentation>Extended type based on AbstractComponentObjectBaseType. Adds elements for source info type definition.</documentation>
        </annotation>
        <complexContent>
            <extension base="diggs:AbstractComponentObjectBaseType">
                <sequence>
                    <element maxOccurs="unbounded" minOccurs="0" name="remark"
                        type="diggs:RemarkPropertyType">
                        <annotation>
                            <documentation>The remark property.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="1" ref="diggs:samplingFeatureRef"/>
                    <element minOccurs="0" name="sourceType" type="diggs:sourceTypeEnumExtType">
                        <annotation>
                            <documentation>Identifies the type of source(s) used, from a controlled
                                list of terms. This property is optional, but should be instantiated
                                in the case where sourceGeometry is not used.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="sourceSpacing"
                        type="diggs:SensorSpacingPropertyType">
                        <annotation>
                            <documentation>The min, mix and/or average spacing between source
                                stations in this configuration.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="noSourceStations" type="positiveInteger">
                        <annotation>
                            <documentation>The number of source stations used in this configuration;
                                ie., the number of locations where sources originated using this
                                configuration.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="noSources" type="positiveInteger">
                        <annotation>
                            <documentation>The number of different sources (source equipment) used
                                in this configuration.</documentation>
                        </annotation>
                    </element>
                    <element name="sourceLocations" type="diggs:SourceLocationsPropertyType">
                        <annotation>
                            <documentation>Contains a SourceLocation object that assocciates
                                specific receiver instruments to their locations.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="SourceInfoArrayPropertyType">
        <annotation>
            <documentation>Property type for source info array references. Used to wrap source info array objects or references in properties.</documentation>
        </annotation>
        <sequence maxOccurs="unbounded">
            <element ref="diggs:SourceInfo">
                <annotation>
                    <documentation>Contains properties about the sources and source locations
                        associated with a single GP_Trackline or GP_ArealSurvey sampling feature in
                        this configuration. If no specific source geometry is included, information
                        about the spacing and number of receivers can be provided using the
                        sourceInterval, noSourceStations properties. If detailed information about
                        specific sources and their locations are provided, The sourceGeometry
                        property is used, If sourceGeometry is used, at least one GP_Source object
                        must exist.</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>
    <element name="SourceLocations" type="diggs:SourceLocationsType">
        <annotation>
            <documentation>An object that contains properties of the source locations and
                references.</documentation>
        </annotation>
    </element>
    <complexType name="SourceLocationsPropertyType">
        <annotation>
            <documentation>Property type for source locations references. Used to wrap source locations objects or references in properties.</documentation>
        </annotation>
        <sequence maxOccurs="1" minOccurs="0">
            <element ref="diggs:SourceLocations">
                <annotation>
                    <documentation>An object that contains details about specific source devices and
                        their locations in this configuration. The object contains two properties:
                        sourceRef and sourceLocations. sourcerRef references a sincle GP_Source
                        object. If only one GP_Source object is listed, then the sourceLocations
                        property may contain any of PointLocation, MultiPointLocation or
                        RecifiedGrid geometries. If MultiPointLocation or RectifiedGrid is used,
                        then either the GP_Source identified is successively moved to all locations
                        of the geometry or the GP_Source represents a group of receivers with
                        identical properties that are installed at each location. This distinction
                        is identified using the IsGroup property of the GP_Source object. If more
                        than one GP_Source is referenced, then use of the PointLocation geometry
                        implies that all sources are located at the same point location. If
                        MultiPointLocation or RectifiedGrid geometries are used, then there must be
                        the same number of individual locations in the geometry as there are
                        GF_Source objects referenced, and the individual locations must be in
                        sourceRef order so that each receiver's location can be
                        determined.</documentation>
                </annotation>
            </element>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <complexType name="SourceLocationsType">
        <annotation>
            <documentation>Extended type based on AbstractObjectType. Adds elements for source locations type definition.</documentation>
        </annotation>
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element maxOccurs="unbounded" name="sourceRef"
                        type="diggs:FeatureReferenceType">
                        <annotation>
