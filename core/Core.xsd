    <!-- MONITOR ELEMENTS                                                                   -->
    <!-- ================================================================================== -->

    <element name="Monitor" type="diggs:MonitorType" substitutionGroup="diggs:AbstractMeasurement">
        <annotation>
            <documentation>An act ("event"), whose results are estimates of the value of a property
                of the investigation target, measured at a time instant or within a time interval,
                and at a location or series of locations, derived from a test
                procedure.</documentation>
        </annotation>
    </element>
    <element name="MonitorResult" type="diggs:MonitorResultType"
        substitutionGroup="gml:AbstractCoverage">
        <annotation>
            <documentation>An object that contains properties of a monitoring activity
                result.</documentation>
        </annotation>
    </element>
    <complexType name="MonitorResultPropertyType">
        <annotation>
            <documentation>Property type for monitor result references. Used to wrap monitor result
                objects or references in properties.</documentation>
        </annotation>
        <sequence>
            <element ref="diggs:TemporalResult"/>
        </sequence>
    </complexType>
    <complexType name="MonitorResultType">
        <annotation>
            <documentation>Restricted type based on AbstractCoverageType. Constrains
                AbstractCoverageType with 2 element(s) for structured monitor result type
                data.</documentation>
        </annotation>
        <complexContent>
            <restriction base="gml:AbstractCoverageType">
                <sequence>
                    <element ref="diggs:timeDomain"/>
                    <element ref="diggs:results"/>
                </sequence>
            </restriction>
        </complexContent>
    </complexType>
    <complexType name="MonitorType">
        <annotation>
            <documentation>Extended type based on AbstractMeasurementType. Adds elements for monitor
                type definition.</documentation>
        </annotation>
        <complexContent>
            <extension base="diggs:AbstractMeasurementType">
                <sequence>
                    <element minOccurs="0" name="samplingTime"
                        type="diggs:TimeIntervalOrInstantPropertyType">
                        <annotation>
                            <documentation>Describes the time interval that the monitor result
                                applies to. This is an optional property but should be used where
                                the timeIntervalList is used in the timeDomain property of the
                                Monitor outcome</documentation>
                        </annotation>
                    </element>
                    <element name="monitorLocation" type="diggs:LocationPropertyType">
                        <annotation>
                            <documentation>Identifies the location associated with the monitoring
                                measurement.This may be the detector or sensor location, or the
                                response zone (such as a well screen interval) over which the
                                measurement applies. In the case where the location is a gemetry
                                aggregate, the monitor results will refer to all locations as a
                                whole. In cases where a sensor has detectors in multiple locations,
                                eqch of which is monitoring properties at the detector location, the
                                monitor results for each detector should be contained within a
                                unique, separate Monitor object.</documentation>
                        </annotation>
                    </element>
                    <element name="outcome" type="diggs:MonitorResultPropertyType">
                        <annotation>
                            <documentation>A property that contains the information about what
                                properties are being measured, the results of the measurement, and
                                the associated locations that the measurement results relate
                                to.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="procedure" type="diggs:ProcedurePropertyType"
                        maxOccurs="unbounded">
                        <annotation>
                            <documentation>The procedure property.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="TemporalResult" substitutionGroup="gml:AbstractCoverage"
        type="diggs:TemporalResultType">
        <annotation>
            <documentation>An object that contains properties and results of a monitoring activity
                result.</documentation>
        </annotation>
    </element>
    <complexType name="TemporalResultType">
        <annotation>
            <documentation>Restricted type based on AbstractCoverageType. Constrains
                AbstractCoverageType with 2 element(s) for structured temporal result type
                data.</documentation>
        </annotation>
        <complexContent>
            <restriction base="gml:AbstractCoverageType">
                <sequence>
                    <element ref="diggs:timeDomain"/>
                    <element ref="diggs:results"/>
                </sequence>
            </restriction>
        </complexContent>
    </complexType>

