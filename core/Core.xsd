    <complexType name="ConstituentCategoryCodeType">
        <annotation>
            <documentation>Concrete type extending CodeType. Provides implementation for constituent
                category code type.</documentation>
        </annotation>
        <simpleContent>
            <extension base="gml:CodeType">
                <attribute name="howDetermined" type="diggs:DescriptorMethodEnumType">
