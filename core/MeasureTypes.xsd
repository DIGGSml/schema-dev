    <simpleType name="DiggsGravitationalGradientUomEnumBaseType">
        <annotation>
            <documentation>This enumerated list adds a new quantity class to the Energistics Unit Dictionary (gravitational gradient) to provide units for gravity survey results</documentation>
        </annotation>
        <restriction base="eml:UomEnum">
            <enumeration value="1/s2">
                <annotation>
                    <documentation>per second squared</documentation>
                </annotation>
            </enumeration>
            <enumeration value="E">
                <annotation>
                    <documentation>Eötvös unit</documentation>
                </annotation>
            </enumeration>
            <enumeration value="mGal/m">
                <annotation>
                    <documentation>milligal per metre</documentation>
                </annotation>
            </enumeration>
            <enumeration value="Gal/m">
                <annotation>
                    <documentation>gal per metre</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>

    <simpleType name="DiggsParticleSizeUomEnumBaseType">
        <annotation>
            <documentation>This enumerated list restricts eml:LengthUom and adds phi unit for reporting particle size diameter.</documentation>
        </annotation>
        <restriction base="eml:UomEnum">
            <enumeration value="phi">
                <annotation>
                    <documentation>phi unit (negative log base 2 of particle diameter in millimetres)</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>

