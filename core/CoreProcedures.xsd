                        <annotation>
                            <documentation>Type or classification of the area correction.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="filterCorrectionApplied" type="boolean">
                        <annotation>
                            <documentation>The filter correction applied property.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="membraneCorrectionApplied" type="boolean">
                        <annotation>
                            <documentation>The membrane correction applied property.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="pistonFrictionCorrectionApplied" type="boolean">
