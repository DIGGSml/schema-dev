            <simpleType>
                <restriction base="string">
                    <enumeration value="Proctor">
                        <annotation>
                            <documentation>Proctor - proctor value.</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="Modified Proctor">
                        <annotation>
                            <documentation>Modified Proctor - modified proctor value.</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="California Test 216">
                        <annotation>
                            <documentation>California Test 216 - california test value.</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="Porter static">
                        <annotation>
                            <documentation>Porter static - porter static value.</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="AASHTO Method A">
                        <annotation>
                            <documentation>AASHTO Method A - aashto method a value.</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="AASHTO Method B">
                        <annotation>
                            <documentation>AASHTO Method B - aashto method b value.</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="AASHTO Method C">
                        <annotation>
                            <documentation>AASHTO Method C - aashto method c value.</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="AASHTO Method D">
                        <annotation>
                            <documentation>AASHTO Method D - aashto method d value.</documentation>
                        </annotation>
                    </enumeration>
                    <enumeration value="ASTM D5080">
                        <annotation>
                            <documentation>ASTM D5080 - astm value.</documentation>
