    <complexType name="LabCompactionTestTrialType">
        <annotation>
            <documentation>Base type for CompactionTestTrials</documentation>
        </annotation>
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element name="trialNo" type="positiveInteger" minOccurs="0">
                        <annotation>
                            <documentation>The count of the test cycle</documentation>
                        </annotation>
                    </element>
                    <element name="waterContent" type="diggs:WaterContentMeasureType" minOccurs="0">
                        <annotation>
                            <documentation>Moisture content measured in this trial.</documentation>
                        </annotation>
                    </element>
                    <element name="wetDensity" type="diggs:DensityOrUnitWeightMeasureType"
                        minOccurs="0">
                        <annotation>
                            <documentation>Density or init weight measured in this trial at specimen
                                moisture content.</documentation>
                        </annotation>
                    </element>
                    <element name="dryDensity" type="diggs:DensityOrUnitWeightMeasureType"
                        minOccurs="0">
                        <annotation>
                            <documentation>Dry unit weight measured in this trial.</documentation>
                        </annotation>
                    </element>
                    <element name="convertedWetDensityDensity"
                        type="diggs:DensityOrUnitWeightMeasureType" minOccurs="0">
