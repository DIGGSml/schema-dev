        <annotation>
            <documentation>Extended type based on AbstractObjectType. Adds elements for membrane info type definition.</documentation>
        </annotation>
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element minOccurs="0" name="membraneMaterial" type="gml:CodeType">
                        <annotation>
                            <documentation>Membrane material, eg. latex, nitrile, etc. Intended to
                                come from a controlled list of terms.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="membraneThickness" type="diggs:LengthMeasureType">
                        <annotation>
                            <documentation>The membrane thickness property.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="membraneYoungsModulus"
