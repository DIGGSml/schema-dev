                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="CBRIncrementPropertyType">
        <annotation>
            <documentation>Property type for cbr increment references. Used to wrap cbr increment objects or references in properties.</documentation>
        </annotation>
        <sequence>
            <element ref="diggs:CBRIncrement"/>
        </sequence>
    </complexType>
    <element name="LabCompactionTest" type="diggs:LabCompactionTestType" abstract="false"
        substitutionGroup="diggs:AbstractLaboratoryTestProcedure">
        <annotation>
            <documentation>An object describing a laboratory compaction test procedure. The
                Compaction Test is a laboratory method of experimentally determining the optimal
                moisture content at which a given soil type will become most dense and achieve its
                maximum dry density.</documentation>
        </annotation>
    </element>
    <complexType name="LabCompactionTestType">
        <annotation>
            <documentation>Base type for CompactionTest</documentation>
        </annotation>
        <complexContent>
            <extension base="diggs:AbstractLaboratoryTestProcedureType">
                <sequence>
                    <element maxOccurs="1" minOccurs="0" name="compactionTestType"
                        type="diggs:CompactionTestEnumType">
                        <annotation>
                            <documentation>Type or classification of the compaction test.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="fieldWetDensity"
                        type="diggs:DensityOrUnitWeightMeasureType">
                        <annotation>
                            <documentation>Wet density of the soil sample as obtained from an
                                in-situ density test 9ASTM D5080)</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="fieldDryDensity"
                        type="diggs:DensityOrUnitWeightMeasureType">
                        <annotation>
                            <documentation>Dry density of the soil sample as obtained from an
                                in-situ density test 9ASTM D5080)</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="fieldWaterContent"
                        type="diggs:ForcePerForceMeasureType">
                        <annotation>
                            <documentation>Water content of the in-place soil sample (ASTM
                                d5080)</documentation>
                        </annotation>
                    </element>
                    <element name="mouldDiameter" type="diggs:LengthMeasureType" minOccurs="0">
                        <annotation>
                            <documentation>The mould diameter property.</documentation>
                        </annotation>
                    </element>
                    <element name="mouldVolume" type="diggs:VolumeMeasureType" minOccurs="0">
                        <annotation>
                            <documentation>The mould volume property.</documentation>
                        </annotation>
                    </element>
                    <element name="rammerDiameter" type="diggs:LengthMeasureType" minOccurs="0">
                        <annotation>
                            <documentation>The rammer diameter property.</documentation>
                        </annotation>
                    </element>
                    <element name="rammerMass" type="diggs:MassMeasureType" minOccurs="0">
                        <annotation>
                            <documentation>The rammer mass property.</documentation>
                        </annotation>
                    </element>
                    <element name="rammerDrop" type="diggs:LengthMeasureType" minOccurs="0">
                        <annotation>
                            <documentation>The rammer drop property.</documentation>
                        </annotation>
                    </element>
                    <element name="rammerIsManual" type="boolean" minOccurs="0">
                        <annotation>
                            <documentation>The rammer is manual property.</documentation>
                        </annotation>
                    </element>
                    <element name="numberOfLayers" type="positiveInteger" minOccurs="0">
                        <annotation>
                            <documentation>The number of layers property.</documentation>
                        </annotation>
                    </element>
                    <element name="blowsPerLayer" type="positiveInteger" minOccurs="0">
                        <annotation>
                            <documentation>The blows per layer property.</documentation>
                        </annotation>
                    </element>
                    <element name="trial" type="diggs:LabCompactionTestTrialPropertyType"
                        minOccurs="0" maxOccurs="unbounded">
                        <annotation>
                            <documentation>The trial property.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="LabCompactionTestTrial" type="diggs:LabCompactionTestTrialType" abstract="false">
        <annotation>
            <documentation>An object containing properties of a single trial increment within a
                laobratory compaction test procedure.</documentation>
        </annotation>
    </element>
    <complexType name="LabCompactionTestTrialPropertyType">
        <annotation>
            <documentation>Property for CompactionTestTrials, required parent element that holds the
                multiple trials.</documentation>
        </annotation>
        <sequence>
            <element ref="diggs:LabCompactionTestTrial" minOccurs="1" maxOccurs="1"/>
