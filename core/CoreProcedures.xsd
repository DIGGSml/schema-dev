            <documentation>An object that contains properties about each shearing increment with the
                shear stage of a triaxial test procedure.</documentation>
        </annotation>
    </element>
    <complexType name="ShearingIncrementPropertyType">
        <annotation>
            <documentation>Property for ConsolidationIncrements</documentation>
        </annotation>
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:ShearingIncrement"/>
        </sequence>
    </complexType>
    <complexType name="ShearingIncrementType">
        <annotation>
            <documentation>Extended type based on AbstractProcedureTrialType. Adds elements for shearing increment type definition.</documentation>
        </annotation>
        <complexContent>
            <extension base="diggs:AbstractProcedureTrialType">
                <sequence>
                    <choice>
                        <sequence>
                            <element minOccurs="1" name="axialLoad" type="diggs:ForceMeasureType">
                                <annotation>
                                    <documentation>Load applied to the top of the
                                        sample</documentation>
                                </annotation>
                            </element>
                            <element minOccurs="1" name="axialDeflection"
                                type="diggs:LengthMeasureType">
                                <annotation>
                                    <documentation>Change in the axial length of the
                                        sample.</documentation>
                                </annotation>
                            </element>
                        </sequence>
                        <sequence>
                            <element minOccurs="1" name="deviatorStress"
                                type="diggs:PressureMeasureType">
                                <annotation>
                                    <documentation>The difference between the major and minor
                                        principal stresses, computed from the axial load divided by
                                        the cross-sectional area of the specimen</documentation>
                                </annotation>
                            </element>
                            <element minOccurs="1" name="axialStrain"
                                type="diggs:LengthPerLengthMeasureType">
                                <annotation>
                                    <documentation>Strain deflection of sample under
                                        load</documentation>
                                </annotation>
                            </element>
                        </sequence>
                    </choice>
                    <element minOccurs="0" name="elapsedTime" type="diggs:TimeMeasureType">
                        <annotation>
                            <documentation>Time since the initiation of consolidation
                                stage</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="totalPoreWaterPressure"
                        type="diggs:PressureMeasureType">
                        <annotation>
                            <documentation>Total Pore water pressure measured</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="inducedPoreWaterPressure"
                        type="diggs:PressureMeasureType">
                        <annotation>
                            <documentation>Pore water pressure induced under loading defined as
                                total pore water pressure minus back pressure</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="totalCellPressure" type="diggs:PressureMeasureType">
                        <annotation>
                            <documentation>The measured confining pressure in the
                                cell.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
