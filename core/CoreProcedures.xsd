                        <annotation>
                            <documentation>Stress rate</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="modeOfFailure" type="gml:CodeType">
                        <annotation>
                            <documentation>Failure mode (eg. shear plane, bulging, etc.), Intended
