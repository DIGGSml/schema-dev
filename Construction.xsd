    <element name="LineLossTest" type="diggs:LineLossTestType"
        substitutionGroup="diggs:AbstractMaterialTestProcedure">
        <annotation>
            <documentation>A test to measure the pressure loss between the grout pump and the grout injection point due to friction in the grout lines.</documentation>
        </annotation>
    </element>
    <complexType name="LineLossTestType">
        <complexContent>
            <extension base="diggs:AbstractMaterialTestlProcedureType">
                <sequence>
                    <element minOccurs="0" name="lineLength" type="eml:LengthMeasure">
                        <annotation>
                            <documentation>The length of the grout line (packer line) from the
                                pressure gauge located at the pump to the pressure gauge at the
                                grout header.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="lineLossCoeffA" type="double">
                        <annotation>
                            <documentation>Coefficient A of the regression equation generated from
                                lineLossData recorded at different flow rates.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="lineLossCoeffB" type="double">
                        <annotation>
                            <documentation>Coefficient B of the regression equation generated from
                                lineLossData recorded at different flow rates.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="lineLossCoeffC" type="double">
                        <annotation>
                            <documentation>Coefficient C of the regression equation generated from
                                lineLossData recorded at different flow rates.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="lineLossR2" type="double">
                        <annotation>
                            <documentation>The correlation coefficient (R^2) for the regression
                                equation generated from lineLossData recorded at different flow
                                rates.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="lineLossData"
                        type="diggs:LineLossDataPropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
