                    <element minOccurs="0" name="volumeWater" type="eml:VolumeMeasureExt">
                        <annotation>
                            <documentation>Volume of water used</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="waterCementRatio"
                        type="eml:ForcePerForceMeasureExt">
                        <annotation>
                            <documentation>Ratio of the weight of water mixed with the cement to
                                weight of cement used for this grout.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="waterCementRatioOfMix"
                        type="eml:ForcePerForceMeasureExt">
                        <annotation>
                            <documentation>Ratio of the weight of total water in the mix to the
                                weight of cement</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="wtCementPerGalH2O" type="eml:ForceMeasureExt">
                        <annotation>
                            <documentation>Weight of cement used per gallon of
                                water.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="binderFactor"
                        type="diggs:DensityOrUnitWeightMeasureType">
                        <annotation>
                            <documentation>Weight of cement devided by the volume of soil in a
                                soil-cement mix.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="binderFactorInPlace"
                        type="diggs:DensityOrUnitWeightMeasureType">
                        <annotation>
                            <documentation>Weight of cement devided by the volume of soil-cement
                                mixture, which is equal to the sum of the volumes of the cement,
                                soil, and water.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="binderContent" type="eml:ForcePerForceMeasureExt">
                        <annotation>
                            <documentation>Weight of cement used per gallon of
                                water.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="volumeRatio" type="eml:VolumePerVolumeMeasureExt">
                        <annotation>
                            <documentation>Ratio of the volume of cenent plus volume of the water in
                                the cement slurry, to the volume of soil in a cement-soil
                                mix</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="GroutingProgram" substitutionGroup="diggs:AbstractProgram"
        type="diggs:GroutingProgramType"/>
    <complexType name="GroutingProgramType">
        <complexContent>
            <extension base="diggs:AbstractProgramType"> </extension>
        </complexContent>
    </complexType>
    <element name="groutRef" type="diggs:FeatureReferenceType">
        <annotation>
            <appinfo source="urn:x-gml:targetElement">Diggs:Grout</appinfo>
            <documentation>A reference to a Grout feature that is associated with this feature. </documentation>
            <documentation>A property element that supports a value by reference only. The target
                value is indicated in the appinfo element.</documentation>
        </annotation>
    </element>
    <element name="GroutSpecimen" substitutionGroup="diggs:AbstractSpecimen"
        type="diggs:GroutSpecimenType">
        <annotation>
            <documentation>A specialization of a specimen that describes the physical conditions of a grout or concrete sample subjected to testing.</documentation>
        </annotation>
    </element>
    <complexType name="GroutSpecimenType">
        <complexContent>
            <extension base="diggs:AbstractSpecimenType">
                <sequence>
                    <element minOccurs="0" name="specimenCondition"
                        type="diggs:GroutSpecimenConditionsPropertyType">
                        <annotation>
                            <documentation>Describes the physical conditions of the sample
                                pre-test</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="componentRef"
                        type="gml:ReferenceType">
                        <annotation>
                            <appinfo>diggs:Admixture or diggs:Binder or diggs:Cement or
                                diggs:Chemical or diggs:Filler</appinfo>
                            <documentation>Pointer to the id of the specific grout batch component
                                tested. More than one reference is allowed where more than one
                                component of a specific type is included together in the specimen
                                tested. In this case each componentRef will point to the components
                                used.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="componentType">
                        <annotation>
                            <documentation>If test is performed on one type of grout component (as
                                opposed to the grout mix itself, this property identifies the
                                specific grout component type tested. From an emumerated list: 1)
                                Admixture 2) Binder 3) Cement 4) Chemical 5) Filler See GroutBatch
                                object for definition of these components</documentation>
                        </annotation>
                        <simpleType>
                            <restriction base="string">
                                <enumeration value="Admixture"/>
                                <enumeration value="Binder"/>
                                <enumeration value="Cement"/>
                                <enumeration value="Chemical"/>
                                <enumeration value="Filler"/>
                            </restriction>
                        </simpleType>
                    </element>
                    <element minOccurs="0" name="formMaterial" type="gml:CodeType">
                        <annotation>
                            <documentation>Mold material. Used when forming sample for compressive
                                testing. Intended to come from a controlled list of
                                terms.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="curingTime" type="eml:TimeMeasure">
                        <annotation>
                            <documentation>The time the specimen is cured prior to
                                testing.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="groutSpecimenExtractionLocation"
                        type="gml:CodeType">
                        <annotation>
                            <documentation>Describes location from where the grout specimen is
                                pulled from the grout batch. Intended to come from a controlled list
                                of terms.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="specificGravityOfSolids"
                        type="diggs:SpecificGravityType">
                        <annotation>
                            <documentation>Specific gravity of grout solids (Gs) is the ratio of the
                                density of a substance to the density of a reference substance,
                                usually water at 4 deg C; equivalently, it is the ratio of the mass
                                of a substance to the mass of a reference substance for the same
                                given volume.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="GroutStage" substitutionGroup="diggs:AbstractObject" type="diggs:GroutStageType">
        <annotation>
            <documentation>An object that carries properties for a grout stage</documentation>
        </annotation>
    </element>
    <complexType name="GroutStagePropertyType">
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:GroutStage"/>
        </sequence>
    </complexType>
    <complexType name="GroutStageType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element maxOccurs="unbounded" minOccurs="0" name="equipment"
                        type="diggs:EquipmentPropertyType">
                        <annotation>
                            <documentation>For equipment specific to this stage, equipment objects
                                or references can be inserted here. Otherwise, use the equipment
                                property in the BoreholeGroutingActivity feature to identiy
                                activity-wide equipment</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" ref="diggs:groutRef">
                        <annotation>
                            <appinfo>diggs:GroutBatch</appinfo>
                            <documentation>This property references the grout sample(s) used for
                                this stage</documentation>
                        </annotation>
                    </element>
                    <element name="stageType" minOccurs="0">
                        <annotation>
                            <documentation>Restricted string; either ascending or descending are
                                allowed.</documentation>
                        </annotation>
                        <simpleType>
                            <restriction base="string">
                                <enumeration value="ascending"/>
                                <enumeration value="descending"/>
                            </restriction>
                        </simpleType>
                    </element>
                    <element minOccurs="0" name="stageInterval"
                        type="diggs:LinearExtentPropertyType">
                        <annotation>
                            <documentation>The depth interval for the stage (measured along the
                                centerline of the borehole), where the grout material was emplaced.
                            </documentation>
                        </annotation>
                    </element>
                    <element name="gaugeHeight" type="eml:LengthMeasure" minOccurs="0">
                        <annotation>
                            <documentation>Height of gauge (H0).</documentation>
                        </annotation>
                    </element>
                    <element name="maxInjectionPressure" minOccurs="0"
                        type="diggs:GroutPressureMeasureType">
                        <annotation>
                            <documentation>Maximum injection pressure during stage
                                filling.</documentation>
                        </annotation>
                    </element>
                    <element name="minInjectionPressure" minOccurs="0"
                        type="diggs:GroutPressureMeasureType">
                        <annotation>
                            <documentation>Minimum injection pressure during stage filling.
                            </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="startTimeFilling"
                        type="gml:TimeInstantPropertyType">
                        <annotation>
                            <documentation>The time instant when filling starts.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="startTimePressurizing"
                        type="gml:TimeInstantPropertyType">
                        <annotation>
                            <documentation>The time instant when pressurizing
                                starts.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="endTime" type="gml:TimeInstantPropertyType">
                        <annotation>
                            <documentation>The time when grouting of interval ends.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="deliveryType" type="gml:CodeType">
                        <annotation>
                            <documentation>The device or method by which the grout is delivered -
                                eg. manchette, tube, sleeve, etc. Intended to come from a controlled
                                list. </documentation>
                        </annotation>
                    </element>
                    <element name="holeVolume" type="eml:VolumeMeasure" minOccurs="0">
                        <annotation>
                            <documentation>Volume of hole within stage interval</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="hydrostaticColumnPressure"
                        type="diggs:pressureMeasureType">
                        <annotation>
                            <documentation>Hydrostatic column pressure determined for the
                                stage.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" ref="diggs:injectionTerminatedBy" maxOccurs="unbounded"/>
                    <element name="injectionTimeSeries" type="diggs:InjectionTimeSeriesPropertyType"
                        minOccurs="0" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Property that contains the measured and calculated values
                                collected during grout injection in this stage. A grout stage can
                                have multiple injection time series</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="lancePlacementMethod">
                        <annotation>
                            <documentation>Method of lance emplacement for this stage - either
                                driven or wash bored, from an enumerated list.</documentation>
                        </annotation>
                        <simpleType>
                            <restriction base="string">
                                <enumeration value="Driven">
                                    <annotation>
                                        <documentation>Lance is driven into the ground by hydraulic
                                            pressure or hammer</documentation>
                                    </annotation>
                                </enumeration>
                                <enumeration value="Wash bored">
                                    <annotation>
                                        <documentation>Lance is emplaced behind a high-pressure
                                            water jet</documentation>
                                    </annotation>
                                </enumeration>
                            </restriction>
                        </simpleType>
                    </element>
                    <element minOccurs="0" name="strokeCount" type="positiveInteger">
                        <annotation>
                            <documentation>Where a piston grout pump is used, total number of pump
                                strokes for the stage.</documentation>
                        </annotation>
                    </element>
                    <element name="totalCement" type="eml:ForceMeasure" minOccurs="0">
                        <annotation>
                            <documentation>Weoght of grout cement used to fill
                                stage.</documentation>
                        </annotation>
                    </element>
                    <element name="totalGroutTake" type="eml:VolumeMeasure" minOccurs="0">
                        <annotation>
                            <documentation>Volume of grout used to fill stage.</documentation>
                        </annotation>
                    </element>
                    <element name="volumeInjectionPerLength" type="eml:VolumePerLengthMeasure"
                        minOccurs="0">
                        <annotation>
                            <documentation>Volume of grout injection per unit length of stage
                                interval filled.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="waterLugeonValue">
                        <complexType>
                            <simpleContent>
                                <extension base="eml:AbstractMeasure">
                                    <attribute fixed="Lu" name="uom" use="required"/>
                                </extension>
                            </simpleContent>
                        </complexType>
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="groutTemperature"
                        type="diggs:TemperatureMeasurementtPropertyType">
                        <annotation>
                            <documentation>Temperature of the grout. This could be a single
                                measurment taken prior to or during the grout stage or an average of
                                temperatures measured during the grouting event. Discrete
                                temperature measurements taken during the grouting should be
                                recorded within the injection time series object.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="maxHeaderPressureAtZeroFlow"
                        type="eml:PressureMeasure">
                        <annotation>
                            <documentation>The pressure measured at the header at zero grout
                                flow.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="staticLineResistanceAtZeroFlow"
                        type="eml:PressureMeasure">
                        <annotation>
                            <documentation>The static line resistance to grout flow during zero flow
                                conditions.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="ContinuousGrouting" substitutionGroup="diggs:AbstractObject"
        type="diggs:ContinuousGroutingType">
        <annotation>
            <documentation>An object that carries properties for continuous grouting performed
                during jet grouting or deep soil mixing activities</documentation>
        </annotation>
    </element>
    <complexType name="ContinuousGroutingPropertyType">
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:ContinuousGrouting"/>
        </sequence>
    </complexType>
    <complexType name="ContinuousGroutingType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <choice>
                        <element maxOccurs="1" minOccurs="0" ref="diggs:groutRef">
                            <annotation>
                                <appinfo>diggs:GroutBatch</appinfo>
                                <documentation>This property references the grout sample(s) used for
                                    this stage</documentation>
                            </annotation>
                        </element>
                        <sequence>
                            <element maxOccurs="1" minOccurs="0" name="penetrationGroutRef"
                                type="diggs:FeatureReferenceType">
                                <annotation>
                                    <appinfo>diggs:GroutBatch</appinfo>
                                    <documentation>This property references the grout sample(s) used
                                        for this stage</documentation>
                                </annotation>
                            </element>
                            <element maxOccurs="1" minOccurs="0" name="withdrawalGroutRef"
                                type="diggs:FeatureReferenceType">
                                <annotation>
                                    <appinfo>diggs:GroutBatch</appinfo>
                                    <documentation>This property references the grout sample(s) used
                                        for this stage</documentation>
                                </annotation>
                            </element>
                        </sequence>
                    </choice>
                    <element maxOccurs="1" minOccurs="0" name="fluid_2"
                        type="diggs:GroutingFluidPropertyType">
                        <annotation>
                            <appinfo/>
                            <documentation>This property holds or references a GroutingFluid object
                                describing the fluid used during jet grouting</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="fluid_3"
                        type="diggs:GroutingFluidPropertyType">
                        <annotation>
                            <appinfo/>
                            <documentation>This property holds or references a GroutingFluid object
                                describing the fluid used during jet grouting</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="groutInterval"
                        type="diggs:LinearExtentPropertyType">
                        <annotation>
                            <documentation>The depth interval for the stage (measured along the
                                centerline of the borehole), where the grout material was emplaced.
                            </documentation>
                        </annotation>
                    </element>
                    <element name="gaugeHeight" type="eml:LengthMeasure" minOccurs="0">
                        <annotation>
                            <documentation>Height of gauge (H0).</documentation>
                        </annotation>
                    </element>
                    <element name="maxInjectionPressureGrout" minOccurs="0"
                        type="diggs:GroutPressureMeasureType">
                        <annotation>
                            <documentation>Maximum injection pressure of the grout during
                                grouting.</documentation>
                        </annotation>
                    </element>
                    <element name="minInjectionPressureGrout" minOccurs="0"
                        type="diggs:GroutPressureMeasureType">
                        <annotation>
                            <documentation>Minimum injection pressure of the grout during grouting.
                            </documentation>
                        </annotation>
                    </element>
                    <element name="maxInjectionPressureFluid_2" minOccurs="0"
                        type="diggs:GroutPressureMeasureType">
                        <annotation>
                            <documentation>Maximum injection pressure of fluid 1 during
                                grouting.</documentation>
                        </annotation>
                    </element>
                    <element name="minInjectionPressureFluid_2" minOccurs="0"
                        type="diggs:GroutPressureMeasureType">
                        <annotation>
                            <documentation>Minimum injection pressure of fluid 1 during
                                grouting.</documentation>
                        </annotation>
                    </element>
                    <element name="maxInjectionPressureFluid_3" minOccurs="0"
                        type="diggs:GroutPressureMeasureType">
                        <annotation>
                            <documentation>Maximum injection pressure of fluid 2 during
                                grouting.</documentation>
                        </annotation>
                    </element>
                    <element name="minInjectionPressureFluid_3" minOccurs="0"
                        type="diggs:GroutPressureMeasureType">
                        <annotation>
                            <documentation>Minimum injection pressure of fluid 2 during
                                grouting.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="startTimeGrouting"
                        type="gml:TimeInstantPropertyType">
                        <annotation>
                            <documentation>The time instant when filling starts.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="endTime" type="gml:TimeInstantPropertyType">
                        <annotation>
                            <documentation>The time when grouting of interval ends.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="mixingType" type="diggs:MixingTypeEnumType">
                        <annotation>
                            <documentation>The type of fluid/grout mix used, from an enumerated list
                                as follows: 1) dry mixing 2) slurry mixing 3) single fluid jet 4)
                                double fluid jet 5) triple fluid jet 6) Other:any
                                string</documentation>
                        </annotation>
                    </element>
                    <element name="groutingTimeSeries" type="diggs:GroutingTimeSeriesPropertyType"
                        minOccurs="0" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Property that contains the measured and calculated values
                                collected during grout injection. Multiple injection time series are
                                allowed to delineate between drilling and grouting, or to allow for
                                stopping of the grouting between injection
                                intervals.</documentation>
                        </annotation>
                    </element>
                    <element name="totalGroutTake" type="eml:VolumeMeasure" minOccurs="0">
                        <annotation>
                            <documentation>Volume of grout used.</documentation>
                        </annotation>
                    </element>
                    <element name="totalFluid_2Take" type="eml:VolumeMeasure" minOccurs="0">
                        <annotation>
                            <documentation>Volume of fluid 1 used.</documentation>
                        </annotation>
                    </element>
                    <element name="totalFluid_3Take" type="eml:VolumeMeasure" minOccurs="0">
                        <annotation>
                            <documentation>Volume of fluid 2 used.</documentation>
                        </annotation>
                    </element>
                    <choice>
                        <element name="groutTemperature"
                            type="eml:ThermodynamicTemperatureMeasureExt" minOccurs="0">
                            <annotation>
                                <documentation>Temperature of the grout. This could be a single
                                    measurment taken prior to or during grouting or an average of
                                    temperatures measured during the grouting event. Discrete
                                    temperature measurements taken during the grouting should be
                                    recorded within the grouting time series object.</documentation>
                            </annotation>
                        </element>
                        <sequence>
                            <element maxOccurs="1" minOccurs="0" name="penetrationGroutTemperature"
                                type="eml:ThermodynamicTemperatureMeasureExt">
                                <annotation>
                                    <appinfo>diggs:GroutBatch</appinfo>
                                    <documentation>This property references the grout sample(s) used
                                        for this stage</documentation>
                                </annotation>
                            </element>
                            <element maxOccurs="1" minOccurs="0" name="withdrawalGroutTemperature"
                                type="eml:ThermodynamicTemperatureMeasureExt">
                                <annotation>
                                    <appinfo>diggs:GroutBatch</appinfo>
                                    <documentation>This property references the grout sample(s) used
                                        for this stage</documentation>
                                </annotation>
                            </element>
                        </sequence>
                    </choice>
                    <element name="fluid_2Temperature" type="eml:ThermodynamicTemperatureMeasure"
                        minOccurs="0">
                        <annotation>
                            <documentation>Temperature of fluid 1. This could be a single measurment
                                taken prior to or during grouting or an average of temperatures
                                measured during the grouting event. Discrete temperature
                                measurements taken during the grouting should be recorded within the
                                grouting time series object.</documentation>
                        </annotation>
                    </element>
                    <element name="Fluid_3Temperature" type="eml:ThermodynamicTemperatureMeasure"
                        minOccurs="0">
                        <annotation>
                            <documentation>Temperature of fluid 2. This could be a single measurment
                                taken prior to or during grouting or an average of temperatures
                                measured during the grouting event. Discrete temperature
                                measurements taken during the grouting should be recorded within the
                                grouting time series object.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="GroutingFluidPropertyType">
        <sequence minOccurs="0">
            <element maxOccurs="1" minOccurs="1" ref="diggs:GroutingFluid"/>
        </sequence>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element name="GroutingFluid" substitutionGroup="diggs:AbstractObject"
        type="diggs:GroutingFluidType">
        <annotation>
            <documentation>An object that contains properites of a fluid that is injected along with
                grout, and possibly another fluid, during jet grouting or deep soil
                miing.</documentation>
        </annotation>
    </element>
    <complexType name="GroutingFluidType">
        <complexContent>
            <extension base="diggs:AbstractObjectType"> </extension>
        </complexContent>
    </complexType>
    <element name="FluidAdditive" substitutionGroup="diggs:AbstractObject"
        type="diggs:FluidAdditiveType">
        <annotation>
            <documentation>An object that carries properties for continuous grouting performed
                during jet grouting or deep soil mixing activities</documentation>
        </annotation>
    </element>
    <complexType name="FluidAdditiveType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element name="additiveType" type="gml:CodeType">
                        <annotation>
                            <documentation>A term, intended to come from a controlled list,
                                identifying the type of material added to the fluid.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="additionEvent"
                        type="diggs:AdditionEventPropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="FluidAdditivePropertyType">
        <sequence>
            <element ref="diggs:FluidAdditive">
                <annotation>
                    <documentation/>
                </annotation>
            </element>
        </sequence>
    </complexType>
    <complexType name="AdditionEventPropertyType">
        <sequence>
            <element ref="diggs:AdditionEvent">
                <annotation>
                    <documentation>An object that provides the history of addiltion of an additive
                        to a fluid.</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>
    <element name="AdditionEvent" type="diggs:AdditionEventType">
        <annotation>
            <documentation>The time and the amount of additive added to the fluid stream during jet
                grouting or deep soil mixing.</documentation>
        </annotation>
    </element>
    <complexType name="AdditionEventType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element minOccurs="0" name="time" type="diggs:TimeIntervalPropertyType">
                        <annotation>
                            <documentation>Date/time when this event information was
                                recorded</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="weightAdded" type="eml:ForceMeasureExt">
                        <annotation>
                            <documentation>Weight of additive added at this event
                                time.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="volumeAdded" type="eml:VolumeMeasureExt">
                        <annotation>
                            <documentation>Volume of additive added at this event
                                time.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="false" name="GroutingTimeSeries" substitutionGroup="gml:AbstractCoverage"
        type="diggs:GroutingTimeSeriesType">
        <annotation>
            <documentation>An object containing time series data recorded during drilling and
                grouting of the hole while jet grouting or deep soil mixing.</documentation>
        </annotation>
    </element>
    <complexType name="GroutingTimeSeriesPropertyType">
        <annotation>
            <documentation>Property type holding a grout stage time series</documentation>
        </annotation>
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:GroutingTimeSeries"/>
        </sequence>
    </complexType>
    <complexType name="GroutingTimeSeriesBaseType">
        <complexContent>
            <restriction base="gml:AbstractCoverageType">
                <sequence>
                    <element ref="diggs:timeDomain"/>
                    <element ref="diggs:groutingDataResults"/>
                </sequence>
            </restriction>
        </complexContent>
    </complexType>
    <complexType name="GroutingTimeSeriesType">
        <complexContent>
            <extension base="diggs:GroutingTimeSeriesBaseType">
                <sequence>
                    <element minOccurs="0" name="initiationTime" type="gml:TimePositionType">
                        <annotation>
                            <documentation>Time instant that injection begins.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="endTime" type="gml:TimePositionType">
                        <annotation>
                            <documentation>Time instant that injection ends.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="totalElapsedTime" type="eml:TimeMeasure">
                        <annotation>
                            <documentation>Total time between injection initiation and injection
