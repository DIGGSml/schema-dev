    <element name="ParticleSize" type="diggs:ParticleSizeType">
        <annotation>
            <documentation>An object that contains properties of a particle size
                measurement.</documentation>
        </annotation>
    </element>

    <element name="ParticleSizeDistribution" type="diggs:ParticleSizeDistributionType"/>

    <complexType name="ParticleSizeDistributionPropertyType">
        <sequence>
            <element ref="diggs:ParticleSizeDistribution">
                <annotation>
                    <documentation>A object that provides summary particle size distribution
                        information (eg. modal grain size, median grain size, d10, d60) etc) for a
                        lithology.</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <complexType name="ParticleSizeDistributionType">
        <complexContent>
            <extension base="diggs:AbstractComponentObjectBaseType">
                <sequence>
                    <element minOccurs="0" name="meanGrainsize"
                        type="diggs:ParticleSizePropertyType">
                        <annotation>
                            <documentation>A text descriptor (eg. fine sand) or numeric value
                                representing the mean grain size.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="modalGrainsize"
                        type="diggs:ParticleSizePropertyType">
                        <annotation>
                            <documentation>A text descriptor (eg. fine sand) or numeric value
                                representing the modal grain size.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="equivalentDiameter"
                        type="diggs:ParticleSizePropertyType">
                        <annotation>
                            <documentation>A text descriptor (eg. fine sand) or numeric value
                                representing the equivalent diameter for the soil.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="minimumDiameter"
                        type="diggs:ParticleSizePropertyType">
                        <annotation>
                            <documentation>A text descriptor (eg. fine sand) or numeric value
                                representing the minimum diameter of particles in the
                                soil.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="maximumDiameter"
                        type="diggs:ParticleSizePropertyType">
                        <annotation>
                            <documentation>A text descriptor (eg. fine sand) or numeric value
                                representing the maximum diameter of particles in the
                                soil.</documentation>
                        </annotation>
                    </element>
                </sequence>
                <attribute name="howDetermined" type="diggs:DescriptorMethodEnumType">
                    <annotation>
                        <documentation>A value that describes the method by which the observation or measurement
                            was determined. </documentation>
                    </annotation>
                </attribute>
            </extension>
        </complexContent>
    </complexType>

    <complexType name="ParticleSizePropertyType">
        <sequence>
            <element ref="diggs:ParticleSize"/>
        </sequence>
    </complexType>

    <complexType name="ParticleSizeType">
        <complexContent>
            <extension base="diggs:ObjectNoDescriptionType">
                <choice>
                    <annotation>
                        <documentation>Either particleSizeValue or particalSizeDescription should be
                            present. Both are allowed.</documentation>
                    </annotation>
                    <sequence>
                        <element name="particleSizeValue" type="diggs:ParticleSizeValueType"
                            minOccurs="1">
                            <annotation>
                                <documentation>A numeric value for the particle size, in length
                                    units such as mm diameter, or phi.</documentation>
                            </annotation>
                        </element>
                        <element name="particleSizeDescription" type="gml:CodeType" minOccurs="0">
                            <annotation>
                                <documentation>A text description for the particle size, such as
                                    "very fine sand", etc.</documentation>
                            </annotation>
                        </element>
                    </sequence>
                    <sequence>
                        <element name="particleSizeDescription" type="gml:CodeType" minOccurs="1">
                            <annotation>
                                <documentation>A text description for the particle size, such as
                                    "very fine sand", etc.</documentation>
                            </annotation>
                        </element>
                    </sequence>
                </choice>
            </extension>
        </complexContent>
    </complexType>

    <complexType name="SizeClassCodeType">
        <simpleContent>
            <extension base="gml:CodeType">
                <attribute name="sizeClass" type="diggs:SizeClassEnumExtType"/>
            </extension>
        </simpleContent>
    </complexType>

    <simpleType name="SizeClassEnumType">
        <annotation>
            <documentation>Definition of size classes: Grain size classification system should be
                identified in the codeSpace attribute, example size ranges for the size classes are:
                sand - for ASTM, particles between 0,075 and 4.75 mm; for Wentworth, particles
                between 0.0625 and 2 mm. gravel - for ASTM, particles between 4.75 and 75 mm; for
                Wentworth, particles above 2 mm granule - for Wentworth, particles between 2 and 4
                mm pebble - for Wentworth, particles between 4 and 64 mm cobble - for ASTM,
                particles between 75 and 300 mm; for Wentworth, particles between 64 and 256 mm
                boulder - for ASTM, particles above 300 mm; for boulder, particles above 256
                mm</documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="xlay"/>
            <enumeration value="silt"/>
            <enumeration value="sand"/>
            <enumeration value="gravel"/>
            <enumeration value="granule"/>
            <enumeration value="pebble"/>
            <enumeration value="cobble"/>
            <enumeration value="boulder"/>
        </restriction>
    </simpleType>

    <simpleType name="SizeClassEnumExtType">
        <union memberTypes="diggs:SizeClassEnumType diggs:OtherNameType"/>
    </simpleType>


    <element name="StratigraphyObservation" type="diggs:StratigraphyObservationType">
        <annotation>
            <documentation>Stratigraphy obsrvations are descriptions of ordered bodies of rock or
                soil, such as formations, biostratigraphic units or aquifers. </documentation>
        </annotation>
    </element>

    <complexType name="StratigraphyObservationPropertyType">
        <sequence maxOccurs="1">
            <element maxOccurs="1" ref="diggs:StratigraphyObservation"/>
        </sequence>
    </complexType>

    <complexType name="StratigraphyObservationType">
        <complexContent>
            <extension base="diggs:AbstractObservationType">
                <sequence>
                    <element name="stratigraphicUnitName" type="gml:CodeType" maxOccurs="unbounded">
                        <annotation>
                            <documentation>The name of the stratigraphic unit (eg. Harold Formation)
                                assigned to this zone. Should come from a controlled list based on
                                the subtype of the layer system.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="stratigraphicUnitDescription"
                        type="diggs:DiggsStringType">
                        <annotation>
                            <documentation>An optional description of the named stratigraphic
                                unit</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="unitType" type="diggs:StratUnitExtEnumType"/>
                    <element maxOccurs="unbounded" minOccurs="0" ref="diggs:placedObservation"/>
                    <element minOccurs="0" ref="diggs:baseBoundary">
                        <annotation>
                            <documentation>Description of the boundary at the base of the observed
                                zone or interval as defined in the observation's location
                                property</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <element name="StratigraphySystem" substitutionGroup="diggs:AbstractObservationSystem"
        type="diggs:StratigraphySystemType">
        <annotation>
            <documentation>An object containing a system of stratrigraphy observations. Observations
                within a stratigraphy system must not overlap, and stratUnit values within a system
                cannot repeat (eg. they must be unique within the system. </documentation>
        </annotation>
    </element>

    <complexType name="StratigraphySystemType">
        <complexContent>
            <extension base="diggs:AbstractObservationSystemType">
                <sequence>
                    <element name="stratigraphyType" type="diggs:StratUnitExtEnumType">
                        <annotation>
                            <documentation>Selected from an enumerated list, the type of
                                stratigraphic units described in this system. Where there may be
                                different strat unit definitions within a system (rare), the
                                dominant type should be listed here and the unitType property of
                                each StratigraphyObservation may be instantiated to identify which
                                observation falls within each type of unit.</documentation>
                        </annotation>
                    </element>
                    <element name="stratigraphyObservation"
                        type="diggs:StratigraphyObservationPropertyType" maxOccurs="unbounded"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <complexType name="ObservationSystemPropertyType">
        <sequence>
            <element ref="diggs:AbstractObservationSystem"/>
        </sequence>
    </complexType>

    <simpleType name="StratUnitEnumType">
        <annotation>
            <documentation>Definitions of the following unit types: 1) allostratigraphic - a body of
                sedimentary rock that is defined and identified only by its bounding discontinuities
                2) biostratigraphic - bodies of rock strata that are defined or characterized on the
                basis of their contained fossils. 3) chemostratigraphic - a body of rock defined on
                the chemical composition of the sediments, rocks, and rock components. 4)
                chronostratigraphic - bodies of rocks, layered or unlayered, that are defined
                between specified stratigraphic horizons which represent specified intervals of
                geologic time. 5) geophysical - bodies of rocks and soils that are distinguished by
                their geophysical properties. The criteria for distinguishing such units may vary
                depending on the engineering application. 6) hydrostratigraphic - a part of a body
                of rock that forms a distinct hydrologic unit with respect to the flow of ground
                water, generally characterized by its porosity and permeability. 7) lithodemic - a
                three-dimensional body composed of one or more intrusive, highly deformed or highly
                metamorphosed rock types, distinguished and delimited on the basis of rock
                characteristics. Its contacts with other rock units may be sedimentary, extrusive,
                intrusive, and tectonic or relate to metamorphic grade. 8) lithostratigraphy -
                bodies of rocks, bedded or unbedded, that are defined and characterized on the basis
                of their lithologic properties and their stratigraphic relations. 9)
                magnetostratigraphic - rock strata unified by similar magnetic characteristics (not
                only magnetic polarity) which allows it to be differentiated from adjacent strata.
                10) pedostratigraphic - units defined based on the stratigraphical and spatial
                relationships and implications of surface and buried soils. It has emerged as a key
                means of interpreting palaeosols; records of metastability in landscapes when rates
                of erosion and accumulation are slow. 11) polarity chronostratigraphic - units
                defined by primary magnetic-polarity record imposed when the rock was deposited or
                crystallized during a specific interval of geologic time. 12) tectonostratigraphic -
                a body characterized by a consistent tectonostratigraphy and metamorphic evolution,
                and bounded by tectonic discontinuities.</documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="allostratigraphic"/>
            <enumeration value="biostratigraphic"/>
            <enumeration value="chemostraitgraphic"/>
            <enumeration value="chronostratigraphic"/>
            <enumeration value="geophysical"/>
            <enumeration value="hydrostraptigraphic"/>
            <enumeration value="lithodemic"/>
            <enumeration value="lithostratigraphic"/>
            <enumeration value="magnetostratigraphic"/>
            <enumeration value="pedostratigraphic"/>
            <enumeration value="polarity chronostratigraphic"/>
            <enumeration value="tectonostratigraphic"/>
        </restriction>
    </simpleType>

    <simpleType name="StratUnitExtEnumType">
        <union memberTypes="diggs:StratUnitEnumType diggs:OtherNameType"/>
    </simpleType>

    <!-- ================================================================ -->
    <!-- MEASUREMENT ELEMENTS                                             -->
    <!-- ================================================================ -->

    <element name="MaterialTest" substitutionGroup="diggs:AbstractMeasurement"
        type="diggs:MaterialTestType"/>

    <element name="MaterialTestResult" type="diggs:MaterialTestResultType"/>

    <complexType name="MaterialTestResultType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element minOccurs="1" ref="diggs:results"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <complexType name="MaterialTestResultPropertyType">
        <sequence>
            <element minOccurs="1" ref="diggs:MaterialTestResult"/>
        </sequence>
    </complexType>

    <complexType name="MaterialTestType">
        <complexContent>
            <extension base="diggs:AbstractMeasurementType">
                <sequence>
                    <element minOccurs="0" name="samplingTime"
                        type="diggs:TimeIntervalOrInstantPropertyType">
                        <annotation>
                            <documentation>Describes the time that the result applies to the
                                property of the investigation target. This is often the time of
                                interaction by a sampling activity or measurement procedure with a
                                real-world feature. </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="resultTime"
                        type="diggs:TimeIntervalOrInstantPropertyType">
                        <annotation>
                            <documentation>Describes the time when the result became available,
                                typically when the procedure associated with the observation was
                                completed. For some observations this is identical to the
                                samplingTime. However, there are important cases where they differ.
                                Example: Where a measurement is made on a specimen in a
                                laboratory,the samplingTime should record the time the specimen was
                                retrieved from its host, while the resultTime should record the time
                                the laboratory procedure was applied. Example: Where monitoring
                                observation results are post-processed, the resultTime is the
                                post-processing time, while the samplingTime preserves the time of
                                initial interaction with the world. </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="validTime"
                        type="diggs:TimeIntervalOrInstantPropertyType">
                        <annotation>
                            <documentation>Describes the time period during which the result is
                                intended to be used.</documentation>
                        </annotation>
                    </element>
                    <element name="outcome" type="diggs:MaterialTestResultPropertyType">
                        <annotation>
                            <documentation>A property that contains the information about what
                                properties are being measured, the results of the measurement, and
                                the associated locations that the measurement results relate
                                to.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="procedure"
                        type="diggs:ProcedurePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>


    <complexType name="MeasurementWhileDrillingType">
        <complexContent>
            <extension base="diggs:AbstractMeasurementType">
                <sequence>
                    <element name="outcome" type="diggs:MWDResultPropertyType"/>
                    <element minOccurs="0" name="procedure" type="diggs:MWDProcedurePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <element name="MeasurementWhileDrilling" substitutionGroup="diggs:AbstractMeasurement"
        type="diggs:MeasurementWhileDrillingType"/>

    <element name="MWDResult" type="diggs:MWDResultType"/>

    <complexType name="MWDResultType">
        <complexContent>
            <restriction base="gml:AbstractCoverageType">
                <sequence>
                    <choice>
                        <element ref="diggs:location"/>
                        <element ref="diggs:timeDomain"/>
                    </choice>
                    <element ref="diggs:results"/>
                </sequence>
            </restriction>
        </complexContent>
    </complexType>

    <complexType name="MWDResultPropertyType">
        <sequence>
            <element ref="diggs:MWDResult"/>
        </sequence>
    </complexType>


    <element name="Monitor" type="diggs:MonitorType" substitutionGroup="diggs:AbstractMeasurement">
        <annotation>
            <documentation>An act ("event"), whose results are estimates of the value of a property
                of the investigation target, measured at a time instant or within a time interval,
                and at a location or series of locations, derived from a test
                procedure.</documentation>
        </annotation>
    </element>

    <element name="MonitorResult" type="diggs:MonitorResultType"
        substitutionGroup="gml:AbstractCoverage">
        <annotation>
            <documentation>An object that contains properties of a monitoring activity
                result.</documentation>
        </annotation>
    </element>

    <complexType name="MonitorResultPropertyType">
        <sequence>
            <element ref="diggs:TemporalResult"/>
        </sequence>
    </complexType>

    <complexType name="MonitorResultType">
        <complexContent>
            <restriction base="gml:AbstractCoverageType">
                <sequence>
                    <element ref="diggs:timeDomain"/>
                    <element ref="diggs:results"/>
                </sequence>
            </restriction>
        </complexContent>
    </complexType>

    <complexType name="MonitorType">
        <complexContent>
            <extension base="diggs:AbstractMeasurementType">
                <sequence>
                    <element minOccurs="0" name="samplingTime"
                        type="diggs:TimeIntervalOrInstantPropertyType">
                        <annotation>
                            <documentation>Describes the time interval that the monitor result applies to. This is an optional property but should be used where the timeIntervalList is used in the timeDomain property of the Monitor outcome</documentation>
                        </annotation>
                    </element>
                    <element name="monitorLocation" type="diggs:LocationPropertyType">
                        <annotation>
                            <documentation>Identifies the location associated with the monitoring measurement.This may be the detector or sensor location, or the response zone (such as a well screen interval) over which the measurement applies. In the case where the location is a gemetry aggregate, the monitor results will refer to all locations as a whole. In cases where a sensor has detectors in multiple locations, eqch of which is monitoring properties at the detector location, the monitor results for each detector should be contained within a unique, separate Monitor object.</documentation>
                        </annotation>
                    </element>
                    <element name="outcome" type="diggs:MonitorResultPropertyType">
                        <annotation>
                            <documentation>A property that contains the information about what
                                properties are being measured, the results of the measurement, and
                                the associated locations that the measurement results relate
                                to.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="procedure" type="diggs:ProcedurePropertyType"
                        maxOccurs="unbounded"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <element name="TemporalResult" substitutionGroup="gml:AbstractCoverage"
        type="diggs:TemporalResultType">
        <annotation>
            <documentation>An object that contains properties and results of a monitoring activity
                result.</documentation>
        </annotation>
    </element>

    <complexType name="TemporalResultType">
        <complexContent>
            <restriction base="gml:AbstractCoverageType">
                <sequence>
                    <element ref="diggs:timeDomain"/>
                    <element ref="diggs:results"/>
                </sequence>
            </restriction>
        </complexContent>
    </complexType>

    <complexType name="MeasurementPropertyType">
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:AbstractMeasurement"/>
        </sequence>
    </complexType>


    <element name="Test" type="diggs:TestType" substitutionGroup="diggs:AbstractMeasurement">
        <annotation>
            <documentation>An act ("event"), whose results are estimates of the value of a property
                of the investigation target, measured at a time instant or within a time interval,
                and within a spatial domain, typically derived from a test
                procedure.</documentation>
        </annotation>
    </element>

    <element name="TestResult" substitutionGroup="gml:AbstractCoverage" type="diggs:TestResultType">
        <annotation>
            <documentation>An object that contains properties that report the results of a
                test.</documentation>
        </annotation>
    </element>

    <complexType name="TestResultPropertyType">
        <sequence>
            <element ref="diggs:TestResult"/>
        </sequence>
    </complexType>

    <complexType name="TestResultBaseType">
        <complexContent>
            <restriction base="gml:AbstractCoverageType">
                <sequence>
                    <element ref="diggs:location"/>
                    <element ref="diggs:results"/>
                </sequence>
            </restriction>
        </complexContent>
    </complexType>

    <complexType name="TestResultType">
        <complexContent>
            <extension base="diggs:TestResultBaseType">
                <sequence>
                    <element minOccurs="0" ref="diggs:gridMappingFunction"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <element name="gridMappingFunction" substitutionGroup="gml:AbstractObject"
        type="gml:CoverageFunctionType">
        <annotation>
            <documentation>The gridMappingFunction property describes the mapping function from the
                domain to the range for gridded locations. If omitted, the gml:startPoint is assumed
                to be the value of the gml:low property in the gml:Grid geometry, and the
                gml:sequenceRule is assumed to be linear and the gml:axisOrder property is assumed
                to be +1 +2.</documentation>
        </annotation>
    </element>

    <complexType name="TestType">
        <complexContent>
            <extension base="diggs:AbstractMeasurementType">
                <sequence>
                    <element minOccurs="0" name="samplingTime"
                        type="diggs:TimeIntervalOrInstantPropertyType">
                        <annotation>
                            <documentation>Describes the time that the result applies to the
                                property of the investigation target. This is often the time of
                                interaction by a sampling activity or measurement procedure with a
                                real-world feature. </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="resultTime"
                        type="diggs:TimeIntervalOrInstantPropertyType">
                        <annotation>
                            <documentation>Describes the time when the result became available,
                                typically when the procedure associated with the observation was
                                completed. For some observations this is identical to the
                                samplingTime. However, there are important cases where they differ.
                                Example: Where a measurement is made on a specimen in a laboratory,
                                the samplingTime should record the time the specimen was retrieved
                                from its host, while the resultTime should record the time the
                                laboratory procedure was applied. Example: Where monitoring
                                observation results are post-processed, the resultTime is the
                                post-processing time, while the samplingTime preserves the time of
                                initial interaction with the world. </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="validTime"
                        type="diggs:TimeIntervalOrInstantPropertyType">
                        <annotation>
                            <documentation>Describes the time period during which the result is
                                intended to be used.</documentation>
                        </annotation>
                    </element>
                    <element name="outcome" type="diggs:TestResultPropertyType">
                        <annotation>
                            <documentation>A property that contains the information about what
                                properties are being measured, the results of the measurement, and
                                the associated locations that the measurement results relate
                                to.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="procedure" type="diggs:ProcedurePropertyType"
                        maxOccurs="unbounded"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <!-- ================================================================ -->
    <!-- GROUP ELEMENTS                                                   -->
    <!-- ================================================================ -->

    <element name="SamplingFeatureGroup" substitutionGroup="diggs:AbstractGroup"
        type="diggs:SamplingFeatureGroupType">
        <annotation>
            <documentation>A top-level Diggs feature that references associated sampling features
                within a group</documentation>
        </annotation>
    </element>

    <complexType name="SamplingFeatureGroupType">
        <complexContent>
            <extension base="diggs:AbstractGroupType">
                <choice>
                    <element maxOccurs="unbounded" ref="diggs:samplingFeatureRef" minOccurs="1"/>
                </choice>
            </extension>
        </complexContent>
    </complexType>

    <complexType name="GroupPropertyType">
        <sequence>
            <element ref="diggs:AbstractGroup" minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <!-- ================================================================ -->
    <!-- EQUIPMENT ELEMENTS                                               -->
    <!-- ================================================================ -->

    <element name="CoringDevice" type="diggs:CoringDeviceType"
        substitutionGroup="diggs:AbstractEquipment" abstract="false">
        <annotation>
            <documentation>An object that contains properties of a borehole coring
                device.</documentation>
        </annotation>
    </element>

    <complexType name="CoringDeviceType">
        <complexContent>
            <extension base="diggs:AbstractEquipmentType">
                <sequence>
                    <element minOccurs="0" name="areaRatio" type="diggs:AreaPerAreaMeasureType">
                        <annotation>
                            <documentation>A sample disturbance parameter, defined as the difference
                                of the squares of the outside and inside diameter of the cutting
                                shoe, divided by the square of the inside diameter of the cutting
                                shoe.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="barrelExtension" type="diggs:LengthMeasureType">
                        <annotation>
                            <documentation>Distance that coring device extends beyond drill bit at
                                full extension. Negative values indicate barrel is recessed behind
                                bit.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="barrelInnerDiameter" type="diggs:LengthMeasureType">
                        <annotation>
                            <documentation>Diamter of inside of core barrel or sampling tube
                            </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="barrelOuterDiameter" type="diggs:LengthMeasureType">
                        <annotation>
                            <documentation>Diameter of outside of core barrel or sampling
                                tube</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="barrelLength" type="diggs:LengthMeasureType">
                        <annotation>
                            <documentation>Length of core barrel or sampling tube</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="insideClearanceRatio"
                        type="diggs:LengthPerLengthMeasureType">
                        <annotation>
                            <documentation>The inside sample diameter minus the cutting shoe inside
                                diameter divided by the cutting shoe inside diameter, expressed as a
                                percentage. A sample disturbance parameter.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="linerPresent" type="boolean">
                        <annotation>
                            <documentation>True if a liner is present in the barrel.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="linerType" type="gml:CodeType">
                        <annotation>
                            <documentation>The type of liner material or the type of coating within
                                the coring device (eg. porcelain coating on a shelby tube). Intended
                                to come from a controlled list of terms.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="outsideClearanceRatio"
                        type="diggs:LengthPerLengthMeasureType">
                        <annotation>
                            <documentation>The outside cutting shoe diameter minus the outside
                                sampler diameter, divided by the outside sampler diameter, expressed
                                as a percentage. A sample disturbance parameter.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="shoeInnerDiameter" type="diggs:LengthMeasureType">
                        <annotation>
                            <documentation>Inner diameter of the cutting shoe</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="shoeLength" type="diggs:LengthMeasureType">
                        <annotation>
                            <documentation>Length of the core shoe, eg. the device that fastens onto
                                the lower end of the main core barrel.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="shoeOuterDiameter" type="diggs:LengthMeasureType">
                        <annotation>
                            <documentation>Inner diameter of the cutting shoe</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="shoeType" type="gml:CodeType">
                        <annotation>
                            <documentation>Refers to the type of shoe used, typically manufacturer
                                specific.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <element abstract="false" name="Detector" type="diggs:DetectorType">
        <annotation>
            <documentation>A component of a sensor that measures one specific property (measurand),
                optionally along a single geometric axis</documentation>
        </annotation>
    </element>

    <complexType name="DetectorType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element minOccurs="0" name="detectorLocation" type="diggs:LocationPropertyType">
                        <annotation>
                            <documentation>The location that the detector measures (eg response zone
                                of the detector)</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="measurand"
                        type="gml:CodeWithAuthorityType">
                        <annotation>
                            <documentation>Contains the code value of the property being measured.
                                Codespace must be defined and the value should occur within a
                                dictionary</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="1" minOccurs="0" name="type" type="gml:CodeType">
                        <annotation>
                            <documentation>A term describing the type of sensor. Intended to come
                                from a controlled list of terms.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="measurementAxisBearing"
                        type="diggs:PlaneAngleMeasureType">
                        <annotation>
                            <documentation>Compass bearing direction (relative to true north)
                                defining the measurement axis direction (for directional
                                measurements, eg. component of an extensometer, accelerometer,
                                etc.)</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="measurementAxisInclination"
                        type="diggs:PlaneAngleMeasureType">
                        <annotation>
                            <documentation>Inclination of measurement axis (measured positively down
                                from horizontal); used for directional measurements, eg.
                                extensometer, accelerometer, etc. </documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <element name="DrillRig" substitutionGroup="diggs:AbstractEquipment" type="diggs:DrillRigType"/>

    <complexType name="DrillRigType">
        <complexContent>
            <extension base="diggs:AbstractEquipmentType">
                <sequence>
                    <element minOccurs="0" name="engine" type="diggs:DiggsStringType">
                        <annotation>
                            <documentation>A text string describing the name and other parameters
                                associated with the engine that drives the drill rig.
                            </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="carrierType" type="gml:CodeType">
                        <annotation>
                            <documentation>A description of the type of device that "carries" the
                                rig, eg. "rubber tire ATV", "truck mounted", "track mounted".
                                Intended to come from a controled list of terms. </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="tireSizeFront" type="diggs:DiggsStringType">
                        <annotation>
                            <documentation>A text string describing the size of the front tires for
                                a rubber tire mounted rig.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="tireSizeRear" type="diggs:DiggsStringType">
                        <annotation>
                            <documentation>A text string describing the size of the rear tires for a
                                rubber tire mounted rig.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="groundBearingPressure"
                        type="diggs:PressureMeasureType">
                        <annotation>
                            <documentation>The typical or maximum pressure exerted on the ground by
                                the weight of the rig.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="numberForwardGears" type="positiveInteger">
                        <annotation>
                            <documentation>The number of forward gears in the
                                transmission.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="numberReverseGears" type="positiveInteger">
                        <annotation>
                            <documentation>The number of reverse gears in the
                                transmission.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="transferCaseInfo" type="diggs:DiggsStringType">
                        <annotation>
                            <documentation>A text string describing the transfer case capabilities,
                                eg. "air shift controls for drill, rear axle (4x2) and front axle
                                (4x4)"</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="axleInfo" type="diggs:DiggsStringType">
                        <annotation>
                            <documentation>A text string describing axle information where
                                applicable, eg. "planetary with no-spin differentials, front and
                                rear"</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="winchCapacity"
                        type="diggs:WinchCapacityMeasureType">
                        <annotation>
                            <documentation>Maximum capacity of the winch (expressed as a
                                weight).</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="waterTankCapacity" type="diggs:VolumeMeasureType">
                        <annotation>
                            <documentation>Maximum volume of the onboard water tank.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="levelingSystem" type="diggs:DiggsStringType">
                        <annotation>
                            <documentation>A text string describing the system used for leveling the
                                rig (eg."Three hydraulic jacks, 36 inch (91.4 cm) stroke"
                            </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="rotaryDriveMaxTorque"
                        type="diggs:MomentOfForceMeasureType">
                        <annotation>
                            <documentation>The maximum torque produced by the rotary drive system.
                            </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="rotaryDriveMaxSpeed"
                        type="diggs:AngularVelocityMeasureType">
                        <annotation>
                            <documentation>The maximum rotational speed of the rotary drive system.
                            </documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="rotaryDriveGearRatio"
                        type="diggs:rotaryDriveGearRatioMeasureType">
                        <annotation>
                            <documentation>The ratio between the input rotation speed at the head
                                and rotation of the bit, depending on the gear
                                selected.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="pullDownForceMax" type="diggs:ForceMeasureType">
                        <annotation>
                            <documentation>Maximum crowd or pulldown force of the feed
                                system.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="feedRateMax" type="diggs:LengthPerTimeMeasureType">
                        <annotation>
                            <documentation>Maximum rate that the rig can feed the drill pipe into
                                the ground</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="retractForceMax" type="diggs:ForceMeasureType">
                        <annotation>
                            <documentation>Maximum retract force of the feed system.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="retractRateMax"
                        type="diggs:LengthPerTimeMeasureType">
                        <annotation>
                            <documentation>Maximum rate that the rig can retract the drill
                                pipe</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="strokeMax" type="diggs:LengthMeasureType">
                        <annotation>
                            <documentation>Maximum stroke height of the feed system.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="crowdSystemType" type="gml:CodeType">
                        <annotation>
                            <documentation>The type of system used to apply downward pressure
                                (typically winch or cylinder). Intended to come from a controlled
                                list of terms.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="crowdCylinderArea" type="diggs:AreaMeasureType">
                        <annotation>
                            <documentation>For a cylinder crowd system, this property holds the
                                cross-sectional area of the crowd sylinder.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="baselineCrowdPressure"
                        type="diggs:PressureMeasureType">
                        <annotation>
                            <documentation>The standard operating pressure for crowd force
                                application on the drill rig,</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="crowdConversionCoeff" type="double">
                        <annotation>
                            <documentation>The ratio of crowd force to the pressure applied
                                (force/pressure) to convert crowd pressure to force.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="baselineTorquePressure"
                        type="diggs:PressureMeasureType">
                        <annotation>
                            <documentation>The standard operating pressure for torque application on
                                the drill rig,</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="noHydraulicMotors" type="integer">
                        <annotation>
                            <documentation>The count of hydraulic motors used in the drilling
                                setup.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="maxHydMotorDisplacement"
                        type="diggs:VolumePerRotationMeasureType">
                        <annotation>
                            <documentation>he maximum volume displaced by the hydraulic motor per
                                revolution.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="minHydMotorDisplacement"
                        type="diggs:VolumePerRotationMeasureType">
                        <annotation>
                            <documentation>The minimum volume displaced by the hydraulic motor per
                                revolution.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="maxHydPressure" type="diggs:PressureMeasureType">
                        <annotation>
                            <documentation>The highest pressure at which the drill rig's hydraulic
                                system can operate.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="maxTorque" type="diggs:MomentOfForceMeasureType">
                        <annotation>
                            <documentation>The highest rotational force that can be applied by the
                                drill rig</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="mastLength" type="diggs:LengthMeasureType">
                        <annotation>
                            <documentation>Nominal length of the mast, typically measured from
                                ground to sheaves.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <complexType name="rotaryDriveGearRatioMeasureType">
        <simpleContent>
            <extension base="double">
                <attribute name="gearNumber" type="positiveInteger" use="required">
                    <annotation>
                        <documentation>The gear number associated with this ratio (eg. 1, 2 or 3
                            etc.</documentation>
                    </annotation>
                </attribute>
            </extension>
        </simpleContent>
    </complexType>

    <complexType name="WinchCapacityMeasureType">
        <simpleContent>
            <extension base="diggs:ForceMeasureType">
                <attribute name="winchLocation" type="string">
                    <annotation>
                        <documentation>Location of the winch on the rig (eg. front,
                            rear)</documentation>
                    </annotation>
                </attribute>
            </extension>
        </simpleContent>
    </complexType>

    <element name="Equipment" type="diggs:EquipmentType" substitutionGroup="diggs:AbstractEquipment"
        abstract="false">
        <annotation>
            <documentation>Generic Equipment object, contains only the properties of the base type.
                Used where a more specialized equipment object is not needed or not
                developed.</documentation>
        </annotation>
    </element>

    <complexType name="EquipmentType">
        <complexContent>
            <extension base="diggs:AbstractEquipmentType"/>
        </complexContent>
    </complexType>

    <element name="Sensor" type="diggs:SensorType" substitutionGroup="diggs:AbstractSensor"
        abstract="false">
        <annotation>
            <documentation>An instrument used to take readings as part of a measurement. A sensor
                inherits from AbstractEquipment and may be assigned to a location via a coordinate
                or by a text description. In addition, a sensor can also have one or more detectors,
                each of which can also be assigned a location, plus a measurand, and a measiurement
                axis orientation.</documentation>
        </annotation>
    </element>

    <element name="sensorRef" type="diggs:FeatureReferenceType">
        <annotation>
            <documentation>A reference to a sensor that is associated with this feature. </documentation>
            <documentation>A property element that supports a value by reference only. The target
                value is indicated in the appinfo element.</documentation>
        </annotation>
    </element>

    <complexType name="SensorPropertyType">
        <sequence>
            <element ref="diggs:Sensor" minOccurs="1" maxOccurs="1"/>
        </sequence>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>

    <complexType name="SensorType">
        <complexContent>
            <extension base="diggs:AbstractSensorType">
                <sequence/>
            </extension>
        </complexContent>
    </complexType>

    <element abstract="false" name="TriaxialTestEquipment"
        substitutionGroup="diggs:AbstractEquipment" type="diggs:TriaxialTestEquipmentType">
        <annotation>
            <documentation>An object that contains properties to capture specific parameters of
                triaxial test proecdure equipment.</documentation>
        </annotation>
    </element>

    <complexType name="TriaxialTestEquipmentType">
        <complexContent>
            <extension base="diggs:AbstractEquipmentType">
                <sequence>
                    <element minOccurs="0" name="loadCellCapacity" type="diggs:ForceMeasureType"/>
                    <element minOccurs="0" name="massOfTopPorousStone" type="diggs:MassMeasureType"/>
                    <element minOccurs="0" name="massOfTopCap" type="diggs:MassMeasureType"/>
                    <element minOccurs="0" name="pistonDiameter" type="diggs:LengthMeasureType">
                        <annotation>
                            <documentation>Type of pressure transducer, eg. single pressure
                                transducer, effective stress transducer. Intended to come from a
                                controlled list of terms.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="poreWaterPressureDeviceMeetsUndrainedSpecs"
                        type="boolean">
                        <annotation>
                            <documentation>For undrained tests, this property is true if pore water
                                pressure measurement system satisfies the requirment that the volume
                                change due to pore water pressure change divided by total volume of
                                specimen divided by the change in pore pressue less than 3.2 X 10-6
                                m2/kN (2.2 X 10-5 in2/lb (ASTM 4761-11, Sec. 5.8)</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="pressureTransducerType" type="gml:CodeType">
                        <annotation>
                            <documentation>Type of pressure transducer, eg. single pressure
                                transducer, effective stress transducer. Intended to come from a
                                controlled list of terms.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <!-- ================================================================ -->
    <!-- PROCEDURE ELEMENTS                                               -->
    <!-- ================================================================ -->

    <element name="SoilSpecimen" type="diggs:SoilSpecimenType"
        substitutionGroup="diggs:AbstractSpecimen">
        <annotation>
            <documentation>A specialization of a specimen that describes the physical conditions of
                a soil sample or samples subjected to testing.</documentation>
        </annotation>
    </element>

    <complexType name="SoilSpecimenType">
        <complexContent>
            <extension base="diggs:AbstractSpecimenType">
                <sequence>
                    <element minOccurs="0" name="conditionPreTest"
                        type="diggs:SpecimenConditionsPropertyType">
                        <annotation>
                            <documentation>Describes the physical conditions of the sample
                                pre-test</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="conditionIntermediate"
                        type="diggs:SpecimenConditionsPropertyType" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Describes the physical conditions of the specimen at an
                                intermediate stage during the test.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="conditionPostTest"
                        type="diggs:SpecimenConditionsPropertyType">
                        <annotation>
                            <documentation>Describes the physical conditions of the specimen
                                post-test. For triaxial tests, this condition is used to report
                                specimen conditions following consolidation stage.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="intactSpecimenPreparationInfo"
                        type="diggs:IntactSpecimenPreparationInfoPropertyType">
                        <annotation>
                            <documentation>Provides information on preparation of intact specimens,
                                such as whether and how specimen was trimmed for
                                testing</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="reconstitutedSpecimenPreparationInfo"
                        type="diggs:ReconstitutedSpecimenPreparationInfoPropertyType">
                        <annotation>
                            <documentation>Provides information on the preparation of reconstituted
                                samples (eg. number of lifts, compactor type, etc.)</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="sizeFraction" type="diggs:SizeFractionPropertyType">
                        <annotation>
                            <documentation>For soil speciments, size fraction from which test
                                portion was obtained</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="specificGravityOfSolids"
                        type="diggs:SpecificGravityMeasureType">
                        <annotation>
                            <documentation>Specific gravity of soil solids (Gs) is the ratio of the
                                density of a substance to the density of a reference substance,
                                usually water at 4 deg C; equivalently, it is the ratio of the mass
                                of a substance to the mass of a reference substance for the same
                                given volume.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="specimenType" type="diggs:SpecimenTypeType">
                        <annotation>
                            <documentation>The structural condition of the soil specimen being
                                tested, defined by the following controlled terms: 1) intact -
                                specimen prepared without disturbance from an "undisturbed" sample
                                (eg. thin-walled sampler); 2) disturbed - specimen prepared from a
                                disturbed sample (eg. thick walled sampler; 3) remolded - specimen
                                prepared from a failed or otherwise disturbed intact specimen; 4)
                                reconstituted - specimen prepared to a predetermined water content
                                and density</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <element name="RockSpecimen" type="diggs:RockSpecimenType"
        substitutionGroup="diggs:AbstractSpecimen">
        <annotation>
            <documentation>A specialization of a specimen that describes the physical conditions of
                a rock sample or samples subjected to testing.</documentation>
        </annotation>
    </element>

    <complexType name="RockSpecimenType">
        <complexContent>
            <extension base="diggs:AbstractSpecimenType">
                <sequence>
                    <element minOccurs="0" name="conditionPreTest"
                        type="diggs:SpecimenConditionsPropertyType">
                        <annotation>
                            <documentation>Describes the physical conditions of the specimen
                                pre-test</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="conditionIntermediate"
                        type="diggs:SpecimenConditionsPropertyType" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Describes the physical conditions of the specimen at an
                                intermediate stage during the test.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="conditionPostTest"
                        type="diggs:SpecimenConditionsPropertyType">
                        <annotation>
                            <documentation>Describes the physical conditions of the specimen
                                post-test. For triaxial tests, this condition is used to report
                                specimen conditions following consolidation stage.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <element name="ReconstitutedSpecimenPreparationInfo"
        type="diggs:ReconstitutedSpecimenPreparationInfoType">
        <annotation>
            <documentation>An object containing properties about a reconsituted specimen for
                testing.</documentation>
        </annotation>
    </element>

    <complexType name="ReconstitutedSpecimenPreparationInfoPropertyType">
        <sequence>
            <element ref="diggs:ReconstitutedSpecimenPreparationInfo"/>
        </sequence>
    </complexType>

    <complexType name="ReconstitutedSpecimenPreparationInfoType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element minOccurs="0" name="compactorType"
                        type="diggs:ReconstitutedSpecimenCompactorType">
                        <annotation>
                            <documentation>Type of compactor used, from an enumerated list: Drop
                                hammer, Harvard miniature, Static rod, Other: (with uncontrolled
                                string following colon).</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="numberOfLifts" type="positiveInteger">
                        <annotation>
                            <documentation>Type of compactor used, from an enumerated list: Drop
                                hammer, Harvard miniature, Static rod, Other: (with uncontrolled
                                string following colon).</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="relativeCompaction"
                        type="diggs:VolumePerVolumeMeasureType"/>
                    <element minOccurs="0" name="waterContentRelativeToOptimum" type="double">
                        <annotation>
                            <documentation>Water content relative to optimum water content,
                                expressed as decimal (eg. water content at optimum =
                                1)</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <simpleType name="ReconstitutedSpecimenCompactorEnumType">
        <restriction base="string">
            <enumeration value="Drop hammer"/>
            <enumeration value="Harvard miniature"/>
            <enumeration value="Static rod">
                <annotation>
                    <documentation>Short integer data value.</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>

    <simpleType name="ReconstitutedSpecimenCompactorType">
        <union memberTypes="diggs:ReconstitutedSpecimenCompactorEnumType diggs:OtherNameType"/>
    </simpleType>

    <element name="SpecimenConditions" type="diggs:SpecimenConditionsType">
        <annotation>
            <documentation>An object that describes the physical conditions of a specimen subjected
                to testing</documentation>
        </annotation>
    </element>

    <complexType name="SpecimenConditionsType">
        <complexContent>
            <extension base="diggs:AbstractComponentObjectType">
                <sequence>
                    <element minOccurs="0" name="dateTime" nillable="false"
                        type="diggs:TimeIntervalPropertyType">
                        <annotation>
                            <documentation>Date and (optionally) time or interval of time during
                                which this specimen condition evaluation was made.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="index" nillable="false" type="positiveInteger">
                        <annotation>
                            <documentation>Used for intermediate evaluations of specimen condition;
                                indicates the order of the intermediate measurements, in index order
                                (eg. 1, 2, 3 etc.)</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="bulkUnitWeight"
                        type="diggs:DensityOrUnitWeightMeasureType">
                        <annotation>
                            <documentation>Initial bulk density or unit weight</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="crossSectionalArea" type="diggs:AreaMeasureType">
                        <annotation>
                            <documentation>The cross-sectional area of the specimen. Specific
                                methods for computing this value may be given in the associated test
                                procedure object.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="dimensionMeasurementTechnique"
                        type="diggs:DimensionMeasurementTechniqueType">
                        <annotation>
                            <documentation>Technique used to measure specimen lengths and diameters.
                                Enumerated list. - 1) Calipers, 2) PI-tape, 3) Other: (uncontrolled
                                string after colon)</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="ringDiameter" type="diggs:LengthMeasureType">
                        <annotation>
                            <documentation>If a cylindrical specimen is contained within a ring, the inside diameter of the ring prior to specimen emplacement.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="dryUnitWeight"
                        type="diggs:DensityOrUnitWeightMeasureType">
                        <annotation>
                            <documentation>Dry density or unit weight of the
                                specimen</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="dryWeight" type="diggs:ForceMeasureType">
                        <annotation>
                            <documentation>The weight of the specimen following
                                drying</documentation>
                        </annotation>
                    </element>
                    <choice minOccurs="0">
                        <element minOccurs="0" name="diameter" type="diggs:LengthMeasureType">
                            <annotation>
                                <documentation>For cylindrical or speherical specimens, a measure of the
                                    specimen's diameter</documentation>
                            </annotation>
                        </element>
                        <sequence minOccurs="0">
                            <element minOccurs="0" name="length" type="diggs:LengthMeasureType">
                                <annotation>
                                    <documentation>A measure of one of the cross-sectional, or
                                        horizontal, dimensions of the prepared
                                        sample.</documentation>
                                </annotation>
                            </element>
                            <element minOccurs="0" name="width" type="diggs:LengthMeasureType">
                                <annotation>
                                    <documentation>A measure of the other of the cross-sectional, or
                                        horizontal, dimensions of the prepared
                                        sample</documentation>
                                </annotation>
                            </element>
                        </sequence>
                    </choice>
                    <element minOccurs="0" name="height" type="diggs:LengthMeasureType">
                        <annotation>
                            <documentation>The vertical dimension of the specimen</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="diameterHeightRatio" type="double">
                        <annotation>
                            <documentation>For cylindrical samples, the ratio of the ratio of specimen diameter to specimen height. To facilitate processing, where relevant, this value should be provided even if the specimen diameter and height are also provided.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="percentSaturation"
                        type="diggs:VolumePerVolumeMeasureType"/>
                    <element minOccurs="0" name="shape" type="diggs:SpecimenShapeType">
                        <annotation>
                            <documentation>The shape of the specimen being tested, defined by the
                                following controlled terms: 1) cylinder; 2) cube 3) cuboid - a
                                square cylinder or cube with uneven sides; 4) non-solid - sample is
                                a paste, liquid, or gas with no defined shape; 5)
                                sphere</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="surfaceArea" type="diggs:AreaMeasureType">
                        <annotation>
                            <documentation>The total surface area of the specimen, computed in
                                various ways dependent on specimen shape.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="temperature"
                        type="diggs:TemperatureMeasurementPropertyType">
                        <annotation>
                            <documentation>Temperature of the specimen, stored in the
                                TemperatureMeasurement object with optional dateTime that identifies
                                specific time instant for the measurement. Multiple temperature
                                measurements are permitted.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="volume" type="diggs:VolumeMeasureType">
                        <annotation>
                            <documentation/>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="waterContent" type="diggs:WaterContentMeasureType"/>
                    <element minOccurs="0" name="wetWeight" type="diggs:ForceMeasureType">
                        <annotation>
                            <documentation>The weight of the sample prior to drying.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="voidRatio" type="double">
                        <annotation>
                            <documentation>Void ratio (for soils) is defined as the volume of voids
                                divided by the volume of solids</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="otherConditionProperty"
                        type="diggs:ParameterPropertyType" maxOccurs="unbounded"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <complexType name="SpecimenConditionsPropertyType">
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:SpecimenConditions"/>
        </sequence>
    </complexType>

    <element name="IntactSpecimenPreparationInfo" type="diggs:IntactSpecimenPreparationInfoType">
        <annotation>
            <documentation>An object containing properties of an intact specimen.</documentation>
        </annotation>
    </element>

    <simpleType name="SpecimenShapeEnumType">
        <restriction base="string">
            <enumeration value="cylinder"/>
            <enumeration value="cube"/>
            <enumeration value="cuboid"/>
            <enumeration value="non-solid"/>
            <enumeration value="sphere"/>
        </restriction>
    </simpleType>

    <simpleType name="SpecimenShapeType">
        <union memberTypes="diggs:OtherNameType diggs:SpecimenShapeEnumType"/>
    </simpleType>

    <simpleType name="SpecimenTypeType">
        <union memberTypes="diggs:OtherNameType diggs:SpecimenEnumType"/>
    </simpleType>

    <simpleType name="SpecimenEnumType">
        <restriction base="string">
            <enumeration value="intact"/>
            <enumeration value="disturbed"/>
            <enumeration value="remolded"/>
            <enumeration value="reconstituted"/>
        </restriction>
    </simpleType>

    <simpleType name="DimensionMeasurementTechniqueEnumType">
        <restriction base="string">
            <enumeration value="Calipers"/>
            <enumeration value="PI-tape"/>
        </restriction>
    </simpleType>

    <simpleType name="DimensionMeasurementTechniqueType">
        <union memberTypes="diggs:DimensionMeasurementTechniqueEnumType diggs:OtherNameType"/>
    </simpleType>

    <complexType name="IntactSpecimenPreparationInfoPropertyType">
        <sequence>
            <element ref="diggs:IntactSpecimenPreparationInfo"/>
        </sequence>
    </complexType>

    <complexType name="IntactSpecimenPreparationInfoType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element minOccurs="0" name="IntactSpecimenType"
                        type="diggs:IntactSpecimenTypeType"/>
                    <element minOccurs="0" name="specimenTrimmedInWetRoom" type="boolean">
                        <annotation>
                            <documentation>For trimmed intact specimens, true if trimming occurs in
                                a wet room.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <simpleType name="IntactSpecimenTypeEnumType">
        <restriction base="string">
            <enumeration value="extruded"/>
            <enumeration value="sub-sampled"/>
            <enumeration value="trimmed">
                <annotation>
                    <documentation>Short integer data value.</documentation>
                </annotation>
            </enumeration>
        </restriction>
    </simpleType>

    <simpleType name="IntactSpecimenTypeType">
        <union memberTypes="diggs:IntactSpecimenTypeEnumType diggs:OtherNameType"/>
    </simpleType>

    <element name="TemperatureMeasurement" type="diggs:TemperatureMeasurementType">
        <annotation>
            <documentation>A temperature measurement optionally taken at a specific time
                instant.</documentation>
        </annotation>
    </element>

    <complexType name="TemperatureMeasurementPropertyType">
        <sequence>
            <element ref="diggs:TemperatureMeasurement"/>
        </sequence>
    </complexType>

    <complexType name="TemperatureMeasurementType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element minOccurs="0" name="dateTime" type="gml:TimePositionType"/>
                    <element minOccurs="1" name="temperature"
                        type="diggs:ThermodynamicTemperatureMeasureType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <element name="Specification" type="diggs:SpecificationType"
        substitutionGroup="diggs:AbstractProcedure" abstract="false">
        <annotation>
            <documentation>An object that describes the specification or method used for testing or
                construction.</documentation>
        </annotation>
    </element>

    <complexType name="SpecificationType">
        <complexContent>
            <extension base="diggs:AbstractProcedureType">
                <sequence>
                    <element name="accredtingBody" type="diggs:DiggsStringType" minOccurs="0"
                        maxOccurs="1">
                        <annotation>
                            <documentation>Identification of the accrediting body and reference
                                number, if applicable.</documentation>
                        </annotation>
                    </element>
                    <element name="shortMethodName" type="diggs:DiggsStringType" minOccurs="0"
                        maxOccurs="1"/>
                    <element name="standardClause" type="diggs:DiggsStringType" minOccurs="0"
                        maxOccurs="1"/>
                    <element name="standardPart" type="diggs:DiggsStringType" minOccurs="0"
                        maxOccurs="1"/>
                    <element name="standardReferenceNumber" type="diggs:DiggsStringType"
                        minOccurs="0" maxOccurs="1"/>
                    <element name="standardTitle" type="diggs:DiggsStringType" minOccurs="0"
                        maxOccurs="1"/>
                    <element name="standardVersion" type="diggs:DiggsStringType" minOccurs="0"
                        maxOccurs="1"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="otherSpecificationProperty"
                        type="diggs:ParameterPropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <!-- ================================================================ -->
    <!-- DEPRECATED ELEMENTS                                               -->
    <!-- ================================================================ -->

    <element name="filterPackInterval" type="diggs:LinearExtentPropertyType">
        <annotation>
            <documentation>The depth interval in which the filter pack surrounding
                the well screen or well openings occur.
                
                DEPRECATED. Filter pack information should be entered as a Backfill object with a reference there to this well opening object.</documentation>
        </annotation>
    </element>

    <element name="holeBearing" type="diggs:PlaneAngleMeasureType">
        <annotation>
            <documentation>Deprecated - use bearing property from abstract
                type</documentation>
        </annotation>
    </element>

    <element name="holePlunge" type="diggs:PlaneAngleMeasureType">
        <annotation>
            <documentation>Depreated - use plunge property from abstract type
            </documentation>
        </annotation>
    </element>

    <element name="chiseling" type="diggs:ChiselingPropertyType">
        <annotation>
            <documentation>Information on chiseling activity in a hole. Used as
                legacy to directly accommodate AGS CHIS group data. This property is
                deprecated and instead, data on chiseling should be encoded within
                the BoreholeConstructionMethod object (to define the interval over
                which chiseling occurs, chiseling tool used, and the
                DrillAdvancement object to record chiseling rate of
                advancement.</documentation>
        </annotation>
    </element>

    <element name="Chiseling" type="diggs:ChiselingType">
        <annotation>
            <documentation>An object containing properties about chiseling activity as part of
                advancement of a borehole.</documentation>
        </annotation>
    </element>

    <complexType name="ChiselingPropertyType">
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:Chiseling"/>
        </sequence>
    </complexType>

    <complexType name="ChiselingType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element name="chiselingLocation" type="diggs:LinearExtentPropertyType">
                        <annotation>
                            <documentation>The measured depth interval over which the chiseling
                                takes place, recorded in the hole's linear referencing
                                system.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="timeTaken" type="diggs:TimeMeasureType">
                        <annotation>
                            <documentation>The amount of time taken to perform the chiseling
                                task.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="chiselingToolUsed" type="gml:StringOrRefType">
                        <annotation>
                            <documentation>Tool used to perform the chiseling. Intended to come from
                                a controlled list. NOTE: This is a string that could come from a
                                controlled list of terms, but could also reference an Equipment
                                metadata object.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <element name="sanitarySealLocation" type="diggs:PointLocationPropertyType">
        <annotation>
            <documentation>Distance to the base of the sanitary seal; measured in
                the borehole's linear reference system. DEPRECATED. Use Backfill object of the borehole to represent the sanitary seal</documentation>
        </annotation>
    </element>

    <element name="sanitarySealType" type="gml:CodeType">
        <annotation>
            <documentation>The type of material used for the sanitary or surface
                seal (eg. bentonite, grout, etc.)
                DEPRECATED, use Backfill object to identify the sanitary seal material</documentation>
        </annotation>
    </element>

 

    <element name="penetrationRate" type="diggs:LengthPerTimeMeasureType">
        <annotation>
            <documentation>Rate of advancement of the drill head. Deprecated - use
                advamcementRate instead</documentation>
        </annotation>
    </element>

</schema>
