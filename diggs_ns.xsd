						<sequence>
							<element maxOccurs="1" minOccurs="0" name="penetrationGroutRef" type="diggs:FeatureReferenceType">
								<annotation>
									<appinfo>diggs:GroutBatch</appinfo>
									<documentation>This property references the grout sample(s) used
                                        for this stage</documentation>
								</annotation>
							</element>
							<element maxOccurs="1" minOccurs="0" name="withdrawalGroutRef" type="diggs:FeatureReferenceType">
								<annotation>
									<appinfo>diggs:GroutBatch</appinfo>
									<documentation>This property references the grout sample(s) used
                                        for this stage</documentation>
								</annotation>
							</element>
