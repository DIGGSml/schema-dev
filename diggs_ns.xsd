	<complexType mixed="false" name="CushionType">
		<annotation>
			<documentation>A compressible material placed between the hammer and the pile or the pile and the pile cap to distribute impact force and reduce damage.</documentation>
		</annotation>
		<complexContent>
			<extension base="diggs:AbstractComponentObjectType">
				<sequence>
					<element minOccurs="0" name="cushionElement" maxOccurs="unbounded" type="diggs:CushionElementPropertyType">
						<annotation>
							<documentation>A reference to a CushionElement object that contains properties for a cushion element.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="totalThicknessBeforeDrive" type="eml:LengthMeasureExt">
						<annotation>
							<documentation>The total thickness of the cushion before driving begins.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="totalThicknessAfterDrive" type="eml:LengthMeasureExt">
						<annotation>
							<documentation>The total thickness of the cushion after driving is complete.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="areaBeforeDrive" type="eml:AreaMeasureExt">
						<annotation>
							<documentation>The area of the cushion before driving begins.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="areaAfterDrive" type="eml:AreaMeasureExt">
						<annotation>
							<documentation>The area of the cushion after driving is complete.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="coefficientOfRestitution">
						<annotation>
							<documentation>A value between 0 and 1 describing the energy returned after impact, 1 is a perfectly elastic impact and 0 is a perfectly plastic impact.</documentation>
						</annotation>
						<simpleType>
							<restriction base="double">
								<minInclusive value="0"></minInclusive>
								<maxInclusive value="1"></maxInclusive>
							</restriction>
						</simpleType>
					</element>
				</sequence>
			</extension>
