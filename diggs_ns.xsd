	<complexType name="LocalityType">
		<complexContent>
			<extension base="diggs:AbstractObjectBaseType">
				<sequence>
					<element name="address" type="diggs:AddressPropertyType" minOccurs="0" maxOccurs="1"></element>
					<element name="district" type="diggs:DiggsStringType" minOccurs="0" maxOccurs="1">
