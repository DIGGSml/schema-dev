						</annotation>
					</element>
					<element maxOccurs="1" minOccurs="0" name="fluid_3" type="diggs:GroutingFluidPropertyType">
						<annotation>
							<appinfo></appinfo>
							<documentation>This property holds or references a GroutingFluid object
                                describing the fluid used during jet grouting</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="groutInterval" type="diggs:LinearExtentPropertyType">
