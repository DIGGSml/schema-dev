	<complexType name="DrillRigType">
		<complexContent>
			<extension base="diggs:AbstractEquipmentType">
				<sequence>
					<element minOccurs="0" name="engine" type="diggs:DiggsStringType">
						<annotation>
							<documentation>A text string describing the name and other parameters associated with the engine that drives the drill rig.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="carrierType" type="gml:CodeType">
						<annotation>
							<documentation>A description of the type of device that "carries" the rig, eg. "rubber tire ATV", "truck mounted", "track mounted". Intended to come from a controled list of terms.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="tireSizeFront" type="diggs:DiggsStringType">
						<annotation>
							<documentation>A text string describing the size of the front tires for a rubber tire mounted rig.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="tireSizeRear" type="diggs:DiggsStringType">
						<annotation>
							<documentation>A text string describing the size of the rear tires for a rubber tire mounted rig.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="groundBearingPressure" type="eml:PressureMeasureExt">
						<annotation>
							<documentation>The typical or maximum pressure exerted on the ground by the weight of the rig.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="numberForwardGears" type="positiveInteger">
						<annotation>
							<documentation>The number of forward gears in the transmission.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="numberReverseGears" type="positiveInteger">
						<annotation>
							<documentation>The number of reverse gears in the transmission.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="transferCaseInfo" type="diggs:DiggsStringType">
						<annotation>
							<documentation>A text string describing the transfer case capabilities, eg. "air shift controls for drill, rear axle (4x2) and front axle (4x4)"</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="axleInfo" type="diggs:DiggsStringType">
						<annotation>
							<documentation>A text string describing axle information where applicable, eg. "planetary with no-spin differentials, front and rear"</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="winchCapacity" type="diggs:WinchCapacityMeasureType">
						<annotation>
							<documentation>Maximum capacity of the winch (expressed as a weight).</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="waterTankCapacity" type="eml:VolumeMeasureExt">
						<annotation>
							<documentation>Maximum volume of the onboard water tank.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="levelingSystem" type="diggs:DiggsStringType">
						<annotation>
							<documentation>A text string describing the system used for leveling the rig (eg."Three hydraulic jacks, 36 inch (91.4 cm) stroke"</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="rotaryDriveMaxTorque" type="eml:MomentOfForceMeasureExt">
						<annotation>
							<documentation>The maximum torque produced by the rotary drive system.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="rotaryDriveMaxSpeed" type="eml:AngularVelocityMeasureExt">
						<annotation>
							<documentation>The maximum rotational speed of the rotary drive system.</documentation>
						</annotation>
					</element>
					<element maxOccurs="unbounded" minOccurs="0" name="rotaryDriveGearRatio">
						<annotation>
							<documentation>The ratio between the input rotation speed at the head and rotation of the bit, depending on the gear selected.</documentation>
						</annotation>
						<complexType>
							<simpleContent>
								<extension base="double">
									<attribute name="gearNumber" type="positiveInteger" use="required">
										<annotation>
											<documentation>The gear number associated with this ratio (eg. 1, 2 or 3 etc.</documentation>
										</annotation>
									</attribute>
								</extension>
							</simpleContent>
						</complexType>
					</element>
					<element minOccurs="0" name="pullDownForceMax" type="eml:ForceMeasureExt">
						<annotation>
							<documentation>Maximum crowd or pulldown force of the feed system.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="feedRateMax" type="eml:LengthPerTimeMeasureExt">
						<annotation>
							<documentation>Maximum rate that the rig can feed the drill pipe into the ground</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="retractForceMax" type="eml:ForceMeasureExt">
						<annotation>
							<documentation>Maximum retract force of the feed system.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="retractRateMax" type="eml:LengthPerTimeMeasureExt">
						<annotation>
							<documentation>Maximum rate that the rig can retract the drill pipe</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="strokeMax" type="eml:LengthMeasureExt">
						<annotation>
							<documentation>Maximum stroke height of the feed system.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="crowdSystemType" type="gml:CodeType">
						<annotation>
							<documentation>The type of system used to apply downward pressure (typically winch or cylinder). Intended to come from a controlled list of terms.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="crowdCylinderArea" type="eml:AreaMeasureExt">
						<annotation>
							<documentation>For a cylinder crowd system, this property holds the cross-sectional area of the crowd sylinder.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="baselineCroudPressure" type="eml:PressureMeasureExt">
						<annotation>
							<documentation>The standard operating pressure for crowd force application on the drill rig,</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="crowdConversionCoeff" type="double">
						<annotation>
							<documentation>The ratio of crowd force to the pressure applied (force/pressure) to convert crowd pressure to force.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="baselineTorquePressure" type="eml:PressureMeasureExt">
						<annotation>
							<documentation>The standard operating pressure for torque application on the drill rig,</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="noHydraulicMotors" type="integer">
						<annotation>
							<documentation>The count of hydraulic motors used in the drilling setup.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="maxHydMotorDisplacement" type="eml:VolumePerRotationMeasureExt">
						<annotation>
							<documentation>he maximum volume displaced by the hydraulic motor per revolution.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="minHydMotorDisplacement" type="eml:VolumePerRotationMeasureExt">
						<annotation>
							<documentation>The minimum volume displaced by the hydraulic motor per revolution.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="maxHydPressure" type="eml:PressureMeasureExt">
						<annotation>
							<documentation>The highest pressure at which the drill rig's hydraulic system can operate.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="maxTorque" type="eml:MomentOfForceMeasureExt">
						<annotation>
							<documentation>The highest rotational force that can be applied by the drill rig</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="mastLength" type="eml:LengthMeasureExt">
						<annotation>
							<documentation>Nominal length of the mast, typically measured from ground to sheaves.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="cuttingToolInfo" type="diggs:CuttingToolArrayPropertyType"></element>
					<element minOccurs="0" name="drillPipeInfo" type="diggs:DrillPipeArrayPropertyType"></element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="DrivenPenetrationTestType">
