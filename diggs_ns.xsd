					<element minOccurs="0" name="binder" type="diggs:BinderPropertyType" maxOccurs="unbounded">
						<annotation>
							<documentation>Property containing information on binders used in mix;
                                includes fly ash, lime, etc., but excluding cement, which is
                                recorded in its own component property</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="cement" type="diggs:CementPropertyType" maxOccurs="unbounded">
						<annotation>
							<documentation>Property containing information on cement used in the
                                grout mix</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="chemical" type="diggs:ChemicalPropertyType" maxOccurs="unbounded">
						<annotation>
							<documentation>Property containing information on cement used in the
                                grout mix</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="filler" type="diggs:FillerPropertyType" maxOccurs="unbounded">
						<annotation>
							<documentation>Property containing information on fillers used in mix.
                                Aggregate is a type of filler, as would be a soil in a soil-cement
                                mixture used to determine the appropriate slurry-soil mixes for deep
                                soil mixing</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="specificGravityMix" type="diggs:SpecificGravityMeasureType">
						<annotation>
							<documentation>The specific gravity of the grout mix</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="specificGravityWater" type="diggs:SpecificGravityMeasureType">
						<annotation>
							<documentation>The specific gravity of water used for mix
                                proportions,</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="totalBatchVolume" type="eml:VolumeMeasure">
						<annotation>
							<documentation>Total volume of batch produced</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="typeMix" type="gml:CodeType">
						<annotation>
							<documentation>Type of grout mix; preferably from a controlled list, eg.
                                cement, chemical, soil-cement, etc.</documentation>
							<documentation>Should this be an enum if only a few common
                                choices?</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="volumeWater" type="eml:VolumeMeasureExt">
						<annotation>
							<documentation>Volume of water used</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="waterCementRatio" type="eml:ForcePerForceMeasureExt">
						<annotation>
							<documentation>Ratio of the weight of water mixed with the cement to
                                weight of cement used for this grout.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="waterCementRatioOfMix" type="eml:ForcePerForceMeasureExt">
						<annotation>
							<documentation>Ratio of the weight of total water in the mix to the
                                weight of cement</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="wtCementPerGalH2O" type="eml:ForceMeasureExt">
						<annotation>
							<documentation>Weight of cement used per gallon of
                                water.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="binderFactor" type="diggs:DensityOrUnitWeightMeasureType">
						<annotation>
							<documentation>Weight of cement devided by the volume of soil in a
                                soil-cement mix.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="binderFactorInPlace" type="diggs:DensityOrUnitWeightMeasureType">
						<annotation>
							<documentation>Weight of cement devided by the volume of soil-cement
                                mixture, which is equal to the sum of the volumes of the cement,
                                soil, and water.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="binderContent" type="eml:ForcePerForceMeasureExt">
						<annotation>
							<documentation>Weight of cement used per gallon of
                                water.</documentation>
						</annotation>
					</element>
					<element minOccurs="0" name="volumeRatio" type="eml:VolumePerVolumeMeasureExt">
						<annotation>
							<documentation>Ratio of the volume of cenent plus volume of the water in
                                the cement slurry, to the volume of soil in a cement-soil
                                mix</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
