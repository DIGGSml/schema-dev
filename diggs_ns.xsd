	<complexType abstract="false" name="AbstractProgramDesignPropertyType">
		<sequence>
			<element ref="diggs:AbstractProgramDesign"></element>
		</sequence>
	</complexType>
	<complexType abstract="true" name="AbstractProgramDesignType">
		<complexContent>
			<extension base="diggs:AbstractFeatureType"></extension>
		</complexContent>
