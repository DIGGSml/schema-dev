	<complexType abstract="true" name="AbstractProgramType">
		<annotation>
			<documentation>The base type for all programs, which extend this type</documentation>
		</annotation>
		<complexContent>
			<extension base="diggs:AbstractFeatureType">
				<sequence>
					<element minOccurs="1" ref="diggs:projectRef">
						<annotation>
							<documentation>The location of the description</documentation>
						</annotation>
					</element>
					<element maxOccurs="unbounded" minOccurs="0" name="design" type="diggs:AbstractProgramDesignPropertyType"></element>
					<element maxOccurs="unbounded" minOccurs="0" name="specification" type="diggs:AbstractProgramSpecificationPropertyType"></element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="AbstractProjectType" abstract="true">
		<complexContent>
			<extension base="diggs:AbstractNamedFeatureType">
				<sequence>
					<element name="referencePoint" type="diggs:PointLocationPropertyType" minOccurs="0" maxOccurs="1">
						<annotation>
							<documentation>Defines the location of the project (usually a
                                location at the ground surface) within a well-known 3D CRS or
                                combined CRS (geographic or projected, and elevation), as an
                                extension of gml:Point object with an additional uncertainty
                                property</documentation>
						</annotation>
					</element>
					<element name="linearExtent" type="diggs:LinearExtentPropertyType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>Defines a project geometry that may be defined as a line in geographic or projected space and contains a LinearExtent geometry object.</documentation>
						</annotation>
					</element>
					<element name="arealExtent" type="diggs:AbstractSurfacePropertyType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>Defines a project geometry that represents the areal extent of a project in geographic or projected space and contains a surface geometry object.</documentation>
						</annotation>
					</element>
					<element name="projectDateTimeSpan" type="diggs:TimeIntervalPropertyType" minOccurs="0" maxOccurs="1">
						<annotation>
							<documentation>The interval of time during which the project was active.</documentation>
						</annotation>
					</element>
					<element name="locality" type="diggs:LocalityPropertyType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>Contains either a Locality object or a PointLocation object that describes, repectively, the non-coordinate or coordinate location of the project. If only one locality property exists for the project, referencePoint should be used to desccribe the coordinate location for the project instead of the PointLocation object.</documentation>
						</annotation>
					</element>
					<element name="purpose" type="gml:CodeType" minOccurs="0" maxOccurs="1">
						<annotation>
							<documentation>A string descxribing the purpose for the project. This is intended to come from a controlled list of terms.</documentation>
						</annotation>
					</element>
					<element maxOccurs="unbounded" minOccurs="0" name="projectEvent" type="diggs:ProjectEventPropertyType">
						<annotation>
							<documentation>Contains a ProjectEvent property describing the occurrence of a time or location related event associated with a project.</documentation>
						</annotation>
					</element>
					<element name="contract" type="diggs:ContractPropertyType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>Contains a Contract object that describes the properties of a business contract associated with the project.</documentation>
						</annotation>
					</element>
					<element maxOccurs="unbounded" minOccurs="0" name="otherProjectProperty" type="diggs:ParameterPropertyType">
						<annotation>
							<documentation>Contains a Parameter object used to describe the name and value of a project property not otherwise explicitly defined in the schema.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="AbstractRelativeReferenceSystemPropertyType">
		<sequence>
			<element maxOccurs="1" minOccurs="1" ref="diggs:AbstractRelativeReferenceSystem"></element>
		</sequence>
	</complexType>
	<complexType name="AbstractRelativeReferenceSystemType">
		<complexContent>
			<extension base="glr:LinearSRSType"></extension>
		</complexContent>
	</complexType>
	<complexType name="AbstractSamplePropertyType">
