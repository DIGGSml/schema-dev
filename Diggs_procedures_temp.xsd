        <complexContent>
            <extension base="diggs:AbstractLaboratoryTestProcedureType">
                <sequence>
                    <element name="trial" type="diggs:LabCBRTestTrialPropertyType" minOccurs="0"
                        maxOccurs="unbounded"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element name="LabCBRTestTrial" type="diggs:LabCBRTestTrialType" abstract="false"
        substitutionGroup="diggs:AbstractObject">
        <annotation>
            <documentation>An object containing properties of a single increment within a lab California Bearing Ratio test procedure.</documentation>
        </annotation>
    </element>
    <complexType name="LabCBRTestTrialPropertyType">
        <annotation>
            <documentation>Property for LabCBRTestTrials, required parent element that holds the
                multiple trials.</documentation>
        </annotation>
