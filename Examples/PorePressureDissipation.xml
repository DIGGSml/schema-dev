<?xml version="1.0" encoding="UTF-8"?>
<Diggs xmlns="http://diggsml.org/schemas/2.5.b" xmlns:diggs="http://diggsml.org/schemas/2.5.b"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:g3.3="http://www.opengis.net/gml/3.3/ce"
    xmlns:glr="http://www.opengis.net/gml/3.3/lr" xmlns:glrov="http://www.opengis.net/gml/3.3/lrov"
    xmlns:diggs_geo="http://diggsml.org/schemas/2.5.b/geotechnical"
    xmlns:witsml="http://www.witsml.org/schemas/131"
    xsi:schemaLocation="http://diggsml.org/schemas/2.5.b ../Complete.xsd"
    gml:id="bcd">

    <documentInformation>
        <DocumentInformation gml:id="d1">
            <creationDate>2020-01-01</creationDate>
        </DocumentInformation>
    </documentInformation>

    <project>

        <Project gml:id="p1">
            <gml:name>Test Project</gml:name>
            <remark>
                <Remark>
                    <content>This could be a virtual project that has no relevance outside of this
            instance</content>
                </Remark>
            </remark>
        </Project>

    </project>

    <samplingFeature>
        <Sounding gml:id="cpt-1">
            <gml:name>Sounding 1</gml:name>
            <role>
                <Role>
                    <rolePerformed>CPT Operator</rolePerformed>
                    <businessAssociate>
                        <BusinessAssociate gml:id="a26">
                            <gml:name>Joe Operator</gml:name>
                            <associatedWith xlink:href="#a25"/>
                        </BusinessAssociate>
                    </businessAssociate>
                </Role>
            </role>
            <investigationTarget>Natural Ground</investigationTarget>
            <projectRef xlink:href="#p1"/>
            <referencePoint>
                <PointLocation gml:id="cpt1" srsName="urn:diggs:def:crs:DIGGS:0.1:26911_5703"
                    srsDimension="3">
                    <gml:pos>387416.665116977 3742645.12297961 6</gml:pos>
                </PointLocation>
            </referencePoint>
            <centerLine>
                <LinearExtent gml:id="ls1" srsDimension="3"
                    srsName="urn:diggs:def:crs:DIGGS:0.1:26911_5703">
                    <gml:posList>387416.665116977 3742645.12297961 6 387416.665116977
                        3742645.12297961 1.44</gml:posList>
                </LinearExtent>
            </centerLine>
            <linearReferencing>
                <LinearSpatialReferenceSystem gml:id="cptsr1">
                    <gml:identifier codeSpace="urn:x-diggs:def:authority:DIGGSINC">urn:diggs:def:fi:DIGGSINC:cptsr1</gml:identifier>
                    <glr:linearElement xlink:href="#ls1"/>
                    <glr:lrm>
                        <glr:LinearReferencingMethod gml:id="lrcpt1">
                            <glr:name>chainage</glr:name>
                            <glr:type>absolute</glr:type>
                            <glr:units>m</glr:units>
                        </glr:LinearReferencingMethod>
                    </glr:lrm>
                </LinearSpatialReferenceSystem>
            </linearReferencing>
            <totalMeasuredDepth uom="m">5.44</totalMeasuredDepth>
            <constructionMethod>
                <SoundingConstructionMethod gml:id="cmcpt">
                    <remark>
                        <Remark>
                            <content>Hand dug top 0.5 foot before conducting the sounding</content>
                        </Remark>
                    </remark>
                    <location>
                        <LinearExtent gml:id="cptml1" srsDimension="1" srsName="#cptsr1">
                            <gml:posList>0 0.15</gml:posList>
                        </LinearExtent>
                    </location>
                </SoundingConstructionMethod>
            </constructionMethod>
            <constructionMethod>
                <SoundingConstructionMethod gml:id="cmcpt2">
                    <gml:description>CPT Sounding</gml:description>
                    <location>
                        <LinearExtent gml:id="cptml2" srsDimension="1" srsName="#cptsr1">
                            <gml:posList>0.15 5.44</gml:posList>
                        </LinearExtent>
                    </location>
                    <constructionEquipment>
                        <Equipment gml:id="cpte-1">
                            <gml:name>Hogentogler 20-ton rig</gml:name>
                        </Equipment>
                    </constructionEquipment>
                </SoundingConstructionMethod>
            </constructionMethod>
        </Sounding>
    </samplingFeature>

    <measurement>
        <Test gml:id="ppt1">
            <gml:name>PPDT at 5 m depth</gml:name>
            <investigationTarget>Natural Ground</investigationTarget>
            <projectRef xlink:href="#p1"/>
            <samplingFeatureRef xlink:href="#cpt-1"/>
            <resultTime>
                <TimeInterval gml:id="tti1">
                    <start>2020-01-01</start>
                </TimeInterval>
            </resultTime>
            <outcome>
                <TestResult gml:id="tr2">
                    <location>
                        <PointLocation gml:id="pwd-1p2" srsDimension="1" srsName="#cptsr1">
                            <gml:pos>5</gml:pos>
                        </PointLocation>
                    </location>
                    <results>
                        <ResultSet>
                            <parameters>
                                <PropertyParameters gml:id="prpa2">
                                    <properties>
                                        <Property gml:id="Ddle167" index="1">
                                            <propertyName>water depth</propertyName>
                                            <typeData>double</typeData>
                                            <propertyClass codeSpace="https://diggsml.org/dictionaries/DIGGSTestPropertyDefinitions.xml#water_depth_calc">calculated depth to water</propertyClass>
                                            <uom>ft</uom>
                                            <nullValue reason="missing">9999</nullValue>
                                        </Property>
                                        <Property index="2" gml:id="Ddle168">
                                            <propertyName>u0</propertyName>
                                            <typeData>double</typeData>
                                            <propertyClass codeSpace="https://diggsml.org/dictionaries/DIGGSTestPropertyDefinitions.xml#pore_pressure_equil">equilibrium pore water pressure (u0)</propertyClass>
                                            <uom>MPa</uom>
                                        </Property>
                                        <Property index="2" gml:id="Ddle169">
                                            <propertyName>kh</propertyName>
                                            <typeData>double</typeData>
                                            <propertyClass codeSpace="https://diggsml.org/dictionaries/DIGGSTestPropertyDefinitions.xml#coef_permeability_horiz">horizontal coefficient of permeability</propertyClass>
                                            <uom>ft/d</uom>
                                        </Property>
                                    </properties>
                                </PropertyParameters>
                            </parameters>
                            <dataValues cs="," decimal="." ts=" ">
                                7.2,12.8,1.8
                            </dataValues>
                        </ResultSet>
                    </results>
                </TestResult>
            </outcome>
            <procedure>
                <diggs_geo:PorePressureDissipationTest gml:id="f2">
                    <testProcedureMethod>
                        <Specification gml:id="sp1">
                            <gml:name>Some ASTM Std.</gml:name>
                        </Specification>
                    </testProcedureMethod>
                    <diggs_geo:dissipationIncrement>
                        <diggs_geo:PorePressureDissipationIncrement gml:id="i1">
                            <diggs_geo:elapsedTime uom="s">0</diggs_geo:elapsedTime>
                            <diggs_geo:u2PorePressure uom="MPa">50</diggs_geo:u2PorePressure>
                        </diggs_geo:PorePressureDissipationIncrement>
                    </diggs_geo:dissipationIncrement>
                    <diggs_geo:dissipationIncrement>
                        <diggs_geo:PorePressureDissipationIncrement gml:id="i2">
                            <diggs_geo:elapsedTime uom="s">5</diggs_geo:elapsedTime>
                            <diggs_geo:u2PorePressure uom="MPa">45</diggs_geo:u2PorePressure>
                        </diggs_geo:PorePressureDissipationIncrement>
                    </diggs_geo:dissipationIncrement>
                    <diggs_geo:dissipationIncrement>
                        <diggs_geo:PorePressureDissipationIncrement gml:id="i3">
                            <diggs_geo:elapsedTime uom="s">10</diggs_geo:elapsedTime>
                            <diggs_geo:u2PorePressure uom="MPa">42</diggs_geo:u2PorePressure>
                        </diggs_geo:PorePressureDissipationIncrement>
                    </diggs_geo:dissipationIncrement>
                    <diggs_geo:dissipationIncrement>
                        <diggs_geo:PorePressureDissipationIncrement gml:id="i4">
                            <diggs_geo:elapsedTime uom="s">15</diggs_geo:elapsedTime>
                            <diggs_geo:u2PorePressure uom="MPa">40</diggs_geo:u2PorePressure>
                        </diggs_geo:PorePressureDissipationIncrement>
                    </diggs_geo:dissipationIncrement>
                </diggs_geo:PorePressureDissipationTest>
            </procedure>
        </Test>
    </measurement>
    <measurement>
        <Monitor gml:id="pwd-1">
            <gml:name>Pore Pressure Dissipation</gml:name>
            <investigationTarget>Natural Ground</investigationTarget>
            <projectRef xlink:href="#p1"/>
            <samplingFeatureRef xlink:href="#cpt-1"/><!-- References CPT sounding -->
            <reading>
                <Reading gml:id="pwd-1-1">
                    <responseZoneLocation>
                        <PointLocation gml:id="pwd-1pl" srsDimension="1" srsName="#cptsr1">
                            <gml:pos>5</gml:pos>
                        </PointLocation>
                    </responseZoneLocation>
                    <outcome>
                        <MonitorResult gml:id="pwd-1mr">
                            <timeDomain>
                                <MultiTimeInstant gml:id="mti1">
                                    <timeMember>
                                        <gml:TimeInstant gml:id="ti1">
                                            <gml:description>Starting time</gml:description>
                                            <gml:timePosition>12:00:00</gml:timePosition>
                                        </gml:TimeInstant>
                                    </timeMember>
                                    <timeMember>
                                        <gml:TimeInstant gml:id="ti2">
                                            <gml:timePosition>12:01:00</gml:timePosition>
                                        </gml:TimeInstant>
                                    </timeMember>
                                    <timeMember>
                                        <gml:TimeInstant gml:id="ti3">
                                            <gml:timePosition>12:02:00</gml:timePosition>
                                        </gml:TimeInstant>
                                    </timeMember>
                                </MultiTimeInstant>
                            </timeDomain>
                            <results>
                                <ResultSet>
                                    <parameters>
                                        <PropertyParameters gml:id="pp1">
                                            <properties>
                                                <Property gml:id="Ddle267" index="1">
                                                  <propertyName>U2</propertyName>
                                                  <typeData>double</typeData>
                                                  <propertyClass codeSpace="https://diggsml.org/dictionaries/DIGGSTestPropertyDefinitions.xml#pore_water_pressure">pore water pressure</propertyClass>
                                                  <uom>MPa</uom>
                                                  <nullValue reason="missing">9999</nullValue>
                                                </Property>
                                            </properties>
                                        </PropertyParameters>
                                    </parameters>
                                    <dataValues cs="," ts=" " decimal=".">
                                        30
                                        100
                                        200
                                    </dataValues>
                                </ResultSet>
                            </results>
                        </MonitorResult>
                    </outcome>
                    <sensor>
                        <Sensor gml:id="id1">
                            <gml:description>shoulder sensor</gml:description>
                            <class>Pore Pressure Transducer</class>
                            <detector>
                                <Detector gml:id="idd">
                                    <measurand codeSpace="https://diggsml.org/dictionaries/DIGGSTestPropertyDefinitions.xml#pore_water_pressure">pore water pressure</measurand>
                                </Detector>
                            </detector>
                        </Sensor>
                    </sensor>
                </Reading>
            </reading>
        </Monitor>
    </measurement>
</Diggs>
